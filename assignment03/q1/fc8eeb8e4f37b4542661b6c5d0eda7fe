            CallDfpAd(adType, HCat+dcop_val+surroundTagVal, Size, Tile, Keys, Position, include_u_tag, use_krux);
        }
     }else{
         CallDfpAd(adType, HCat+dcop_val+surroundTagVal, Size, Tile, Keys, Position, include_u_tag, use_krux);
     }

</script>
