<script type='text/javascript'>
  //<![CDATA[
    (function() {
      var is_bar_not_visible = !BLOOMBERG.util.isScrolledIntoView('#bottom_recommended_bar');
    
      //hide no matter what - after a 2 seconds show it only if it's enabled
      $('#bottom_recommended_bar').hide();
    
      if (BLOOMBERG.recommender.article_page_bars_enabled()) {
        var collaborative_success_callback = function(stories) {
    
          if (stories.length < 6) {
            //Don't show the bar if we have less then 6 stories, just return and we'll show the backup
            return;
          }
    
          var items = "";
          $.each(stories, function(i, item){
            //This recreates shared/primary/_video_thumnbail_band.haml. TODO: Refactor this and other code to use a template system or load the data only instead of duplicating the partial.
            items = items + "<div class='news item";
            if (i == 5) {
               items = items + " last";
            }
            items = items + "'><div class='item_container thumbnail'><div class='thumbnail_container'><a href='"+item.url+"'><img alt='"+item.headline+"' height='80' src='http://www.bloomberg.com/image/"+item.thumbnailID+".jpg' width='140'/>";
            var asset_type = item.assetType;
            if (asset_type == null || asset_type == "") {
              asset_type = 'Story';
            }
            else if ( asset_type == 'Video') {
              var duration = item.duration,
                       formatted_duration = BLOOMBERG.tv_vid.generate_duration(duration);
              items = items + "<div class='overlay'></div><div class='video_info'><span class='duration'>"+formatted_duration+"</span></div>"
            }
            items = items + "</a></div><div class='item_content'><a class='headline q black' href='"+item.url+"' data-id='"+item.assetID+"' data-type='"+asset_type+"'>"+item.headline+"</a>"+
            '<div class="q_style_button add_to_q" title="Add to queue" style="opacity: 1;">Q</div></div></div></div>';
            if (i == 5) {
              //don't bother with any stories returned beyond 6 - this partial isn't designed for them
              return;
            }
          });
          if (!BLOOMBERG.recommender.bottom_bar_loaded) {
              $("#bottom_recommended_bar .stories_with_thumbnail").html(items);
              BLOOMBERG.recommender.show_bottom_bar();
          }
        };
    
        if ( "true" == 'true' && "true" == 'true') {
         BLOOMBERG.recommender.get_recommeded_articles("http://personalization.bloomberg.com/", "MSMJFI6JTSER01", is_bar_not_visible, collaborative_success_callback);
         setTimeout( function() {BLOOMBERG.recommender.show_bottom_bar()}, BLOOMBERG.recommender.reco_timeout);
        }
        else {
          BLOOMBERG.recommender.show_bottom_bar();
        }
      }
    
    })();
  //]]>
</script>

