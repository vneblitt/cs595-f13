<script>
   ;(function(button){
       button.on('click', "a.listen", function(e){
           e.preventDefault();
           var setProgressBar = function(time){
               var setProgressBar = time/duration * 100;
               //set the css %
               mediaTimeCurrent.css("width", setProgressBar+"%");
           };
           var playerInstance, type, duration,
               bucket = $(this).closest(".resaudio"),
               mediaTimeCurrent = $("#mediaTimeCurrent219591071");

           //If we are using html5 audio, it will pass the if statement
           if ($('html').hasClass('touch') && $('html').hasClass('video')) {
               playerInstance = document.getElementById("media219591071");
               type = "audio";
               duration = playerInstance.duration;
               $(playerInstance).on('playing', function(){
                   duration = playerInstance.duration;
                   bucket.removeClass("isLoading");
               });
               $(playerInstance).on('ended', function(){
                   setState("pause");
               });
               //On time update, we should update the progress bar
               $(playerInstance).on('timeupdate', function() {
                    setProgressBar(playerInstance.currentTime);
               });
           } else {
               //We are using jwPlayer
               playerInstance = jwplayer("jwPlayer219591071");
               type = "jwplayer";
               duration = playerInstance.getDuration();
               playerInstance.onPlay(function(){
                   bucket.removeClass("isLoading");
               });
               playerInstance.onComplete(function(){
                   setState("pause");
               });

               var timeupdate = function(){
                   var time = playerInstance.getPosition();
                   setProgressBar(time);
                   if(time < duration){
                       setTimeout(timeupdate, 150);
                   }
               };
               timeupdate();
           };
           var setState = function(state){
               $(this).data('status', state);
               switch(state){
                   case "play":
                       bucket.addClass("playing isLoading");
                       break;
                   case "pause":
                       bucket.removeClass("playing");
                       break;
               }
           }
           switch($(this).data("status"))  {
               case "play":
                       if(type==="jwplayer"){
                           playerInstance.stop();
                       }else{
                           playerInstance.pause();
                       }
                   setState.apply(this, ["pause"]);
                   break;
               case "pause":
                   playerInstance.play();
                   setState.apply(this, ["play"]);
                   break;
               default:
                   playerInstance.play();
                   setState.apply(this, ["play"]);

           }
       });
   }($("#res219591071")));
</script><!-- END ResourceLiveStream.mtpl -->         <script>
            
