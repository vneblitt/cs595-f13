<script type="text/javascript">
	
	
	var thisSub = "news"
	if (thisSub != "sports" && thisSub != "living" && thisSub != "opinion" && thisSub != "multimedia" && thisSub != "outdoors") {
		thisSub = "home";
	} else if (thisSub == "outdoors") {
		thisSub = "sports";
	}
	jQuery(document).ready(function () {
		var hideSub;
		var showSub;
		var hideHover;
		var showActive;
		jQuery('#subnav_'+thisSub).css("display", "block");
		jQuery('#'+thisSub).addClass("active");
		var navLi = jQuery('#mainnav li:not(.noSubNav)');
		navLi.bind("mouseenter", function () {
			navLi.removeClass("active");
			jQuery(this).addClass("active");
			jQuery('.subnav').hide();
			jQuery('#subnav_' + this.id).show();
		});
		jQuery('#nav').add("#sub").bind("mouseleave", function () {
			hideSub = setTimeout("jQuery('.subnav').hide()", 2000);
			showSub = setTimeout("jQuery('#subnav_'+thisSub).show()", 2000);
			hideHover = setTimeout("jQuery('#mainnav li:not(.noSubNav)').removeClass('active')", 2000);
			showActive = setTimeout("jQuery('#'+thisSub).addClass('active')", 2000);
		});
		jQuery('#nav').add("#sub").bind("mouseenter", function () {
			clearTimeout(hideSub);
			clearTimeout(showSub);
			clearTimeout(hideHover);
			clearTimeout(showActive);
		});
	});
</script>
