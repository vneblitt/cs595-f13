jQuery('#poll-1556-a97d3c0e02aa5cc1a1e3e5e69ae7a35b-results > .note-on-results .note-toggle').click(function(evt) {
    evt.preventDefault();
    var container = $('#poll-1556-a97d3c0e02aa5cc1a1e3e5e69ae7a35b');
    var form = $('#poll-1556-a97d3c0e02aa5cc1a1e3e5e69ae7a35b-form form'); 
    
