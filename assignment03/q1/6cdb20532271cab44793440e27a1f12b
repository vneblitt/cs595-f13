			<script type='text/javascript'>function readCookie(name) {   var nameEQ = name + "=";   var ca = document.cookie.split(';');   for(var i=0;i < ca.length;i++) {   var c = ca[i];   while (c.charAt(0)==' ') c = c.substring(1,c.length);   if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);   }   return null;   }   var x = readCookie('c_mId');   if (x)  {   try {if (typeof adTargetConsumerInfo == 'undefined') {document.write('<scr'+'ipt type="text/javascript" src="include-member-ad.javascript"></scr'+'ipt>');   }   } catch(err) {}}   else {var adTargetConsumerInfo = ';rg=ur';var adTargetYieldExInfo = '';}</script><script type='text/javascript'>   var ordString=';ord=' + cachebuster;   var scriptSrc='http://ad.doubleclick.net/N4011/adj/trb.latimes/news/opinion;;ptype=s;slug=la-ed-abortion-20130903;pos=T;dcopt=ist;sz=728x90;tile=1;at=MedicalProceduresandTests;at=SocialIssues;at=Abortion;at=HealthandMedicalProfessionals;at=CrimeLawandJustice;iab=heal;iab=soc;';   var noscriptHref='http://ad.doubleclick.net/N4011/jump/trb.latimes/news/opinion;;ptype=s;slug=la-ed-abortion-20130903;pos=T;sz=728x90;tile=1;at=MedicalProceduresandTests;at=SocialIssues;at=Abortion;at=HealthandMedicalProfessionals;at=CrimeLawandJustice;iab=heal;iab=soc?" target="_blank" rel="nofollow"';   var noscriptSrc='http://ad.doubleclick.net/N4011/ad/trb.latimes/news/opinion;;ptype=s;slug=la-ed-abortion-20130903;pos=T;dcopt=ist;sz=728x90;tile=1;at=MedicalProceduresandTests;at=SocialIssues;at=Abortion;at=HealthandMedicalProfessionals;at=CrimeLawandJustice;iab=heal;iab=soc?" width="728" height="90" border="0" alt=""';   var noscriptHrefEndOfStr=' target="_blank" rel="nofollow"';   var noscriptSrcEndOfString=' width="728" height="90" border="0" alt=""';   indexOfHrefQuestion='263';   indexOfLastImgSrcQuestion='271';   var yieldExParam=';u=';   var ptypeStr='ptype|s!';   var posStr='pos|T!';   var revenueScienceStr='';   var sizeStr='sz|728x90!';   try {       if (adTargetConsumerInfo != un