<script type="text/javascript">

var setGSCookie = function(key, value, exp) {  
	var expires = new Date();  
	expires.setTime(expires.getTime() + parseInt(exp));
	document.cookie = key + '=' + value + ';expires=' + expires.toUTCString();  
}; 

var updateGSCookie = function(key, value) {  
	document.cookie = key + '=' + value;  
};
  
var getGSCookie = function(key) {  
	var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');  
	return keyValue ? keyValue[2] : null;  
};  

var GSTriggerPrompt = function() {	
    var ARTICLE_URL = 'http://www.latimes.com/opinion/editorials/la-ed-abortion-20130903,0,6757217.story';
	var CONTENT_ID = (new Date).getTime();
    var el = document.createElement('script');
 	var url = 'http://survey.g.doubleclick.net/survey?site=39480498' +
              '&url=' + encodeURIComponent(ARTICLE_URL) +
              (CONTENT_ID ? '&cid=' + encodeURIComponent(CONTENT_ID) : '') +
              '&random=' + (new Date).getTime() +
              '&after=1';
    el.setAttribute('src', url);
    var head = document.getElementsByTagName('head')[0] ||
        document.getElementsByTagName('body')[0];
    head.appendChild(el);
};

(function initializeGoogleSurveys(limit) { 
	if (limit <= 0) return;
	var ms;
	
	if (!window.trb || !(ms = trb.meteringService) || 
		!ms.modalHandlers || !ms.modalHandlers.click) {
		setTimeout(function() { initializeGoogleSurveys(limit ? limit - 1 : 250);
		}, 250);
		return; 
	}

	ms.modalHandlers.click.cancel = function googleSurveyModalClickHandler(dialog, event, target, currentElement, cancelStop) {
		event.preventDefault ? event.preventDefault() : (event.returnValue = false);

		if (navigator.userAgent.toLowerCase().indexOf('safari') > 0 && navigator.userAgent.toLowerCase().indexOf('chrome') < 0) {
			trb.meteringService.abandon(); //Safari is not supported so follow current DSS rule
		} else {
			//Only show survey in non hard-walled content areas
			if (parseInt(trb.meteringService.meterData[0].matchedRule.limit) > 0) {

				//Make sure user only sees survey 2 times in 24 hours or completes one survey per 24hours
				showDSurvey = false;
				showMSurvey = false;
				
				if (getGSCookie('g_SurveyD') != null) {
					var cnt = getGSCookie('g_SurveyD');
				    showDSurvey = (parseInt(cnt) < 2) ? true : false;
				} else {
					setGSCookie('g_SurveyD', '0', '86400000'); //Expires 24 hours
					showDSurvey = true;
				}

				if (getGSCookie('g_SurveyM') != null) {
					var cnt = getGSCookie('g_SurveyM');
				    showMSurvey = (parseInt(cnt) >= 10) ? false : true;
				} else {
					setGSCookie('g_SurveyM', '0', '2592000000'); //Expires 30 Days
					showMSurvey = true;
				}
				
				if (showDSurvey && showMSurvey) {
					GSTriggerPrompt();
					var cnt = getGSCookie('g_SurveyD');
					updateGSCookie('g_SurveyD', parseInt(cnt) + 1);
                    dialog.modals[0].closeDialog(); 
                    dialog.modals[0].removeOverlay(); 
                    dialog.cancelStop();
				} else { 
					trb.meteringService.abandon(); 
				}
				
			} else {
				trb.meteringService.abandon();
			}	

		}	

	}
})();

var gSurveyComplete = function(){
  //Survey completed callback
  googleSurveyAddStoredPage();
  updateGSCookie('g_SurveyD', '3'); //User can only complete 1 survey per 24 hours
  var cnt = getGSCookie('g_SurveyM');
  updateGSCookie('g_SurveyM', parseInt(cnt) + 1); //User can only complete 10 surveys per 30 days
};

var googleSurveyAddStoredPage = function(){
    ms = trb.meteringService;
	ms.getPageUrl(null, function(pageUrl) {
		ms.storage.getPage(pageUrl, function(page) { ms.getStoredPages(function(pages) {
				if (ms.util.arrayIndexOf(ms.storedPages, page) < 0) { 
					pages.push(page); 
					ms.storage.setItem(ms.storedPagesKey, ms.util.stringify(pages)); 
				}
			}); 
		});
	});
};
</script>


    
    
    
