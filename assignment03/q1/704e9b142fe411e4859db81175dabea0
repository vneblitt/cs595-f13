    <script type="text/javascript">
    var p = 1;
    var host = location.href;
    
    var hostLength = host.length;
    var lastChar = host.charAt(hostLength-1);
    if (lastChar === '/') {
      host = host.substring(0, host.length-1);
    }
    
    $(document).ready(function() {
      FB.init({appId: "400928743256315", status: true, cookie: true});
      if ($('#nextPage').length === 0) {
        $('#morePosts').remove();
      }
      readyState();
    });
    
    function readyState() {
      $.each($('.month'), function(i, elem) {
        var mo = elem.id;
        mo = mo.split('-');
        if (elem.innerHTML.lastIndexOf('.') === 0 ) {
        } else {
          if (mo[1] === "05" || mo[1] === "06" || mo[1] === "07"){
            if (mo[1] === "06") { elem.innerHTML = "June"; }
            if (mo[1] === "07") { elem.innerHTML = "July"; }
          } else {
            elem.innerHTML += ".";
          }
        }
      });
      
      $('.sharing').on("click", function (event){
          var url = $(this).attr("href");
          var windowName = $(this).attr("target");
          var windowWidth, windowHeight;
          if ($(this).parent().attr("class") == "twitter") {
      		  windowWidth = 550;
      		  windowHeight = 340;
      		  var tweet = $(this).closest("article").find(".lm-tweetBody").attr("data-tweet-body");
      		  if (tweet && tweet !== "") {
      		    tweet = encodeURIComponent(tweet);
      		    url = url + "&text=" + tweet;
      		  } else {
      		    url = url + "&text=An%20image%20from%20the%20%23LivelyMorgue%2C%20a%20collection%20of%20treasures%20from%20the%20%40nytimes%20photography%20archive%3A";
      		  }
          } else if ($(this).parent().attr("class") == "facebook") {
            var fbUrl = $(this).attr("data-fb-link");
            var fbImg = $(this).closest("article").find(':first-child img').attr("src");
            var obj = {
              method: 'feed',
              display: 'popup',
              link: fbUrl,
              picture: fbImg,
              name: 'The Lively Morgue',
              caption: 'An archival photograph from The New York Times.'
            };
            FB.ui(obj);            
          } else {
            windowWidth = 650;
            windowHeight = 520;
          }
          
          if ($(this).parent().attr("class") != "facebook") {
            var windowLeft = parseInt((screen.availWidth/2) - (windowWidth/2));
            var windowTop = parseInt((screen.availHeight/2) - (windowHeight/2));

      		  var windowSize = "width=" + windowWidth + ",height=" + windowHeight + ",left=" + windowLeft + ",top=" + windowTop + ",screenX=" + windowLeft + ",screenY=" + windowTop + ",scrollbars=no";
      		  window.open(url, windowName, windowSize);
          }
    		  
          event.preventDefault();
      });
    }
    
    function fixHeights(node) {
      var newHeights = $('div'+node).find(':first-child img').outerHeight();
      $('div'+node).find(':last-child img').css({maxHeight:newHeights});
    }

    function showMore() {
      p++;
      var morePosts = $('#morePosts');
      morePosts.toggleClass('loading');
      var morePostText = 'Show more posts';
      morePosts.html('Loading…');
      morePosts.animate({
      }, 300, function() {
        $.get(host + '/page/' + p, function(data) {
          morePosts.html('Loaded.');
          var c = $(data).find('#content').html();
          $('article').last().after(c);
          var d = $(data).find('#nextPage').html();
          if (d) {
            morePosts.html(morePostText);
            morePosts.toggleClass('loading');
          } else {
            $('#morePosts').remove()
          }
          readyState();
        });
      });
    }
    function flip(postId) {
      var flipCP = '#centerpiece'+postId;
      
      fixHeights(flipCP);
      
      $(flipCP).toggleClass('unflipped').toggleClass('flipped');
      $(flipCP).removeClass('init');
      var flipAside = '#sidebar'+postId;
        $(flipAside).addClass('flipping');
        $(flipAside).siblings('.flipper').addClass('flipping');
        $(flipAside).toggleClass('unflipped').toggleClass('flipped');
        setTimeout( function() {
          $(flipAside).removeClass('flipping');
          $(flipAside).siblings('.flipper').removeClass('flipping');
        }, 500);
    }
    try{Typekit.load();}catch(e){}
    </script>
