	function KeySearch(e, input){ 
	                      if(e != null){
	                        if (e.keyCode == 13 && input != null){
	                                Search(input);
	                                return false;
	                            } 
	                        } 
	                    }
	
	                function MouseSearch(inputId){
	                    var input = Sys.UI.DomElement.getElementById(inputId);
	                    if (input != null){
	                        Search(input);
	                        return false;
	                    }
	                }
	
	                function Search(textbox){
	                                    var text = textbox.value;
	                                    if (text != '' && text != null ){
	                                        var select = $get('M_PlaceHolderSearchArea_SearchScope');
	                                        if(select != null){
	                                            if (select.selectedIndex == 0){
	                                                window.location = '/Search/results.aspx?k=' + text;
	                                            }
	                                            else {
	                                                window.open('http://www.loc.gov/search/?q=' + text);
	                                            }
	                                        }
	                                    }
	                                    
	                }
	//]]>
