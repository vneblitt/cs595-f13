<script type="text/javascript">
    var initialValue_RIDefault;

    function HandleClientLoaded_RIDefault(sender, eventArgs) {
        initialValue_RIDefault = sender.get_minimumValue();
        HandleClientValueChange_RIDefault(sender, null);
    }

    function HandleClientValueChange_RIDefault(sender, eventArgs) {
        var wrapperDiv = document.getElementById('Boxes_Wrapper_RIDefault');
        var contentDiv = document.getElementById('Boxes_Content_RIDefault');

        var oldValue = (eventArgs) ? eventArgs.get_oldValue() : sender.get_minimumValue();
        var change = sender.get_value() - oldValue;

        var contentDivWidth = contentDiv.scrollWidth - wrapperDiv.offsetWidth;
        var calculatedChangeStep = contentDivWidth / ((sender.get_maximumValue() - sender.get_minimumValue()) / sender.get_slideStep());

        initialValue_RIDefault = initialValue_RIDefault - change * calculatedChangeStep;

        if (sender.get_value() == sender.get_minimumValue()) {
            contentDiv.style.left = 0 + 'px';
            initialValue_RIDefault = sender.get_minimumValue();
        }
        else {
            contentDiv.style.left = initialValue_RIDefault + 'px';
        }
    }
    
</script>


        </div>

