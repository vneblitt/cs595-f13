	/* <![CDATA[ */
	
	/* Use a public URL or OB will fail */

	canonicalURL = jQuery( 'link[rel=canonical]' ).attr( 'href' );
	replacement = 'www.ew.com';
	pattern = RegExp( /dev.*\.ew\.com/ );
	canonicalURL = canonicalURL.replace( pattern, replacement );
	pattern = RegExp( /qa.*\.ew\.com/ );
	canonicalURL = canonicalURL.replace( pattern, replacement );
	pattern = RegExp( /preview\.ew\.com/ );
	canonicalURL = canonicalURL.replace( pattern, replacement );

	/* Update OB div */
	outbrain = jQuery( '.OUTBRAIN.ew_ob_SF_1' );
	if ( jQuery('body#tvrecaps.home').length > 0 ) {
		outbrain.attr('data-widget-id', 'SFR').addClass('ew_ob_SFR').removeClass('ew_ob_SF_1');
	}
	dataSrc = outbrain.attr( 'data-src' );
	outbrain.attr( { 'data-src' : canonicalURL } );  
	
	/* ]]> */
	</script>
	<script type="text/javascript" async="async" src="http://widgets.outbrain.com/outbrain.js"></script>
</div>

				</div>
							
			<div id="tvgrid"></div>
			<div id="zap2it-footer">
				<p id="zap2it-logo"><a href="http://affiliate.zap2it.com/tvlistings/ZCGrid.do?aid=entweekly" target="_blank">TV Listings From <span>Zap2it</span></a></p>
				<p id="see-full"><a href="http://affiliate.zap2it.com/tvlistings/ZCGrid.do?aid=entweekly" target="_blank">See full TV listings</a></p>
			</div>
			
						
			<div class="cmWrapper">
				<script>
					var cmAd = adFactory.getCmAd(300, 250, "column", "ofie");
					cmAd.write();
				</script>
			</div>
			
					
	
