<script>
	jQuery( document ).ready( function () {
			
			function zeroPad(num, places) {
			  var zero = places - num.toString().length + 1;
			  return Array(+(zero > 0 && zero)).join("0") + num;
			};
			
			// Get starting date
			if ( getUrlVars()[ 'calendarDate' ] != null && getUrlVars()[ 'calendarDate' ].length > 0 ) {
				var eventCalendarToday = getUrlVars()[ 'calendarDate' ];
			} else {
				var eventCalendarTodayTemp = new Date();
				var eventCalendarToday = eventCalendarTodayTemp.getFullYear() + '-' + 
										zeroPad( eventCalendarTodayTemp.getMonth() + 1, 2) + '-' + 
										zeroPad( eventCalendarTodayTemp.getDate(), 2);
			};
			
			// Remove past dates and trim to just 5 days, the default
			if ( getUrlVars()[ 'calendarMaxDays' ] != null && getUrlVars()[ 'calendarMaxDays' ].length > 0 ) {
				var eventCalenderMaxDays = getUrlVars()[ 'calendarMaxDays' ];
			} else {
				var eventCalenderMaxDays = 5;
			}
			var eventCalendarCount = 0;
			jQuery( '#event-calendar.finales ol.wrapper li.date' ).each( function (index ) {
					var eventCalendarDate = jQuery(this).attr( 'data-date' );
					
					if ( eventCalendarDate < eventCalendarToday || 
							eventCalendarCount >= eventCalenderMaxDays ) {
						jQuery(this).remove();
					} else {
						eventCalendarCount++;
					}
				}
			);
			
			// Decide to expose the module if loaded
			var eventCalendarCount = jQuery( '#event-calendar.finales ol.wrapper li.date' ).size();
			if ( eventCalendarCount > 0 ) {
				jQuery( '#event-calendar.finales ol.wrapper li.date:even' ).removeClass( 'odd' );
				jQuery( '#event-calendar.finales ol.wrapper li.date:odd' ).removeClass( 'even' );
				jQuery( '#event-calendar.finales ol.wrapper li.date:even' ).addClass( 'even' );
				jQuery( '#event-calendar.finales ol.wrapper li.date:odd' ).addClass( 'odd' );
				
				jQuery( '#event-calendar' ).fadeIn( 'slow' );
			};
		}
	);
</script>
