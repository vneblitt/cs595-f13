<script type="text/javascript">
    window.loadTriblocalTown = function() {
        var $ = jQuery;
        var town_id = readCookie('triblocal_town_id');
        var frames = $(".triblocal-headline-frame");
        var qs = "";
        if(town_id || town_id == -1)
            qs = '?' + town_id;

        frames.each(function () {
            $(this).attr('src', $(this).attr('srcx') + qs);
        });
    };

    // Used on /about/membership and for
    // homepage premium content promo
    if ( !window.check_for_meteringService ) {
        window.check_for_meteringService = function(callback) {
            if ( !window.meteringServiceCheckTimers )
                window.meteringServiceCheckTimers = [];

            if ( callback ) {
                // "Unique enough" key for callback timer
                var key = escape(callback.toString().replace(/\n|\s/gi, "").slice(0, 100));
            }

            if ( typeof trb.meteringService == 'undefined') {
                if ( callback ) {
                    if ( window.meteringServiceCheckTimers[key] )
                        clearTimeout( window.meteringServiceCheckTimers[key] );

                    window.meteringServiceCheckTimers[key] = setTimeout(
                        window.check_for_meteringService, 1000, callback);
                }
                return false;
            } else {
                if ( callback ) {
                    if ( window.meteringServiceCheckTimers[key] ) {
                        clearTimeout( window.meteringServiceCheckTimers[key] );
                    }
                    callback();
                }
                return true;
            }
        };
    }

    jQuery(document).ready(function(){
        var $ = jQuery;
        window.loadTriblocalTown();

        // adjust the sliding billboard if it's too big
        if($(".slidingbillboard > div").width())
            $(".slidingbillboard").css("margin-left", "-5px");

        // handle some generic classes for adjusting stuff based
        // on user status
        if(trb.meteringService == undefined) {
            // Detect user status some other way
            if(readCookie('c_mId')) {
                var name = readCookie('c_unm');
                $("body").addClass('user-logged-in');
                $(".user-name-here").text(name);
            }
        } else {
            window.check_for_meteringService(function() {
                if (window.trb.meteringService.storedData.userId) {
                    window.trb.meteringService.getUserData(function(user) {
                        var name = user.getFirstName();
                        if ( name == '' )
                            name = user.getUsername();

                        $("body").addClass('user-logged-in');
                        $(".user-name-here").text(name);
                    });
                }
            });
        }
    });
</script>



<!-- END OF STUFF YOU SHOUDLN'T TOUCH. -->
    




   


    

    
    
    
