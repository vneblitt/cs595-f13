			  <script type="text/javascript">
			      function SearchSubmit() {
			          document.searchform.q.value = document.aspnetForm.q.value;
			          document.searchform.submit();
			      }

			      function entsub(e) {
			          var keyPressed;
			          if (window.event)
			              keyPressed = window.event.keyCode; // IE
			          else
			              keyPressed = e.which; // Firefox

			          if (keyPressed == 13) {
			              //alert("you hit enter key");
			              SearchSubmit();
			              return false;
			          } else
			              return true;
			      }
			  </script>
