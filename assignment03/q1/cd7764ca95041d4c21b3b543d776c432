  <script type="text/javascript">
  

  $( document ).ready(function() {
    $('#twittext').hide();
    $('#facetext').hide();
    $('#plustext').hide();
    // Email starts expanded, remember to edit this if that changes
    expanded = "email";
    
    $('.expandable').click(function() {
      console.log("clicked");
      switch ($(this).attr('id')) {
        case "emailbox":
          $('#twittext').hide();
          $('#facetext').hide();
          $('#plustext').hide();
          $('#subsubmit').removeAttr("disabled");
          if ( expanded != "email") {
            $('.expandable').closest('li').switchClass('expand', 'contract');
            $(this).closest('li').switchClass('contract', 'expand');
            expanded = "email";
          }
          break;
        case "facebox":
          $('#subsubmit').attr("disabled", "disabled");
          $('#twittext').hide();
          $('#plustext').hide();
          if ( expanded != "facebox") {
            $('.expandable').closest('li').switchClass('expand', 'contract');
            $(this).closest('li').switchClass('contract', 'expand', 10, function(){ $('#facetext').show(); } );

            expanded = "facebox";
          }
          break;
        case "twitbox":
          $('#subsubmit').attr("disabled", "disabled");
          $('#facetext').hide();
          $('#plustext').hide();
          if ( expanded != "twitter") {
            $('.expandable').closest('li').switchClass('expand', 'contract');
            $(this).closest('li').switchClass('contract', 'expand');
            $(this).closest('li').switchClass('contract', 'expand', 10, function(){ $('#twittext').show(); } );

            expanded = "twitter";
          }
          break;
        case "plusbox":
          $('#subsubmit').attr("disabled", "disabled");
          $('#twittext').hide();
          $('#facetext').hide();
          
          if ( expanded != "googleplus") {
            $('.expandable').closest('li').switchClass('expand', 'contract');
            $(this).closest('li').switchClass('contract', 'expand', function(){ $('#plustext').show(); } );
            expanded = "googleplus";
          }
          break;
      }
    });
    
  });  

  </script>

    

