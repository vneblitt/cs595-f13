              <script>
                // This function creates an <iframe> (and YouTube player)
                //    after the API code downloads.
                var player3;
                window.heroSlide3 = function() {
                  window.PLAYERS.player3 = new YT.Player('player-3', {
                    height: $(window).height(),
                    width: $(window).width(),
                    videoId: 'A4fSuwkezEE',
                    wmode: 'transparent',
                    autohide: 1,
                    playerVars: {
                      rel: 0, 
                      autohide: 1,
                      showinfo: 0,
                      wmode: 'transparent'
                    },
                    events: {
                      "onStateChange": window.toggleDescription,
                      "onError": function(errorCode) { console.log(errorCode) }
                    }
                  });
                  
                  return window.PLAYERS.player3;
                }

              </script>
      
