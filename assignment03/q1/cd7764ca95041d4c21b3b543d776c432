<script type="text/javascript">
   window.PLAYERS = {}
   $(function() {
     $("#hero").rotator({
			next_button:     '.next',
			previous_button: '.back'
		});
    
    
    window.onYouTubeIframeAPIReady = function() {
          
      window.heroSlide1 ? heroSlide1() : null
      window.heroSlide2 ? heroSlide2() : null
      window.heroSlide3 ? heroSlide3() : null
      window.heroSlide4 ? heroSlide4() : null
      window.heroSlide5 ? heroSlide5() : null
    }

    window.toggleDescription = function(playerState) {
      console.log('toggle')
      var idNumber = playerState.target.a.id.split("player-")[1]
      if (playerState.data == 1) {
        $('#text-' + idNumber).hide()
        $("#hero").tabs('rotate', 0, false );
        $("#bar, footer").slideUp()
      } else {
        $('#text-' + idNumber).show()
        $("#hero").tabs('rotate', 8000, false );
        $("#bar, footer").slideDown()
      }
    }
	});
</script>

