<script type="text/javascript">

   $(function() {
   
    var $panel,
        $panel2,
        $panels,
        $caroufredsel_wrapper
    
    //Responsive rotator code
    $panel = $("#panel")
    
    //If it's a high window, set two rows of feed items
    if ($(window).height() > 1200) {
      $panel.after('<div id="panel2" class="cf panel ui-tabs-panel" />').next().html($panel.html());
      $("#panel article:odd").remove()
      $("#panel2 article:even").remove()
      $panel2 = $("#panel2")

      
      $panel.carouFredSel({
        synchronise	: "#panel2",
        responsive: false,
        direction: 'left',
        auto: false,
        prev: $('.feed-back'),
        next: $('.feed-next'),
        items: {
          visible: 4,
          filter: "article"
        },
        debug: true
      })
      

      $panel2.carouFredSel({
        responsive: false,
        direction: 'left',
        auto: false,
        items: {
          visible: 4,
          filter: "article"
        },
        debug: true
      })
    } else {
      $panel.carouFredSel({
        responsive: false,
        direction: 'left',
        auto: false,
        prev: $('.feed-back'),
        next: $('.feed-next'),
        items: {
          visible: 4,
          filter: "article",
        },
        debug: true
      });
    }
    
    $caroufredsel_wrapper = $('.caroufredsel_wrapper')
    
    if ($(window).height() < 750) {
      $caroufredsel_wrapper.slideUp()
    }
    
    $panels = $("#panel, #panel2")
    var resize = function() {
      if ($(window).width() > 2300) { 
        $panels.trigger("configuration", ["items.visible", 8]);
      } else if ($(window).width() > 2100) {
        $panels.trigger("configuration", ["items.visible", 7]);
      } else if ($(window).width() > 1800) {
        $panels.trigger("configuration", ["items.visible", 6]);
      } else if ($(window).width() > 1490) {
        $panels.trigger("configuration", ["items.visible", 5]);
      } else if ($(window).width() > 1270) {
        $panels.trigger("configuration", ["items.visible", 4]);
      } else if ($(window).width() > 960) {
        $panels.trigger("configuration", ["items.visible", 3]);
      } else if ($(window).width() > 0) { 
        $panels.trigger("configuration", ["items.visible", 2]);
      }
    }
    
    resize()
    

    if ($(window).height() > 750) {
        $("#updates").removeClass("contracted")
        $("#updates").addClass("expanded")
    }
    
    $(window).resize(function () { 
      resize()
      if ($(window).height() > 750) {
        $("#updates").removeClass("contracted")
        $("#updates").addClass("expanded")
      }
    });
    
    $("#updates").click(function() {
      $caroufredsel_wrapper.slideToggle("medium", function() {
        if($caroufredsel_wrapper.is(":visible")) {
          $("#updates").removeClass("contracted")
          $("#updates").addClass("expanded")
        } else {
          $("#updates").removeClass("expanded")
          $("#updates").addClass("contracted")
        }
      });
    });
	});
</script>

