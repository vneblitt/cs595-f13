<script type="text/javascript"><!--//--><![CDATA[//><!-- 
function getMetaContentsOf(name) { 
var keywords = ""; 
var metas = document.getElementsByTagName("meta"); 
// loop through the meta tags, capturing the contents of the one with name="keywords"
for(var i = 0; i < metas.length; i++) { 
if (metas[i].name == name) { 
keywords = metas[i].content; 
break; 
} 
} 

// split up the first piece and strip out non-ASCII content
var keywords_array = keywords.split("|"); 
return keywords_array[0].replace(/[^A-Za-z0-9]/g," ").substring(0,1024); 
} 

// call the ad using the keywords captured from the meta tag
document.write('<scr' + 'ipt type="text/javascript" src="http://fwevents.planetdiscover.com/ads-meta/view.jsp?lid=37&keywords=' + getMetaContentsOf('keywords') + '"></scr' + 'ipt>'); 
