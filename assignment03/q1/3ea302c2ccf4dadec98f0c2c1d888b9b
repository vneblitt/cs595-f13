    ?>
 
    
    <?php echo $start_time; ?>
 
    <div class="link_timestamp">Wednesday, Sep. 25</div>
    <a href="http://www.loudountimes.com/news/article/loudoun_school_board_enters_contract_with_jefferson123" class="headline_link2">Loudoun School Board enters contract with Jefferson</a>

    

