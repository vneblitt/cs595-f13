<h1>The Giant Panda Cam</h1>
<p><em>Sponsored by <strong>Ford Motor Company Fund</strong></em></p>
<p class="right"><a href="/animals/webcams/pchelp.cfm">? Help with Cam</a></p>



 <div class="darkgray">Panda Cam 1 | <a href="/animals/webcams/giant-panda.cfm?cam=panda03">Panda Cam 2</a></div> 

 <div class="center blackbox"> 
 
  <div class="center" id='my-video'>Loading...</div>
  <script>
	jwplayer('my-video').setup({
		playlist: [{
		    image: "http://nationalzoo.si.edu/animals/webcams/images/loading-background.jpg",
			title: "Start the Panda Cam",
			sources: [
				{file: "http://160.111.252.144:1935/live_edge_panda/smil:panda02_all.smil/jwplayer.smil"},
				{file: "http://160.111.252.144:1935/live_edge_panda/smil:panda02_all.smil/playlist.m3u8"}
			]
		}],			
			autostart: false,
        	fallback: false,
			mute: true,
			events: {
				onTime: function(event) {
					if(event.position>=900){
						this.pause();
					}
				}
			},
			ga: {}
    });
  </script>
 

 <script>
  function resizePlayer(){
    if(jwplayer().getWidth() == 640) { 
      jwplayer().resize(480,270); 
    } else {
      jwplayer().resize(640,360);
    }
  };
 </script>
 </div>
 

