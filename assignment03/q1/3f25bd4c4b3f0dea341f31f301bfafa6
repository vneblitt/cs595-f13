  function show_embedcode() {
    $('div.embedcodearea').show();
    $('div.embedcode .explain').hide();
    $('.show_embedcode').click(hide_embedcode);
    return false;
  }

  function hide_embedcode() {
    $('div.embedcodearea').hide();
    $('div.embedcode .explain').show();
    $('.show_embedcode').click(show_embedcode);
    return false;
  }

  $(document).ready( function() {

    $('.hide_embedcode').click(hide_embedcode);
    $('.show_embedcode').click(show_embedcode);

  });

</script>



