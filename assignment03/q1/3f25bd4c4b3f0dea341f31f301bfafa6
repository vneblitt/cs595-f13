<script type="text/javascript">

function goto_page(p) {
    var comment_url = '/comments.html?petition_id=49093' +
                      '&sig_page=' + p + 
                      '&sig_page_size=10' +
                      '&max_page=172';
    
    $('#pet-signers').load(comment_url,
        function () { 
            $('#pet-signers-loading').hide();
            $('#pet-signers').show();
        });
}


function doFlagComment(user_id) {
    // called by flag comment onclick
    if (confirm('Flag this comment as inappropriate?')) {
        $("#flag-comment-user-id").val(user_id);
        $("#flag-comment-form").submit();
    }
}

function showMore() {
    $('#few-targets').hide();
    $('#all-targets').show();
}

function t( id ) { //toggle!
    var el = document.getElementById(id);
    el.style.display = el.style.display == 'block' ? 'none' : 'block';
}
function submitFlag(reason) {
    $("#flag-petition-form-reason").val(reason);
    $("#flag-petition-form").submit();
}

function changed_country() {
    var country = $('select.country_select').val();
    if (country == "United States") {
        $('.region').hide();
        $('.postal').hide();
        $('.zip').show();
        $('.state').show();
    } else {
        $('.zip').hide();
        $('.state').hide();
        $('.region').show();
        $('.postal').show();
    }
}
function toggleOrganizationName() {
    $('input[name=thirdparty_optin][type=checkbox]').click(function() {
        $('#organization_receive').toggle($(this).attr("checked"))
    });
}
$(function() { 
    changed_country();
    EmailSuggestion.init();
    toggleOrganizationName();
});
</script>






