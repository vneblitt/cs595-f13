        <script type="text/javascript">

nbc.validateEmail = function(submittedAddr) {
	
	if( (submittedAddr.length > 0) && (submittedAddr != "Email") ) {
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		return emailReg.test(submittedAddr);
	} else {
		alert("Invalid address.");
		return false;
	}
}
<!--TEMPLATE LINE 207-->
nsu227661978 = {};
nsu227661978.hasOmnitureFired = false;

        nsu227661978.clearSignup = function(dasEle){
            str=dasEle.value;
            str2=str.replace(" ", "");
            if(str2.toLowerCase() == "email"){
                dasEle.value="";
            }
        }

nsu227661978.signUserUp = function(email) {
	var isAddressValid = nbc.validateEmail(email);
	thisNewsletterID = "227661978";
	
		if(isAddressValid != true) {
		return false;
		} else {
		nsu227661978.checkUserStatus(email);
		}
}
		
	
	nsu227661978.fireOmniValues = function() {
		nbcu.un = nbcu_account;
		nbcu.prop7 = 'News Newsletter Sign Up';
		nbcu.eVar7 = 'News Newsletter Sign Up';
		nbcu.events = 'event19';
		nbcu.linkTrackVars = 'prop7,eVar7,events';
		nbcu.linkTrackEvents = 'event19';
		U.log(nbcu.prop7 + ' | ' + nbcu.eVar7 + ' | ' + nbcu.events + ' | ' + nbcu.linkTrackVars);
		try {
		nbcu.tl(true,'o','News Newsletter Sign Up Complete');
		}
		
		catch(e) {
		U.log(e);
		}
		nsu227661978.hasOmnitureFired = true;
		return false;
	}
	
	nsu227661978.checkUserStatus = function(email) {
		jQuery.ajax({type: 'POST',
				data: { h: 'v3_edialog', email: email, lookup:'yes' },
				url: '/i/dispatcher/',
				dataType: 'json',
				
				success: function(data) {
				nsu227661978.fireOmniValues();
				U.log(data);
					if(data.status == 200) {
						U.log(data.status + ": User is OK!");
							if(jQuery.inArray(thisNewsletterID,data.newsletters) != -1) {
								U.log("singleNLSignup:checkUserStatus | User is already signed up.");
								jQuery('#nsEmail').val("You've already signed up before!");
	                            jQuery('#nsEmail').css({"color":"red","text-transform":"none"});
								return false;
							} else {
								U.log("singleNLSignup:checkUserStatus | User is not signed up.");
								nsu227661978.subscribeUser(email);
							}
					} else {
					U.log(data.status + ": User is not in the system.");
					nsu227661978.subscribeUser(email);
					}
				}
		});
	}
	
	
	nsu227661978.subscribeUser = function(email) {
		U.log("Attempting to add user to this mailing list...");
		jQuery.ajax({
			type: 'POST',
			data: {h: 'v3_edialog', email: email, update: 'yes',addAudienceKeys:thisNewsletterID},
			url: '/i/dispatcher/',
			dataType: 'json',
			
			success: function(data) {
				U.log(data);
				U.log("User added successfully.");
				jQuery('#nsForm').html('<span class="nsConfirm">Thank you for signing up!</span>');
			}
		
		});
	
	
	}




jQuery(document).ready(function() {
	jQuery('#nsForm').submit(function() {
        capturedEmail = jQuery('#nsEmail').val();
        nsu227661978.signUserUp(capturedEmail);
        return false;
    });
});


        
        </script>
			
