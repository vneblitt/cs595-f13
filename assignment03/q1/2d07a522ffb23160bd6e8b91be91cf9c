jQuery(document).ready(function() {
jQuery("a.lbFile").fancybox({
  'overlayShow'   : true,
  'overlayOpacity':0.6,
  'autoDimensions':false,
  'width':654,
  'height':368,
  'transitionIn'  : 'fade',
  'transitionOut' : 'fade',
  'titlePosition' : 'inside',
  'hideOnOverlayClick' : false,
  'enableEscapeButton':true,
  'titleFormat' : function(title, itemArray, currentArray, selectedIndex, currentIndex, currentOpts) {
  
  // Figure out which lightbox information to pull from the JS array on the page.
  // These correspond to values in nbc.lightBoxEvergreenUrls
  console.log(jQuery(itemArray[0]));
  clickedLinkIDSplit = jQuery(itemArray[0]).attr('id').split('-');
  console.log(clickedLinkIDSplit[1]);
  clickedLinkIndex = parseInt(clickedLinkIDSplit[1]);
  
  return '<p class="lbTitleRegion">'+nbc.lightBoxEvergreenTitles[clickedLinkIndex-1]+'</p><a class="lbFullPageLink" href='+nbc.lightBoxEvergreenUrls[clickedLinkIndex-1]+'><img alt="View in full page" width="131" height="24" src="http://media.'+nbc.host+'/designimages/viewInFullPage.png"></a><p class="lbSummaryRegion">'+nbc.lightBoxEvergreenSummary[clickedLinkIndex-1]+' <a href="'+nbc.lightBoxEvergreenUrls[clickedLinkIndex-1]+'">Read more</a></p><div id="lbTwitterButton" class="lbSocial"><a href="http://twitter.com/share" class="twitter-share-button" data-count="horizontal" data-url="'+nbc.lightBoxEvergreenUrls[clickedLinkIndex-1]+'" data-via="'+nbc.pageTwitterID+'">Tweet</a></div>';
  },
  
  'onComplete':function() {
  jQuery('#fancybox-title').append('<div id="lbFacebookButton" class="lbSocial"><div class="fb-like" data-href="'+encodeURIComponent(nbc.lightBoxEvergreenUrls[clickedLinkIndex-1])+'" data-send="false" data-layout="button_count" data-width="160" data-show-faces="false" data-action="recommend" data-font="arial"></div></div><div id="lbGoogleButton" class="lbSocial"><g:plusone size="medium" href='+encodeURIComponent(nbc.lightBoxEvergreenUrls[clickedLinkIndex-1])+'></g:plusone></div>');        
