<script type="text/javascript">
// Only if Javascript is enabled...
window.onload = function(e) {
// ...set cursor style only for those columns that are sortable...
var allTableHeaders = document.getElementsByTagName('th');
for(var n = 0; n < allTableHeaders.length; n++) {
if(allTableHeaders[n].getAttribute('sortable') == 'true') {
allTableHeaders[n].style.cursor = 'pointer';
}
}
// ...unhide the sort text verbiage as well...
document.getElementById('iCIMS_SortTextRow').className = '';
// ...and unhide the sort arrow entities. We hide the sort arrow and unhide it if Javascript is enabled
// to solve an edge case where user had Javascript on sorted, then Javascript was turned off.
// All other evidence of sortability would disappear except the arrows.
if(document.getElementById('iCIMS_SortArrowEntity')) {
document.getElementById('iCIMS_SortArrowEntity').className = '';
}
}
</script>
<script type="text/javascript">
function icims_addCookieMessage() {
var cookieEnabled = navigator.cookieEnabled ? true : false;
if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled) {
document.cookie = "testcookie";
cookieEnabled = document.cookie.indexOf("testcookie") != -1 ? true : false;
}
if (!cookieEnabled) {
document.getElementById('iCIMS_NoCookiesMessage').style.display = 'block';
}
}
// Either append our cookie check onload or just run it.
if (typeof window.onload != 'function') {
window.onload = function() {
icims_addCookieMessage();
};
} else {
var icims_originalOnload = window.onload;
window.onload = function() {
icims_originalOnload();
icims_addCookieMessage();
};
}
</script>
