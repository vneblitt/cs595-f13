		if (iframeSrc.indexOf('pubads.g.doubleclick.net/gampad/adx') > -1 && typeof _gpt_simpleUrlTargetingParams !== 'undefined'){
			iframeSrc += '&t=' + _gpt_simpleUrlTargetingParams;
		}
		var node = document.getElementById('doubleclick_1661099782');
		node.innerHTML = '<iframe width="300" height="250" scrolling="no" frameborder="0" allowtransparency="true" src="' + iframeSrc + '" marginwidth="0" marginheight="0"></iframe>';
});
} else {
	
		var iframeSrc = 'http://pubads.g.doubleclick.net/gampad/adx?iu=2620/nbcnews/politics/first_read&sz=300x250&c=1661099782&ord=1661099782';
		if (iframeSrc.indexOf('pubads.g.doubleclick.net/gampad/adx') > -1 && typeof _gpt_simpleUrlTargetingParams !== 'undefined'){
			iframeSrc += '&t=' + _gpt_simpleUrlTargetingParams;
		}
		var node = document.getElementById('doubleclick_1661099782');
		node.innerHTML = '<iframe width="300" height="250" scrolling="no" frameborder="0" allowtransparency="true" src="' + iframeSrc + '" marginwidth="0" marginheight="0"></iframe>';
}
</script>

