    // Manage the social horizontal toggles on the landing pages.
requirejs(["jquery", "hcs/script"], 
		function($, unused0) {

      $(function(){
        $('.social').find('.toggle').click(function(e){
          e.preventDefault();
          $(this).closest('.social').toggleClass('visible');
        });
      });
});
