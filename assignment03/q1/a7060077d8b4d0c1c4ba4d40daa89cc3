    var slatebCookieCatch = function() {
      var cv,pv;
      var persist=false;
      var cn = "Origin_Type=";
      var cp = "persistTreatment=";
      var ca = document.cookie.split(';');
      for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(cn) == 0) {
          cv = c.substring(cn.length,c.length);
        }
        if (c.indexOf(cp) == 0) {
          pv = c.substring(cp.length,c.length);
          if (pv.indexOf('true') == 0) {
            persist = true;
          }
        }

      }
      
      if (cv != "beta" && persist == false) {
        document.cookie = "Origin_Type=beta; expires=Wed, 27 Nov 2013 18:41:25 GMT; path=/; domain=.slate.com";
      }
      
      };
