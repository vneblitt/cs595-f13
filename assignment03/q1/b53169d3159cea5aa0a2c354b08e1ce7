  <script type="text/javascript">
  function addCommas(nStr)
  {
  	nStr += '';
  	x = nStr.split('.');
  	x1 = x[0];
  	x2 = x.length > 1 ? '.' + x[1] : '';
  	var rgx = /(\d+)(\d{3})/;
  	while (rgx.test(x1)) {
  		x1 = x1.replace(rgx, '$1' + ',' + '$2');
  	}
  	return x1 + x2;
  }  
  jq(function(){
    jq('li.twitter-share-button a, li.facebook-share-button a').click(function(){
      try {
        var newwindow = window.open(jq(this).attr('href'), 'sharer', 'toolbar=0,status=0,width=626,height=436');
        if (!newwindow) {
          //popups blocked, just open in new window...
          return true;
        }
        if (window.focus) {
          newwindow.focus();
        }
        return false;
      } catch (e) { return true; }
    });
  });
  </script>

  <script type="text/javascript">
window.fbAsyncInit = function() {
  jq(function(){
  FB.api({
      method: 'fql.query',
      query: 'SELECT total_count FROM link_stat ' +
             'WHERE url="http://www.fbi.gov/wanted/seeking-info/aaron-alexis" ORDER BY total_count LIMIT 1;'
    },
    function(response) {
        try{
            var count = response[0].total_count;
            if(count !== undefined && count > 0){
                jq('li.facebook-share-button a span').
                    html(' (' + addCommas(response[0].total_count) + ')');
                jq('li.facebook-share-button a span').fadeIn();
            }
        }catch(e){}
    }
  );
  });
};</script>
  <script type="text/javascript">
  (function() {
  var e = document.createElement('script'); e.async = true;
  e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
    jq('head').append(e);
  }());
  </script>

  <script type="text/javascript">
jq(function(){
try{
jq.ajax({
    url : 'http://urls.api.twitter.com/1/urls/count.json?url=http://www.fbi.gov/wanted/seeking-info/aaron-alexis',
    dataType : 'jsonp',
    success : function(data){
        count = data['count'];
        if(count > 0){
            jq('li.twitter-share-button a span').
                html('(' + addCommas(count) + ')');
            jq('li.twitter-share-button a span').fadeIn();
        }
    }
});
}catch(ex){
    //do nothing..
}
});</script>




