<script>window.jQuery ||  document.write(unescape("%3Cscript src='http://news.harvard.edu/gazette/wp-content/themes/gazette20/a/j/jquery-1.9.1.min.js' type='text/javascript'%3E%3C/script%3E"))</script>
<script src="http://news.harvard.edu/gazette/wp-content/themes/gazette20/a/j/jquery.flexslider.js"></script>
<script src="http://news.harvard.edu/gazette/wp-content/themes/gazette20/a/j/jquery.tinyscrollbar.js?v=20130528"></script>
<script src="http://news.harvard.edu/gazette/wp-content/themes/gazette20/a/j/functions.js?v=20130709"></script>

<script>

$(window).scroll(function(){

	if ( getInternetExplorerVersion() != 8 ) {
		var $totalHeight = $('.sticky').outerHeight() + $('#logo').outerHeight() + $('.top-wrapper').outerHeight() + $('.feature-photo-side .article-feature').outerHeight() +  $('.article-feature-caption').outerHeight() + 10;

		var scroll_top = $(window).scrollTop();      

		if (scroll_top <= $totalHeight) {      
		$('#sticky-filter').hide();
		} 
		if (scroll_top > $totalHeight) {
		$('#sticky-filter').show();
		} 
	}
});

</script>

<script>
var prev_width = $(window).width();
var prev_height = $(window).height();

window.onresize = function(event) {
	var w_width = $(window).width();
	
	if ( ( w_width>1242 && prev_width<=1242 ) || ( w_width<=1242 && prev_width>1242 ) ) {
		if ( ( w_width>1242 && prev_width<=1242 ) ) {
			$('#nav-open-btn').hide();
			$('#nav-close-btn').hide();
			$('#nav-open-btn').removeAttr('style');
			$('#nav-close-btn').removeAttr('style');
		}
		prev_width = w_width;
	}
	
	var w_height = $(window).height();
	if ( w_height != prev_height ) {
		if ( use_tinyscroller() ) {
		$('#scrollbar1').tinyscrollbar({invertscroll: true});
		}
		else {
			set_scroller_height();
		}
	}
}

</script>
