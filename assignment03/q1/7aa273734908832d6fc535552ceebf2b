<script type="text/javascript">
	$(document).ready(function() {
		// convert all relative dates
		$("._relativeDate").each(function() {
			this.innerHTML = getRelativeTime(this.innerHTML);
			$(this).css("visibility", "visible");
		});

		// bind url "copy to clipboard" textboxes
		$("input._copyUrl").bind("click", function() {
			this.focus();
			this.select();
		});

		// bind all local scroll links
		$("._localScroll").live("click", function(e) {
			var href = $(this).attr("href");
			var name = href.substring(href.indexOf("#")+1);
			scrollToAnchor(name);
			e.preventDefault();
		});


					// popauth
			var p = new popauth("twitter", '/ajax/twitter/get-auth-url', function(e, ot, ov) {
				$.getJSON('/ajax/twitter/get-access-token?ot='+ot+'&ov='+ov, function(data) {
					if (data.success) {
						setTimeout(function(){ window.location = window.location; }, 0.1);	// refresh
					} else {
						displayUserMessage("Login failed, please try again");
					}
				});
			});
			// bind button
			$("#btnLogin").bind("click", function(e) {
				e.preventDefault();
				p.start();	// start auth
			});

		
		$('.listTabs .tab a').click(function(e) {
			if (!$(this).closest('.listTabs').hasClass('_nonAjax')) {
				if($(this).attr('rel') && $('#'+$(this).attr('rel')).length) {
				$(this).closest('.listTabs').find('.active').removeClass('active');
				$(this).parent().addClass('active');
				$(this).closest('.listWrapper').find('.listBoxes .listBox').hide();
					var target = $(this).attr('rel');
					$('#'+target).show();
				}
				e.preventDefault();
			}
		});

		$.preloadImages = function() {
			for (var i = 0; i<arguments.length; i++) {
				img = new Image();
				img.src = arguments[i];
			}
		}
		$.preloadImages(
			'http://static.ow.ly/4-9-34/images/new/maps/map-12-hover.png',
			'http://static.ow.ly/4-9-34/images/new/maps/map-12-active.png',
			'http://static.ow.ly/4-9-34/images/new/maps/map-19-hover.png',
			'http://static.ow.ly/4-9-34/images/new/maps/map-19-active.png',
			'http://static.ow.ly/4-9-34/images/new/maps/map-73-hover.png',
			'http://static.ow.ly/4-9-34/images/new/maps/map-73-active.png'
		);
	});
</script>

