                    $("#menu-mobile ul").toggle();
                });
                
                $('#search').live('click', function(){
                    $('#search_bg').fadeToggle();
                });
                
                $('.close-search').live('click', function(){
                    $('#search_bg').fadeOut();
                });
                
                

               /* $('.photo-slideshow').pxuPhotoset({
                    "lightbox" : true,
                    
                    "rounded" : false
                });*/
                
                /* ZOOM EFFECT - FANCYBOX */
                $content.on("focusin", function(){
                    $("a.zoomFancy, a.zoomFancyTab").fancybox({
                        /*'transitionIn'	:	'elastic',
                        'transitionOut'	:	'elastic',
                        'speedIn'		:	300, 
                        'speedOut'		:	200,
                        'hideOnContentClick': true,
                        'overlayShow'	:	true,*/
                        'padding'       :   [40,0,0,0]
                    });
                });
                
                /* if internet explorer 6-7 */	
                jQuery.each(jQuery.browser, function(i, val) {
                    if((val=="7.0")||(val=="6.0")||(val=="8.0")){
                        alert("You are using Internet Explorer " + val + ", please upgrade your browser.");
                        
                        $(window).resize(function() {
                            $("#header").css("width", "100%").css("width", "-=40px");
                        });
                    }
                });
                
                
                /* return top */
                if (!Modernizr.touch){
                    $("#backToTopButton").click(function(){
                        var target = $("html");  
                        $('html, body').animate({
                            scrollTop: target.offset().top  
                        }, 1000);
                    });
                   
                    var menu2 = $("#menu-2");
                    menu2.html($("#menu").clone());
                    var topDiv = $("#backToTopButton");
                    var topDivTop = "1000";
                    var view = $( window );
                    view.bind("scroll resize", function(){
                        var viewTop = view.scrollTop();
                        
                        if (viewTop > topDivTop){
                            topDiv.animate({"bottom": "0px"}, {queue: false, duration: 200});
                            
                                menu2.show().animate({top: "0px",opacity:0.95},{queue: false, duration: 200}).fade;
                            
                        }
                        else if (viewTop < topDivTop){
                            topDiv.animate({"bottom": "-70px"}, {queue: false, duration: 200});
                            
                                menu2.animate({top: "-50px",opacity:0},{queue: false, duration: 150});
                            
                        }
                
                    });
                }
                
                
                /* Twitter feed */
                
                var scrollApi;
                
                twitterFetcher.fetch('350002996547051520', '', 20, true, false, false, '', false, handleTweets);
                
                function handleTweets(tweets){
    	          var x = tweets.length;
		          var n = 0;
		          var element = document.getElementById('tweets');
		          var html = '<ul>';
		          while(n < x) {
		            html += '<li>' + tweets[n] + '</li>';
		            n++;
		          }
		          html += '</ul>';
		          element.innerHTML = html;
                  
                  $('#tweets').BTScroll({
                		hScroll:false,
            			vScroll:true,
            			vScrollbar:true,
            			hScrollbar:false,
                        bounce: true,
                        bounceLock:true,
                        topOffset:0
            	  });
                  
                  scrollApi = $('#tweets').data('BTScroll');

                  
		      }
              
              $(window).bind('resize', function(e) {
                  if(scrollApi) scrollApi.refresh();
              });

            });
            
        </script>
        
        
        
