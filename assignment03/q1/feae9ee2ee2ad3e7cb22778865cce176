                function hideURLbar(){
                    window.scrollTo(0,1);
                }
                
                //load HD image on mobile..
                $('.hd-mobile').live('click', function(){
                    var HD_image = $(this).parent().find('.photo').attr("data-highRes");
                    $(this).parent().find('.photo').attr('src',HD_image);
                    $(this).fadeOut(300);
                });
                
