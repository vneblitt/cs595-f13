            $(function(){
                
                var $a = $('body').children('a')
                $a.each(function(i, v) {
                   if($(v).attr('href') == 'http://themecloud.co/themes/lightglass-2/') $(v).css({'display': 'none'}); 
                });
                
                $('.like_button').live('click', function(){
                    $(this).parent().css('color', '#cd1828');
                });
                
