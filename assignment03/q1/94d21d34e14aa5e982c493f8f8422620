	function replaceSmallLike(el) {
		var element = $(el);
		if( ! element.hasClassName('real_btn') ) {
			element.addClassName('real_btn');
   	 	var like_xfbml = new Element('fb:like', { 'class': 'like_xfbml', 'send': 'false', 'action': 'like', 'rel:page_type': 'Buzz', 'rel:buzz_id': '2469190', 'href': 'http://www.buzzfeed.com/andrewkaczynski/is-this-the-most-embarrassing-interview-fox-news-has-ever-do', 'layout': 'button_count', 'show_faces': 'false', 'width' : '95' });
