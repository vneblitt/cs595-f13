		<li id="4368655" rel:buzz_id="2469190" rel:comment_id="4368655" class="contrib  bf_dom " rel:data='{"comment_id":"4368655","type":"text"}' rel:ptool="true">

	

	
	
	<div class="what notranslate"  id="contribwhat_4368655">
		<div class="user-panel">
									<div class="who">
				<a rel:gt_act="contrib/avatar" class="avatar" href="/stevet19"><img src="http://s3-ak.buzzfeed.com/static/user_images/webdr06/2013/8/6/19/stevet19-13880-1375832461-32.jpg" width="25" height="25" alt="" /></a>
