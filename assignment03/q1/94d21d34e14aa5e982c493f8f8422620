	    <a href="javascript:;" class="share-icon bf_dom sub-buzz-link-share" id="link2469190_1427852" rel:bf_bucket_data="{'link-share':{'id':'2469190_1427852'}}" rel:gt_act="share/link/list-item:1" rel:gt_label="http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4"></a>
		<a href="javascript:;" class="share-icon bf_dom sub-buzz-response-share hidden response-button" id="response2469190_1427852" rel:bf_bucket_data="{'response-share':{'id':'2469190_1427852', 'item':''}}" rel:gt_act="share/contribute/list-item:1" rel:gt_label="http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4"></a>
	</div>



  
   	<div class="video-embed-big video-embed-area" id="video_buzz_element_2469190_1427852" rel:thumb="http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.jpg"><span class="print">Video available at: <a href="http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4">http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4</a>.</span></div>
	<script type="text/javascript" charset="utf-8">
	/* <![CDATA[ */		
		BuzzLoader.register(function() {
			try {
				var agent=navigator.userAgent.toLowerCase();
				var is_mobile = agent.match(/iphone|ipod|android|ipad/);
							
				var vd = new bf2_terminal_Video;
				vd.detect_type("http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4");
				var src_link = "http://buzzfeed-video1.s3.amazonaws.com/video/2013/07/28/81-466x264.mp4";
				if (vd.type && vd.type != null) {
					src_link = vd.src_link;
				}

				var so = BF_initSwfObject(src_link,{width:625,height:376});

        // Flowplayer
				if (so.flowplayer) {
				  
				  so.height = 360;
					

					if (is_mobile) so.use_html5 = true;
					so.write("video_buzz_element_2469190_1427852");

				// SWF object / non-yt mobile
				} else if (is_mobile && vd.type == 'buzzfeed') {
					if (is_mobile) so.use_html5 = true;
					so.write("video_buzz_element_2469190_1427852");
				} else if ( (! is_mobile && so.installedVer && so.installedVer.major < 8) || ( is_mobile && vd.type != 'youtube') ) {
					$('video_buzz_element_2469190_1427852').addClassName('video-embed-noflash');
					$('video_buzz_element_2469190_1427852').style.height = '340px';
				} else {
					if (vd.type == 'youtube') {

						if(isIE8() || isIE7()) {
							Event.observe(window, 'beforeunload', function(){ //for ie8 we need to handle page exits in a special manner
								$$("object").each(function(vidPlayer) { //grab the player(s)
									vidPlayer.remove(); //remove it so js calls no longer occur on exit to non-existant objects
								});
							}, false);
							if(isIE8()) { //internet explorer doesn't have access to the src from SWFObject, so must pass in alternative manner
								so.addParam('wmode','opaque');
								$('video_buzz_element_2469190_1427852').fire("video:dfp", { src: src_link }); //fire off a message in a bottle
							}
                            					so.addParam("wmode", "opaque");
                            					
							so.write($('video_buzz_element_2469190_1427852')); //IE8 can't handle <embed>, must use SWFObject to get <object>
						} else {
							var start_time = vd.start_time ? '&start=' + vd.start_time : '';
							if(window.autoplay != undefined) var autoplay = window.autoplay ? 1 : 0;
							
							var you_tube_markup = '';
							var video_pl = [{
								video_id: vd.id,
								title: 'Is This The Most Embarrassing Interview Fox News Has Ever Done?',
								author: 'andrewkaczynski',
								uri: 'is-this-the-most-embarrassing-interview-fox-news-has-ever-do',
								thumbnail: vd.thumbs[2]
							}];

							


							if (video_pl.length == 1) {
							
								var you_tube_tmpl = new Template('<embed id="embed_2469190_1427852" src="#{src}" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="#{width}" height="#{height}" bgcolor="#000" wmode="opaque" class="ytvideo"></embed>');
								var you_tube_markup = you_tube_tmpl.evaluate({ src: src_link+'?version=3&hl=en&fs=1&enablejsapi=1&playerapiid=embed_2469190_1427852&autoplay='+ autoplay + start_time , width: 625, height: 376 });
							
							}

							$('video_buzz_element_2469190_1427852').innerHTML = you_tube_markup;
						}
	          } else if (vd.type == 'vimeo') {

	            var vimeo_tmpl = new Template('<iframe src="#{src}" width="#{width}" height="#{height}" frameborder="0"></iframe>');
	            var vimeo_markup = vimeo_tmpl.evaluate({ src: src_link+'?color=ee3322', width: 625, height: 376 });
	            $('video_buzz_element_2469190_1427852').innerHTML = vimeo_markup;
			} else {
					try{so.addParam("wmode", "opaque");} catch(e) {}
					so.write("video_buzz_element_2469190_1427852");
				}
					}
				} catch(e) {
					console.log(e);
				}

			}, 10);
	/* ]]> */
	</script>
  
 </div>

	
					<div class="sub_buzz_source_via buzz_attribution"><a href="/andrewkaczynski">buzzfeed.com</a>
    </div>
		
	

	
		
	






















</div>










