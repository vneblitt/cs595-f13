<script>
jQuery(function($){
    var fulltext_visible = false,
        supplement_data_visible = false;
    $("#page-fulltext-toggle").live("click", function(event){
        event.preventDefault();
        if( $(event.target).parents('.imageviewer_text').hasClass('enabled') ){
            if( !fulltext_visible ){
                setTimeout(function(){
                    $("#page-fulltext").fadeIn("slow");
                }, 500);
                fulltext_visible = true;
            }else{
                $("#page-fulltext").fadeOut("slow");
                fulltext_visible = false;
            }
        }
    });
    $("#page-data-toggle").live("click", function(event){
        event.preventDefault();
        if( $(event.target).parents('.imageviewer_text').hasClass('enabled') ){
            if( !supplement_data_visible ){
                setTimeout(function(){
                    $("#page-data").fadeIn("slow");
                }, 500);
                supplement_data_visible = true;
            }else{
                $("#page-data").fadeOut("slow");
                supplement_data_visible = false;
            }
        }
    });
});
</script>

<script type="text/javascript">
var currentPage = 1,
    showReferenceStrip = false;



var tileSources = [],
    resources = [
    [
        {
            "mimetype": "image/gif", 
            "url": "http://lcweb2.loc.gov/gmd/gmd436/g4360/g4360/mf000063.gif", 
            "height": 198, 
            "width": 150, 
            "levels": 1, 
            "size": 11217
        }, 
        {
            "url": "http://memory.loc.gov/cgi-bin/image-services/jp2.py?data=/home/www/data/gmd/gmd436/g4360/g4360/mf000063.jp2&res=3", 
            "width": 730, 
            "levels": 1, 
            "mimetype": "image/jpeg", 
            "height": 962
        }, 
        {
            "url": "http://memory.loc.gov/cgi-bin/image-services/jp2.py?data=/home/www/data/gmd/gmd436/g4360/g4360/mf000063.jp2&res=2", 
            "width": 1460, 
            "levels": 1, 
            "mimetype": "image/jpeg", 
            "height": 1924
        }, 
        {
            "mimetype": "image/jp2", 
            "url": "http://lcweb2.loc.gov/gmd/gmd436/g4360/g4360/mf000063.jp2", 
            "height": 7697, 
            "width": 5841, 
            "levels": 7, 
            "size": 6790791
        }, 
        {
            "mimetype": "image/tiff", 
            "url": "http://lcweb2.loc.gov/gmd/gmd436/g4360/g4360/mf000063.tif", 
            "height": 7697, 
            "width": 5841, 
            "levels": 1, 
            "size": 134894108
        }
    ]
],
	levels = [],
    source,
    i, j;
    
for( i = 0; i < resources.length; i++ ){
    for( j = 0; j < resources[ i ].length; j++ ){
        source = resources[ i ][ j ];
        if( source.mimetype == "image/jp2" || source.mimetype == "image/mrsid" ){
            tileSources.push({
                source:           source,
                files:            resources[ i ],
                width:            source.width,
                height:           source.height,
                tileSize:         512, 
                tileOverlap:      0, 
                minLevel:         0, 
                maxLevel:         source.levels - 1,
                preserveViewport: true,
                getTileUrl: function( level, x, y ) {
                    serverPattern = /http:\/\/[^\/]+/;
                    var jp2Server = serverPattern.exec(this.source.url),
						jp2_file = this.source.url.replace(
							jp2Server, 
							"/home/www/data"
						), 
						i;    
                    
					for( i = this.source.levels -1 ; i >= 0; i--){
						levels.push(i);
					}
                        
                    // For maps jp2 stuff:
                    //   x,y  is center of tile adjusted for scale
                    //   level is 0 based and reversed compared to seadragon
                    var x1    = this.tileSize / this.getLevelScale( level ) * x,
                        y1    = this.tileSize / this.getLevelScale( level ) * y,
                        delta = ( this.tileSize / this.getLevelScale( level ) ) / 2;

					return jp2Server + '/cgi-bin/image-services/jp2.py?' +
                        '&data=' + jp2_file + 
                        '&x=' + ( x1 + delta )  +
                        '&y=' + ( y1 + delta ) +
                        '&res=' + levels[ level ] +
                        '&width=' + this.tileSize +
                        '&height=' + this.tileSize +
                        '&jpegLevel=' + 80;
				}
			});
		}
	}
}
    


var viewer  = new OpenSeadragon.Viewer({
    id:                 'viewer-container',
    toolbar:            'ctrl_imageviewer',
    prefixUrl:          '/search/static/js/openseadragon/',
    showNavigator:      true,
    showReferenceStrip: tileSources.length > 1 ? true : false,
    referenceStripScroll: 'vertical',
    autoHideControls:   false,
    nextButton:         'next',
    previousButton:     'previous',
    zoomInButton:       'zoom-in',
    zoomOutButton:      'zoom-out',
    homeButton:         'home',
    fullPageButton:     'full-screen',
    tileSources:        tileSources,
    preserveViewport:   true,
    
    onPageChange:       function( data ){
        var downloadArea = $("#ctrl_imageviewer .files"),
            page = data.page + 1,
            hash = location.hash,
            files = viewer.tileSources[ page - 1 ],
            hasText = false,
            label,
            link,
            match,
            i;
        
        if( 0 <= page <= viewer.tileSources.length ){
            match = hash.match(/seq-(\d*)/);
            if( match ){
                match = match[1];
                hash = hash.replace('seq-'+match, 'seq-'+page);
            }else{
                hash = hash + 'seq-' + page;
            }
            location.hash = hash;
        }    
        checkPage();
        
        if( files.levels ||  files.files ){
            $( downloadArea ).empty();
            files = files.levels || files.files;
            $( "#page-data" ).empty();
            $( "#page-fulltext" ).empty();
            for( i = 0; i < files.length; i++ ){
                if( files[i].fulltext ){
                    label = "Text";
                    link = "#page-fulltext";
                    hasText = true;
                    $( "#page-fulltext" ).html(files[i].fulltext);
                    continue;//no pipe if we dont add a link
                }else if( files[i].mimetype.match('json') ){
                    label = "Info";
                    link = "#page-data";
                    $( "#page-data" ).append(
                        $("<a href='"+files[i].resource+"'>"+files[i].title+"</a>")
                    );
                    continue;//no pipe if we dont add a link
                }else{
                    label = files[i].mimetype.substring(
                        files[i].mimetype.indexOf('/') + 1,
                        files[i].mimetype.length 
                    ).toUpperCase()
                    
                    if (files[i].size || files[i].width){
                        label = label + "(" + (
                            files[ i ].size ?
                                Math.round( Number( files[ i ].size )/1000 ) + " KB" :
                                files[ i ].width + " x " + files[ i ].height + "px"
                        ) + ")";
                    }
                    
                    link = files[i].url;
                    $( downloadArea ).append(
                        '<li class="imageviewer_fit">\
                            <a href="'+link+'" >'+label+'</a>\
                        </li>'
                    );
                }
                
                if( ( i + 1 ) < files.length ){
                    $( downloadArea ).append(
                        "<span class='quiet'>&nbsp;|&nbsp;</span>"
                    );
                }
            }
        }
        
        if( hasText ){
            $('#page-fulltext-toggle').parents('.imageviewer_text').addClass('enabled');
        }else{
            $('#page-fulltext-toggle').parents('.imageviewer_text').removeClass('enabled')
        }
        
    }
});
    
viewer.addHandler('open', checkPage);

$('form.imageviewer_page_form').submit(function(event){
    event.stopPropagation();
    event.preventDefault();
    
    var hash = location.hash,
        match = hash.match(/seq-(\d*)/),
        page = $("#page_number").attr('value');
    if( Number(page) <= tileSources.length  ){
        if( match ){
            match = match[1];
            hash = hash.replace('seq-'+match, 'seq-'+page);
        }else{
            hash = hash + 'seq-' + page;
        }
        location.hash = hash;
        checkPage();
    }else{
        $("#page_number").attr('value', currentPage)
    }
    return false;
    
});
var initialSet = false;
var checkPage = function(){

    var hash = location.hash,
        match = hash.match(/seq-(\d*)/) ?
            hash.match(/seq-(\d*)/) :        
            ["seq-"+currentPage, currentPage],
        page,
        img;
        
    viewer.removeHandler('open', checkPage);
    
    
    if( match && viewer.tileSources.length > 1 ){
        page = Number(match[1]);
        if( page != currentPage || !initialSet ){
            initialSet = true;
            currentPage = page;
            OpenSeadragon.requestAnimationFrame(function(){
                $("#page_number").attr('value', currentPage);
                viewer.goToPage( page - 1 );
                jQuery("#pages .pagination > strong").text(
                    page + " of " + viewer.tileSources.length
                )
                img = jQuery('#previous > img').attr('src');
                if( page > 1 ){
                    if( img && img.match( /.*off.png$/ ) ){
                        jQuery('#previous > img').attr(
                            'src', 
                            img.replace('off.png', 'on.png')
                        );
                    }
                } else {
                    if( img && img.match( /.*on.png$/ ) ){
                        jQuery('#previous > img').attr(
                            'src', 
                            img.replace('on.png', 'off.png')
                        );
                    }
                }
                
                img = jQuery('#next > img').attr('src');
                if( page < viewer.tileSources.length ){
                    if( img && img.match( /.*off.png$/ ) ){
                        jQuery('#next > img').attr(
                            'src', 
                            img.replace('off.png', 'on.png')
                        );
                    }
                } else {
                    if( img && img.match( /.*on.png$/ ) ){
                        jQuery('#next > img').attr(
                            'src', 
                            img.replace('on.png', 'off.png')
                        );
                    }
                }
                //alert("page "+ page)
            });
        }
    }
}
checkPage();

</script>


            
        
	   

        
    
</div>
    

                <div class='clear'></div>
            </div>
        </div>
    
    </div>
    
