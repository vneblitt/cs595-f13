<script>
LOCSuggest = {
	serviceUrl: 'http://www.loc.gov/search/suggest/'
};
CDN_URL = 'http://cdn.loc.gov/';
SUGGEST_URLS= {
    JQUERY:'http://cdn.loc.gov/js/lib/jquery-1.5.1.min.js',
    SUGGEST:'http://cdn.loc.gov/suggest/loc.suggest-1.1.1.js'
};
MEDIA_URL = 'http://media.loc.gov';
</script>
<script src='http://cdn.loc.gov/js/LAB-2.0.3.js'></script>
<script src='http://cdn.loc.gov/js/lib/jquery-1.7.2.js'></script>
<script src='http://cdn.loc.gov/js/lib/modernizr-1.5.min.js'></script>
<script src='http://cdn.loc.gov/sites/search.js'></script>
<script type="text/javascript">		
jQuery(function(){	
    //jquery ui 1.8.10 autocomplete collides with our autocomplete
    $.fn.autocomplete = null;
        
    //during development we like to hijax some urls to play with their
    //presentation in the project-1 national library layouts
    if( window.location.search.match("project=1") ){
        
        $("a").bind( 'click', function( event ){
            var target = event.currentTarget,
                base,
                id;
                
            if( target.tagName.toLowerCase() == 'a' ){
                //set the base url depending on where you are running
                //you development server. eg http://localhost:8082/search
                base = window.location.href.substring( 
                    0,
                    window.location.href.indexOf( '/search' ) 
                ) + "/item/";
                
                if ( $(target).attr( 'rel' ).indexOf('/item/') != -1 ){
                    id = $(target).attr('rel');
                    id = id.substring(
                        id.indexOf( '/item/' ) + 6,
                        id.length 
                    );
                }
                
                if( base && id ){
                    //hijax result to present in p1 format
                    target.href = base + id;
                }
            }
        });
    }
});
</script>


    
