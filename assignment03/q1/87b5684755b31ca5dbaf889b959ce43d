<script>
function setSearchBoxBg(m)
{
	o = document.getElementById("q");
	if(m==1)
	{
		o.style.background = '#FFFFFF url(/files/inc/graphics/image/gif/unfccc_google_search.gif) left no-repeat'; 
	}
	else
	{
		o.style.background = '#ffffff';
	}
}
setSearchBoxBg(1);
</script>


