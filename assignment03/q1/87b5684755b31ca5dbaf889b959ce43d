<script>
function addjustCssRules(s,c)
{
	nR = document.styleSheets.length;
	for(iR = 0; iR < nR; iR++)
	{
		checkThisCss = true;
		mysheet = document.styleSheets[iR];
		if(mysheet.href)
		{
			checkThisCss = false;
		}

		if(checkThisCss == true)
		{
			myrules = false;
			if (document.styleSheets[iR].cssRules)
			{
				myrules=document.styleSheets[iR].cssRules;
			}
			else if (document.styleSheets[iR].rules)
			{
				myrules=document.styleSheets[iR].rules;
			}
			if(myrules)
			{
				for (i=0; i<myrules.length; i++)
				{
					if(myrules[i].selectorText==".contpart")
					{
						if(document.styleSheets[iR].cssRules)
						{
							document.styleSheets[iR].cssRules[i].style.color = c;
							document.styleSheets[iR].cssRules[i].style.fontSize = s+"%";
						}
						if(document.styleSheets[iR].rules)
						{
							document.styleSheets[iR].rules[i].style.color = c;
							document.styleSheets[iR].rules[i].style.fontSize = s+"%";
						}
					}
					if(myrules[i].selectorText==".contpart table")
					{
						if(document.styleSheets[iR].cssRules)
						{
							document.styleSheets[iR].cssRules[i].style.color = c;
						}
						if(document.styleSheets[iR].rules)
						{
							document.styleSheets[iR].rules[i].style.color = c;
						}
					}
				}
			}
		}
	}
}
</script>


