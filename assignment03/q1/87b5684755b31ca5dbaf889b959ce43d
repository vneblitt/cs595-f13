<script>

function locateToHomePage()
{
	document.location.href="/";
}

function XBrowserAddHandler(target,eventName,handlerName)
{ 
    if ( target.addEventListener )
        target.addEventListener(eventName, handlerName, false);
    else if ( target.attachEvent )
        target.attachEvent("on" + eventName, handlerName);
    else
        target["on" + eventName] = handlerName;
}

function makeLogoClickable()
{
	if(document.body.getElementsByTagName("img")[2])
	{
		var logo = document.body.getElementsByTagName("img")[2];
		
		var logoFound = logo.src.search(/logo.+/);
		if (logoFound != -1)
		{
			logo.style.cursor = "pointer";
			XBrowserAddHandler(logo, "click", new Function("locateToHomePage()"));
		}
	}
}

function getMainElement()
{
	i = 0;
	if(document.body)	return document.body
	a = document.getElementsByTagName("table");
	while (element = a[i++])
	{
		if (element.className == "contentContainer")
		{
		      return element;
		}
	}
	return false;
}
function registerMousUpEvent(ie)
{
	if(document.getElementById("searchProposalList"))
	{
		obj = getMainElement();
		obj.setAttribute("onmouseup","cleanSuggestions();");
		if(ie)
		{
			if(ie==6) document.getElementById("qBox").style.width = "176px";
			if(ie==7) document.getElementById("qBox").style.width = "176px";
			if(ie==8) document.getElementById("qBox").style.width = "176px";
		}
	}
}
registerMousUpEvent(ie);
</script>

