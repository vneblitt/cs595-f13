  padding-left:5px!important
}
.gog.left-rail .ideas-finder-head {
	text-indent:-9999px;
	background: transparent url(http://www.washingtonpost.com/wp-srv/gog/images/icons/gog-logo-idea-finder.png) no-repeat scroll 70px 0px;
	width:276px;
	height:26px;
}
.gog.left-rail .ideas-finder-head .rule{
	top:-3px;
	width:65px;
}

/*end:gog-left-rail-ideas-finder.css*/
				</style>
				<script type="text/cjs">
					//start: gog-left-rail-ideas-finder.js
$(".gog.left-rail .ideasForm #keyword").bind("focus", function() {
	var self = $(this);
	var suggestText=self.attr("suggest_text");
	if (self.attr("value") == suggestText) {
		self.attr("value", "");
	}
});
$(".gog.left-rail .ideasForm #keyword").bind("blur", function() {
	var self = $(this);
	var suggestText=self.attr("suggest_text");
	if (self.attr("value") == "") {
		self.attr("value", suggestText);
	}
});


$("form#gog-ideas-finder").submit(function(){
	//clear suggest text
	var suggestText=$(".gog.left-rail .ideasForm #keyword").attr("suggest_text");
	if ($(".gog.left-rail .ideasForm #keyword").val()==suggestText||$(".gog.left-rail .ideasForm #keyword").val()=="" ){
		$(".gog.left-rail .ideasForm #keyword").val("#");
	}
	$(".gog.left-rail .ideasForm #gog-ideas-finder select").each(function(){
		var self=$(this);
		if (self.val()==""){ //if select has no valule, remove it from form submit
			self.attr("name","");
		} else if (self.find("option:selected").attr("type_group")=="true"){ //if select has "type_group" attr and equals "true", change the name of field to "group"
			self.attr("name","group");
		} else { //else, restore its "name" attribute from "name_field" attribute.
			self.attr("name",self.attr("name_field"));
		}
	})
})
$(".gog.left-rail .ideasForm #gog-ideas-finder a.gogButton").click(function(){
	$("form#gog-ideas-finder").submit();
})

/*open table and ideas finder omniture tracking*/

//idea finder keyword
$(".gog.left-rail #keyword").on("focusin",function(){
	logEvent("gog",$(this).attr("omniture-tracking"));
});
//idea finder select
$(".gog.left-rail select[omniture-tracking*='left rail']").on("change",function(){
	logEvent("gog",$(this).attr("omniture-tracking"));
});
//idea finder checkbox
	$(".gog.left-rail input[type='checkbox'][omniture-tracking*='left rail']").on("click",function(){
	logEvent("gog",$(this).attr("omniture-tracking"));
});
	
$(".gog.left-rail form[omniture-tracking*='left rail']").on("submit",function(){
	logEvent("gog",$(this).attr("omniture-tracking"));
});

//end: gog-left-rail-ideas-finder.js
				</script>
