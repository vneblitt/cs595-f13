    <script type="text/javascript">
        if (document.location.href.indexOf('template=desktop') === -1) {
            if (!(document.cookie.indexOf("sd=") == 0 || document.cookie.indexOf("; sd=") >= 0 )) {
                if (window.screen.width*window.screen.height/Math.pow((window.devicePixelRatio == undefined || navigator.userAgent.match(/(iPhone|iPod)/i) ? 1 : window.devicePixelRatio),2) < 480000) {
                    // checking if it's an iframe
                    var iframe_param = document.location.href.indexOf('template=iframe') !== -1 ? window.location.search : '';
                    window.location="m/" + iframe_param;
                }
            }
            if ((document.cookie.indexOf("sd=1") == 0 || document.cookie.indexOf("; sd=1") >= 0 )) {
                document.cookie= "sd=;expires=Thu, 01 Jan 1970 00:00:01 GMT;domain=.washingtonpost.com;"; // clear cookie
            }
        }
    </script>
