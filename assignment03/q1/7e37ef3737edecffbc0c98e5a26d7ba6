                    var tp_ad_zone5 = "<!-- TP_Ad_Zone5 -->\n    <div id='div-gpt-ad-1380123697713-0' style='width:728px; height:90px;' class='banner-ad'>\n        <script type='text\/javascript'>\n            googletag.cmd.push(function() { googletag.display('div-gpt-ad-1380123697713-0'); });\n        <\/script>\n    <\/div>";
            jQuery("#ad_zone5").append(tp_ad_zone5);
            } else {
        // the full nav is not visible, so assume this is a mobile display
    }

    // display single post and sidebar ads on both desktop and mobile    
            var tp_ad_zone1 = "<!-- TP_Ad_Zone1 -->\n   <div id='tp-ad-zone1' style='width:100%'><\/div>\n   <script type='text\/javascript'>\n       window._taboola.push({ article: 'auto' });\n       _taboola.push({ mode: 'autosized-generated-2r-bmc', container: 'tp-ad-zone1', placement: 'bmc' });\n   <\/script>";
        jQuery("#ad_zone1").append(tp_ad_zone1);
    
            var tp_ad_zone2 = "<!-- TP_Ad_Zone2 -->\n    <div id='div-gpt-ad-1380123695885-0' style='width:300px; height:250px;' class='side-box'>\n    <script type='text\/javascript'>\n        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1380123695885-0'); });\n    <\/script>\n    <\/div>";
        jQuery("#ad_zone2").append(tp_ad_zone2);
    
            var tp_ad_zone3 = "<!-- TP_Ad_Zone3 -->\n    <div id='div-gpt-ad-1380123696490-0' style='width:300px; height:250px;' class='side-box'>\n    <script type='text\/javascript'>\n        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1380123696490-0'); });\n    <\/script>\n    <\/div>";
        jQuery("#ad_zone3").append(tp_ad_zone3);
    
            var tp_ad_zone8 = "<!-- TP_Ad_Zone8 -->\n    <div id='tp-ad-zone8' class='side-box'><\/div>\n    <script type='text\/javascript'>\n        window._taboola.push({ home: 'auto' });\n        _taboola.push({ mode: 'thumbs-2r', container: 'tp-ad-zone8', placement: 'right-rail' });\n    <\/script>";
        jQuery("#ad_zone8").append(tp_ad_zone8);
        
    // display the newswhip widget on both desktop and mobile
    var newswhipWidget = "<script src=\"http:\/\/www.newswhip.com\/WidgetJavaScriptAction?style=thinkprogress&widgetRequested=id_4&count=5&removeInteractions=false&removePreview=false&removeImage=false&widgetTitle=Most Shared on Health\" type=\"text\/javascript\">\r\n<\/script>\r\n<div id=\"newswhip-trending\"><\/div>\r\n<div class=\"tp-powered\">\r\n\t<span>Powered by <a href=\"http:\/\/www.newswhip.com\/\" target=\"_blank\">NewsWhip<\/a><span class=\"ico\"><\/span><\/span>\r\n<\/div>";
    jQuery("#newswhip-widget").append(newswhipWidget);

    // display the sidebar donate ad on either desktop or mobile
    var sidebarDonateAd = "<a href=\"\/donate\/\"><img src=\"http:\/\/thinkprogress.org\/wp-content\/themes\/tp4\/images\/donate.jpg\" class=\"side-ad\" alt=\"Donation Ad\"><\/a>";
    jQuery("#sidebar-donate-ad").append(sidebarDonateAd);

});

