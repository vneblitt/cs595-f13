<script type="text/cjs">
		
(function($){
	var wapo_login_id = TWP.StaticMethods.getCookie("wapo_login_id");
	var s_vi = TWP.StaticMethods.getCookie("s_vi");
	
	var url = "http://md1.washingtonpost.com/search-recommendation/recommend.jsonp?callback=?&startat=1&count=5&types=Article,Blog";
	if ( !!wapo_login_id ) { url += "&wapo_login_id="+wapo_login_id;}
	if ( !!s_vi ) { url += "&s_vi=="+s_vi; }

	$.getJSON(url,function(data){
		if(data == null || data.results == undefined || data.results == null){
			return;
		}
		var results = data.results;

		var output = '<style>.module.recommended-articles ul li{font-weight:normal;}</style><div class="module recommended-articles"><p class="heading heading2">The Washington Post Recommends</p><ul class="normal">{{results}}</ul></div>';

		var lis = '';
		for(var i=0;i < data.results.length; i++){
			var result = data.results[i];
			var klass = (i==0) ? ' class="first"' : (i+1==data.results.length) ? ' class="last"' : '' ;
			var url = result.url ;
			var tid = 'tid=btm_rex_'+(i+1);
			var delim = '?';
			if ( url.indexOf('?') != -1 ) {
				delim = '&';
			}
			url += delim+tid;
			lis += '<li'+klass+'><a href="'+url+'">'+result.headline+'</a></li>'
		}

		if ( !!lis ) {
			output = output.replace("{{results}}",lis);
			$(output).insertAfter('#recommended-articles-marker');
		}
	});

})(jQuery);
</script><!--/wp-row container of content-->
