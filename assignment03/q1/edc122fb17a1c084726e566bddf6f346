        <script type="text/javascript">
          try {
            if (jQuery) {
              jQuery(document).ready(function($) {
                /* Add click handling to toggleable elements. */
                $("#DateIndex li.toggle").click(function(event) {
                  /* Only respond to clicks on the toggleable element. */
                  if (!$(event.target).is("li.toggle ul a")) {
                    event.preventDefault();
                    $(this).siblings("li.toggle").removeClass("selected");
                    $(this).siblings("li.toggle").find("ul").hide();
                    $(this).toggleClass("selected");
                    $(this).find("ul").toggle();
                  }
                  console.log($(event.target).not("li.toggle ul a"));
                });
              });
            }
          }
          catch (e) {
            
          }
        </script>
