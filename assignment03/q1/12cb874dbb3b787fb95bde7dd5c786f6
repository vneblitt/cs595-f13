  <script>
    var $ = jQuery;
    function tallyChamberCounts(positions) {
      var categories = _.chain(positions).map(function(member) {
        return member.newCategory;
      }).uniq().value();

      var ordered_category= ["broad authorization","limited authorization","undecided","unknown","no action"]
      var category_counts = _.map(ordered_category,function(category){
       return  _.select(positions, function(inner_cat) {
          return (inner_cat.newCategory === category);
        }).length;
      });

      return {
        'support': category_counts[0] + category_counts[1],
        'undecided': category_counts[2],
        'unknown': category_counts[3],
        'against': category_counts[4]
      }
    }

    function renderVotes(chamber, votes) {
      _.each(_.pairs(votes), function(option) {
        var selector = '#' + option[0] + ' .' + chamber;
        $(selector).text(option[1]);
      });
    }

    var house_votes = tallyChamberCounts(houseData);
    var senate_votes = tallyChamberCounts(senateData);
    renderVotes('house', house_votes);
    renderVotes('senate', senate_votes);
  </script>

</div -->
