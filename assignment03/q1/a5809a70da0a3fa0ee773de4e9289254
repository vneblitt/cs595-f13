    //<!--

 var selectedTab; //make it global

//for reading query string
function getParameterByName(name) {
  name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
  var regexS = "[\\?&]" + name + "=([^&#]*)";
  var regex = new RegExp(regexS);
  var results = regex.exec(window.location.href);
  if(results == null) {
return 'tab2';
  }
  else {
    return decodeURIComponent(results[1].replace(/\+/g, " "));
  }
}


(function($) {


  $(document).ready(function() {
    //Default Action
    $(".tab_content").hide(); //Hide all content
    var tabFlag = getParameterByName('tab');
    if (!tabFlag || tabFlag === 'tab1') {
        selectedTab = 'tab1';
    } else {
       selectedTab = 'tab2';
    }

    if (selectedTab === 'tab1') {
        $("ul.tabnav li:first").addClass("active").show(); //Activate first tab
        $(".tab_content:first").show(); //Show first tab content
    } else {
        $("ul.tabnav li:eq(1)").addClass("active").show(); //Activate first tab
        $(".tab_content:eq(1)").show(); //Show first tab content
    }

    //On Click Event
    $("ul.tabnav li").click(function() {
      $("ul.tabnav li").removeClass("active"); //Remove any "active" class
      $(this).addClass("active"); //Add "active" class to selected tab
      $(".tab_content").hide(); //Hide all tab content
      //Find the rel attribute value to identify the active tab + content
      var activeTab = $(this).find("a").attr("href");
      selectedTab = activeTab.substring(1);
      $(activeTab).fadeIn(); //Fade in the active content
      return false;
    });

    $('a.tabLink').click(function(event) {
      var link = $(event.target);
      if ('' != $(link).attr('rel')) {
        var tab = $(link).attr('rel');
        $('#' + tab).click();
      } else {
        var tab = $(link).attr('href');
        $(tab).click();
        return false;
      }
    });
  });
})(jQuery);
    //-->
</script>    </head>

