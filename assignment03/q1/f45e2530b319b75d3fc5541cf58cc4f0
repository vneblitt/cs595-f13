// Replace span.comments with a.comments
// If we place the <a> inline, chrome renders it outside of the main
// figure link.
$("span.comments").each(function() {
  var new_element = $("<a/>");
  $.each(this.attributes, function(i, attrib) {
    $(new_element).attr(attrib.name, attrib.value);
  });
  $(this).replaceWith(new_element);
});


