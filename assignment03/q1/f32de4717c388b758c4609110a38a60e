    <script>
        
        jQuery(document).ready( function () {
            jQuery('.careeronestop-occupation-field').each( function () {
                declareCareerOneStopOccupationBox(jQuery(this));
            });
        });
        
        function declareCareerOneStopOccupationBox(textboxObject) {
        
            jQuery(document).ready( function () {
                            
                textboxObject = jQuery(textboxObject);
                
                var faceClone = textboxObject.clone();
                faceClone.removeAttr('id');
                faceClone.removeAttr('wiztag');
                faceClone.insertAfter(textboxObject);
                faceClone.get(0).faceOf = textboxObject;
                textboxObject.hide();
                faceClone.css('cursor', 'pointer');
                faceClone.val('Click here to select an Occupation');
                faceClone.css('color', '#1a6894');
                faceClone.bind('click', function () {
                    
                    lastColorBoxTrigger = jQuery(this);
                    
                    selectedCode = 0;
                    jQuery.colorbox({
                        html: jQuery('.occupationlookup-container-outter').html(),
                        width: '575px',
                        height: '575px'
                    });
                    
                    // Wait for the colorbox to display, then run this code-block
                    setTimeout( function () {
                        jQuery('.occupationlookup-form:visible').attr('onsubmit', 'return false;');
                        jQuery('.occupationlookup-form input:visible').focus();
                        jQuery('.occupationlookup-form-text-input:visible').focus();
                        declareCareerOneStopOccupationBox_colorBoxIsNowShown();
                        jQuery('.occupationlookup-form-text-input:visible').focus();
                    }, 500);
                });
            });
        }

        function declareCareerOneStopOccupationBox_colorBoxIsNowShown() {
        
            // Event handeler for when the user preses enter on the imput box
            jQuery('.occupationlookup-form:visible').bind('submit', function (e) {
                var jqThis = jQuery(this);
                
                // Show loading spinner
                jQuery('.occupationlookup-results-container').css('text-align', 'center');
                jQuery('.occupationlookup-results-container').html('<img style="padding: 30px;" src="/sites/all/themes/bususa/images/ajax-loader.gif" />');
                
                careerOneStopAPI_findOccupationCodesByKeyword(jqThis.find('input').val(), function (data) {
                    last_return_careerOneStopAPI_findOccupationCodesByKeyword = data;
                    if ( typeof data != 'string' ) {
                        
                        // Clear the area
                        jQuery('.occupationlookup-results-container').css('text-align', '');
                        jQuery('.occupationlookup-results-container').html(' ');
                        
                        for ( var x = 0 ; x < data.length ; x++ ) {
                            var newResultItem = jQuery('\
                                <div class="occupationlookup-result" occtitle="' + data[x]['OccupationTitle'] + '" occupationcode="' + data[x]['OccupationCode'] + '"> \
                                    <div class="occupationlookup-result-title"> \
                                        ' + data[x]['OccupationTitle'] + ' \
                                    </div> \
                                    <div class="occupationlookup-result-detail"> \
                                        ' + data[x]['OccupationDescription'] + ' \
                                    </div> \
                                </div> \
                            ');
                            jQuery('.occupationlookup-results-container').append(newResultItem);
                        }
                        
                        // Event handeler for when the user clicks on a result/occupation
                        jQuery('.occupationlookup-result').bind('click', function () {
                            var jqThis = jQuery(this);
                            selectedCode = jqThis.attr('occupationcode');
                            lastColorBoxTrigger.val( jqThis.attr('occtitle') + ' [Click to Change]' );
                            lastColorBoxTrigger.get(0).faceOf.val(selectedCode);
                            
                            // Clear the area
                            jQuery('.occupationlookup-results-container').css('text-align', '');
                            jQuery('.occupationlookup-results-container').html(' ');
                            
                            jQuery.colorbox.close();
                        });
                    }
                });
                
                e.preventDefault();
                return false;
            });
            
            jQuery('.occupationlookup-form:visible').attr('onsubmit', 'return false;');
        }
        
        function careerOneStopAPI_findOccupationCodesByKeyword(searchTerm, callbackFunction) {
            var url = '/sys/careerOneStopAPI_findOccupationCodesByKeyword?search=' + searchTerm;
            jQuery.get(url, callbackFunction);
        }
        
        // Custom JavaScript for the SelectUSA/"Invest in the USA" wizard
        jQuery(document).ready( function () { /* Coder Bookmark: CB-ROSLF2M-BC */

            // When the user clicks the "Is your business located within the U.S" checkbox, reset the values of the questions which (visibility wise) depends on this question
            jQuery('.wizard-container.wizard-uniqueid-selectusa .wizard-tag-business_is_in_usa_yes').bind('click', function () { /* Coder Bookmark: CB-DX08AOM-BC */
                jQuery('input.wizard-tag-location_location_textbox').val('or type a location');
                jQuery('.wizard-question-step-id-1.wizard-question-question-id-2 select').val('Select a State');
                jQuery('.wizard-question-step-id-1.wizard-question-question-id-2 select').change();
            });
            
            // When the user clicks the "Is your business located outside the U.S" checkbox, reset the values of the questions which (visibility wise) depends on this question
            jQuery('.wizard-container.wizard-uniqueid-selectusa .wizard-tag-business_is_in_usa_no').bind('click', function () { /* Coder Bookmark: CB-WDYLJI6-BC */
                jQuery('.wizard-question-step-id-1.wizard-question-question-id-5 select').val('Select a State');
                jQuery('.wizard-question-step-id-1.wizard-question-question-id-5 select').change();
            });
            
            // When the user clicks the "Have you identified one or more U.S. locations you are interested in locating your investment" checkbox, reset the values of the questions which (visibility wise) depends on this question
            jQuery('.wizard-container.wizard-uniqueid-selectusa .wizard-question-step-id-2.wizard-question-question-id-0').bind('click', function () { /* Coder Bookmark: CB-LLSCB72-BC */
                jQuery('.wizard-question-step-id-2.wizard-question-question-id-1 select').val('Select a State');
                jQuery('.wizard-question-step-id-2.wizard-question-question-id-1 select').change();
            });
        
            // When the textbox under "Please enter the location" gain focus, make the "or type a location" text dissappear.
            jQuery('.wizard-container.wizard-uniqueid-selectusa input.wizard-tag-location_location_textbox, .wizard-container.wizard-uniqueid-selectusa input.wizard-tag-investment_location_textbox').bind('focus', function () { 
                /* Coder Bookmark: CB-4TBNE77-BC */
                var jqThis = jQuery(this);
                if ( jqThis.val() == 'or type a location' ) {
                    jqThis.val('');
                    jqThis.css('color', 'black')
                }
            });

            // When the textbox under "Please enter the location" losses focus, make the "or type a location" text reappear when nessesary.
            jQuery('.wizard-container.wizard-uniqueid-selectusa .wizard-tag-location_location_textbox, .wizard-container.wizard-uniqueid-selectusa input.wizard-tag-investment_location_textbox').bind('blur', function () { 
                /* Coder Bookmark: CB-WG20JJA-BC  */
                var jqThis = jQuery(this);
                if ( jQuery.trim(jqThis.val()) == '' ) {
                    jqThis.val('or type a location')
                    jqThis.css('color', '#696969')
                }
            });
            
        });
        
        /* string wizardSelectUSA_generateEmailMessage()
         * Generates an email message to auto-populate the email form in the SelectUSA Wizard based 
         * on the questions/anwsers the user supplied in the SelectUSA Wizard.
         */
        function wizardSelectUSA_generateEmailMessage() { /* Coder Bookmark: CB-J52M1ZJ-BC */
            var autoMessage = '';
            
            var location_location_dropdown_value = jQuery.trim(jQuery('.wizard-question-step-id-1.wizard-question-question-id-2 option:checked').text());
            if ( location_location_dropdown_value == 'Select a State' ) {
                location_location_dropdown_value = '';
            }
            var location_location_textbox_value = jQuery('input[wiztag="location_location_textbox"]').val();
            if ( location_location_textbox_value == 'or type a location' ) {
                location_location_textbox_value = '';
            }
            if ( location_location_dropdown_value != '' && location_location_textbox_value != '' ) {
                location_location_dropdown_value += ' and ';
            }
            var location_location_value = location_location_dropdown_value + location_location_textbox_value;
            if ( jQuery.trim(location_location_value) !== '' ) {
                location_location_value = ', in ' + location_location_value;
            }
            
            
            
            var investment_location_dropdown_value = jQuery.trim(jQuery('.wizard-question-step-id-2.wizard-question-question-id-1 option:checked').text());
            if ( investment_location_dropdown_value == 'Select a State' ) {
                investment_location_dropdown_value = '';
            }
            var investment_location_textbox_value = jQuery('input[wiztag="investment_location_textbox"]').val();
            if ( investment_location_textbox_value == 'or type a location' ) {
                investment_location_textbox_value = '';
            }
            if ( investment_location_dropdown_value != '' && investment_location_textbox_value != '' ) {
                investment_location_dropdown_value += ' and ';
            }
            var investment_location_value = investment_location_dropdown_value + investment_location_textbox_value;

            // First senctence
            autoMessage += "I currently own and operate a business in the United States" + location_location_value;
            if ( tagDependencyMet('business_is_in_usa_no') == true ) {
                var userBusinessInCountry = jQuery.trim( jQuery('.wizard-question-step-id-1.wizard-question-question-id-5 select').val() );
                if ( userBusinessInCountry == 'Select a Country' ) {
                    autoMessage += " and globally";
                } else {
                    autoMessage += " and globally in " + userBusinessInCountry;
                }
            } else {
                if ( jQuery.trim(location_location_value) == '' ) {
                    autoMessage += " and/or globally";
                }
            }
            autoMessage += ". ";
            
            // Second sentence - based on 2nd slide anwsers
            if ( tagDependencyMet('ident_location_interest_yes') == true ) {
                
                autoMessage += "I am interested in investment opportunities";
                if ( jQuery.trim(investment_location_value) !== '' ) {
                    autoMessage += " in " + investment_location_value;
                }
                
                var interestedIndustries = [];
                if ( tagDependencyMet('ignore_for_results_automotive') == true ) { interestedIndustries.push('automotive'); }
                if ( tagDependencyMet('ignore_for_results_aerospace') == true ) { interestedIndustries.push('aerospace'); }
                if ( tagDependencyMet('ignore_for_results_biotechnology') == true ) { interestedIndustries.push('biotechnology'); }
                if ( tagDependencyMet('ignore_for_results_chemical') == true ) { interestedIndustries.push('chemical'); }
                if ( tagDependencyMet('ignore_for_results_consumer_goods') == true ) { interestedIndustries.push('consumer_goods'); }
                if ( tagDependencyMet('ignore_for_results_energy') == true ) { interestedIndustries.push('energy'); }
                if ( tagDependencyMet('ignore_for_results_environmental_technology') == true ) { interestedIndustries.push('environmental_technology'); }
                if ( tagDependencyMet('ignore_for_results_financial_services') == true ) { interestedIndustries.push('financial_services'); }
                if ( tagDependencyMet('ignore_for_results_healthcare') == true ) { interestedIndustries.push('healthcare'); }
                if ( tagDependencyMet('ignore_for_results_logistics') == true ) { interestedIndustries.push('logistics'); }
                if ( tagDependencyMet('ignore_for_results_machinery') == true ) { interestedIndustries.push('machinery'); }
                if ( tagDependencyMet('ignore_for_results_media') == true ) { interestedIndustries.push('media'); }
                if ( tagDependencyMet('ignore_for_results_pharmaceuticals') == true ) { interestedIndustries.push('pharmaceuticals'); }
                if ( tagDependencyMet('ignore_for_results_professional_services') == true ) { interestedIndustries.push('professional_services'); }
                if ( tagDependencyMet('ignore_for_results_retail_trade') == true ) { interestedIndustries.push('retail_trade'); }
                if ( tagDependencyMet('ignore_for_results_semiconductors') == true ) { interestedIndustries.push('semiconductors'); }
                if ( tagDependencyMet('ignore_for_results_software') == true ) { interestedIndustries.push('software'); }
                if ( tagDependencyMet('ignore_for_results_textiles') == true ) { interestedIndustries.push('textiles'); }
                if ( tagDependencyMet('ignore_for_results_travel') == true ) { interestedIndustries.push('travel'); }

                if ( interestedIndustries.length > 0 ) {
                    autoMessage += ", specifically in the following industries: ";
                    autoMessage += interestedIndustries.join(', ') + '. ';
                } else {
                    autoMessage += ". I have not yet identified location(s) to invest in. ";
                }
            } else {
                autoMessage += ". I have not yet identified location(s) to invest in. ";
            }
            
            // Third slide 
            if ( tagDependencyMet('invest_subsid_no') == true ) {
                autoMessage += "I do not have specific questions about investing at this time. ";
            }
            
            // Type of Assistance Slide 
            if ( tagDependencyMet('fed_reg_assistance_yes') == true ) {
                typeOfAssistanceChecks = [];
                if ( tagDependenciesMet('funding') == true ) { typeOfAssistanceChecks.push('funding'); }
                if ( tagDependenciesMet('information') == true ) { typeOfAssistanceChecks.push('information'); }
                if ( tagDependenciesMet('tax_incentives') == true ) { typeOfAssistanceChecks.push('tax_incentives'); }
                if ( tagDependenciesMet('technical_assistance') == true ) { typeOfAssistanceChecks.push('technical_assistance'); }
                
                if ( typeOfAssistanceChecks.length > 2 ) {
                    var lastWord = typeOfAssistanceChecks.pop();
                    typeOfAssistanceChecks = typeOfAssistanceChecks.join(', ') + ' and ' + lastWord;
                } else {
                    typeOfAssistanceChecks = typeOfAssistanceChecks.join(', ');
                }
                
                autoMessage += "That said, I would need some assistance regarding United States federal regulations and some insight into the types of " + typeOfAssistanceChecks + " resources available for investors.";
            }
            
            return autoMessage.replace('. . ', '. ');
        }
        
        /* void wizardEvent_EmailBtnClick()
         * This function is triggered when the user clicks on div.wizard-emailbtn-link
         * This div-markup is stored in wizard-abstract.tpl.php and is always hidden unless forced-visible
         * Right now, this button is only used with the SelectUSA wizard and may need to be refactored if to be used with another wizard.
         */
        function wizardEvent_EmailBtnClick() {
            
            // Bug killer - dont continue this function if this function was just recently called less than 100 miliseconds ago
            if ( typeof ignore_trigger_wizardEvent_EmailBtnClick != 'undefined' && ignore_trigger_wizardEvent_EmailBtnClick == true ) {
                return;
            } else {
                ignore_trigger_wizardEvent_EmailBtnClick = true;
                setTimeout( function () {
                    ignore_trigger_wizardEvent_EmailBtnClick = false;
                }, 100);
            }
            
            // We will validate the "Contact Us" form in the "Select USA" wizard below... Now we shall determin the error message to display to the user when the form is NOT valid
            if ( jQuery('body.responsive-layout-mobile').length > 0 ) {
                var formNotValidMessage = 'The following fields are required to successfully submit the form: Name, Subject, and Message. Please fill in all required fields.';
            } else {
                var formNotValidMessage = 'The following fields are required to successfully submit the form: Name, Company Name, E-Mail Address, Subject, and Message. Please fill in all required fields.';
            }
            
            // Email form Validation - name
            var emailAddressInput = jQuery('.wizard-tag-contact_name input');
            if ( emailAddressInput.val() == '' ) {
                alert(formNotValidMessage);
                emailAddressInput.focus();
                return false;
            }
            
            // Email form Validation - company name (do not validate on mobile)
            var emailAddressInput = jQuery('.wizard-tag-company_name input');
            if ( emailAddressInput.val() == '' && jQuery('body.responsive-layout-mobile').length == 0 ) {
                alert(formNotValidMessage);
                emailAddressInput.focus();
                return false;
            }
            
            // Email form Validation - email address (do not validate on mobile)
            var emailAddressInput = jQuery('.wizard-tag-contact_email input');
            if ( emailAddressInput.val() == '' && jQuery('body.responsive-layout-mobile').length == 0 ) {
                alert(formNotValidMessage);
                emailAddressInput.focus();
                return false;
            }
            
            // Email form Validation - email subject
            var emailSubjectInput = jQuery('.wizard-tag-contact_sub input');
            if ( emailSubjectInput.val() == '') {
                alert(formNotValidMessage);
                emailSubjectInput.focus();
                return false;
            }
            
            // Email form Validation - email messsage
            var emailMessageInput = jQuery('.wizard-tag-contact_msg textarea');
            if ( emailMessageInput.val() == '' ) {
                alert(formNotValidMessage);
                emailMessageInput.focus();
                return false;
            }
            
            // Check an invisible checkbox statting that we have filled out this form - this is used to allow the next section to show
            jQuery('.wizard-tag-email_filled input').attr('checked', 'checked');
            checkboxClickEventHandelerForTagging(true);
            enforceVisibilityBasedOnDependencies();
            
            // Collect information from the email-form elements
            var emailFormElements = {}
            jQuery('.questions-for-step-5-container').find('input, select').each( function () {
                var jqThis = jQuery(this);
                emailFormElements[jqThis.attr('id')] = jqThis.val();
            });
        
            // Create spinner message to show while the AJAX session is being processed
            var initialHTML = '<div class="selectusa-email-dispatch-container" style="padding: 30px;"><img src="/sites/all/themes/bususa/images/ajax-loader.gif" /> Please wait while we process this request...</div>';
            jQuery.colorbox({ html: initialHTML });
            var postData = buildWizardPostData();
            postData['emailFormElements'] = emailFormElements;
            lastEmailDispatchPostData = postData;
           
            // Post to /sys/selectusa-dispatch-email and show the responce HTML
            jQuery.post('/sys/selectusa-dispatch-email', postData, function (data) {
                jQuery('.selectusa-email-dispatch-container').fadeOut( function () {
                    jQuery('.selectusa-email-dispatch-container').html(data);
                    jQuery('.selectusa-email-dispatch-container').show();
                    jQuery.colorbox.close();
                });
                
            }).fail( function () {
                alert('An AJAX error has occured while executing this request!');
            } ).always( function () {
                // Simulate click of the Next button to continue in the wizard
                jQuery('.wizard-next-link input').click();
                // If we are in mobile layout force-expand the current select section
                if ( jQuery('.questions-for-step-' + wizard_current_step + '-container:visible').length == 0 ) { // to test if this section is visible or not, try jQuery-selecting the question's contain within this section with :visible
                    jQuery('.sections-container-top .section.section-number-' + wizard_current_step).click();
                }
            });
        }
        
        /* void selectUSA_custom_damon()
         * A maintenance function to be run every couple hundred miliseconds - performs custom changes to the SelectUSA Wizard
         */
        function selectUSA_custom_damon() {
            
            // Set the Next button to say "Send" when on the appropriate slide
            if ( jQuery('.wizard-container.wizard-state-slide-5').length > 0 ) {
                jQuery('.next-container input').val('Send');
                wizard_contactus_slide_is_selected = true;
            } else {
                jQuery('.next-container input').val('Next');
                wizard_contactus_slide_is_selected = false;
            }
            
            // If this is the first time this function has run since the 'Contact Us' slide has been selected...
            if ( typeof wizard_contactus_slide_was_selected != 'undefined' && wizard_contactus_slide_was_selected == false && wizard_contactus_slide_is_selected == true ) {
                // ...then auto-populate the email message field
                var msg = wizardSelectUSA_generateEmailMessage(); // This function is defined in global.js
                jQuery('textarea[wiztag="contact_msg"]').val(msg);
            }
            
            wizard_contactus_slide_was_selected = wizard_contactus_slide_is_selected
        }
    
    </script>
    
