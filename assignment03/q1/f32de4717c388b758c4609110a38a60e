        <script>
        
            // Event handelers for click events on Parature fly-in icons
            jQuery('#parature-ticket a').bind('click', function () {
                setTimeout( function () { /* Sigh, dont know why this setTimeout is nessesary, but there seems to be a bug in Chrome 29.0.1 where this is needed. */
                    var targetLink = 'http://help.business.usa.gov/ics/support/default.asp?deptID=30030&task=ticket'; 
                    if ( lastGoogleTranslatedLanguage != 'Select Language' ) {
                        targetLink += '&setLanguage=' + lastGoogleTranslatedLanguage;
                    }
                    document.location = targetLink;
                }, 100);
            });
            jQuery('#parature-feedback a').bind('click', function () {
                setTimeout( function () { /* Sigh, dont know why this setTimeout is nessesary, but there seems to be a bug in Chrome 29.0.1 where this is needed. */
                    var targetLink = 'http://help.businessusa.gov/ics/support/ticketnewwizard.asp?style=classic&feedback=true'; 
                    if ( lastGoogleTranslatedLanguage != 'Select Language' ) {
                        targetLink += '&setLanguage=' + lastGoogleTranslatedLanguage;
                    }
                    document.location = targetLink;
                }, 100);
            });
            jQuery('#parature-ss a').bind('click', function () {
                setTimeout( function () { /* Sigh, dont know why this setTimeout is nessesary, but there seems to be a bug in Chrome 29.0.1 where this is needed. */
                    var targetLink = 'http://help.business.usa.gov/ics/support/kbsplash.asp?deptID=30030&task=knowledge'; 
                    if ( lastGoogleTranslatedLanguage != 'Select Language' ) {
                        targetLink += '&setLanguage=' + lastGoogleTranslatedLanguage;
                    }
                    document.location = targetLink;
                }, 100);
            });
        
           //chat window resize
            function launchChatWindow(url) {
                var left=window.screen.width-430;
                var windowStyle='top=100,left='+left+',width=450,height=650,menubar=no,location=no,resizable=yes,toolbar=no,scrollbars=yes,status=no,';
                window.open(url,'',windowStyle);
            }
                
            //check for agent availability
            var time = new Date().getMilliseconds();

            function loadScript(url) {
                var script = document.createElement('script');
                script.setAttribute('src', url);
                document.getElementsByTagName('head')[0].appendChild(script);
            }

            function updateImage(image, id) {
               
                if( $(image).hasClass('available') ) {
                    
                    if ( scroll_chat == 215 ) {
                        $("#parature_chat_min").attr('src', '/sites/all/themes/bususa/images/parature_chat_hover.png');
                        jQuery("#parature-chat").css('background', 'rgb(21, 86, 123)');
                    } else {
                        $("#parature_chat_min").attr('src', '/sites/all/themes/bususa/images/parature_chat.png');
                        jQuery("#parature-chat").css('background', '#f3f3f3');
                    }
                    jQuery("#parature-chat .icon-text").text("Chat with a Specialist");
                    jQuery("#parature-chat span").css('color', '#fff');
                    jQuery('#e0221f2a-9807-4b4b-9fac-9937209ee8b7').attr('title', 'Chat with a Specialist');
                    
                } else {
                    $("#parature_chat_min").attr('src', '/sites/all/themes/bususa/images/parature_chat_offline.png');
                    jQuery("#parature-chat").css('background', '#f3f3f3');	
                    jQuery("#parature-chat span").css('color', '#4d4d4d');
                    jQuery("#parature-chat .icon-text").text("Chat Unavailable");
                    jQuery("#e0221f2a-9807-4b4b-9fac-9937209ee8b7").attr('title', 'Chat Unavailable');
                    jQuery('#parature-chat').hide();
                }
            }

            //Time ensures that we don't cache the script
            loadScript(document.location.protocol + '//help.business.usa.gov/ics/csrchat/ChatButtonHttpModule.aspx?buttonId=e0221f2a-9807-4b4b-9fac-9937209ee8b7&clientId=30027&deptId=30030&time=' + time);
                
            jQuery(document).ready(function() {
            
             $('.parature-div a').click(function () {      
                window.location = $(this).attr('href');
            });
            
            //parature icons
                 parature_chat = $("#parature-chat");
                 parature_ticket = $("#parature-ticket");
                 parature_ss = $("#parature-ss");
                 parature_feedback = $("#parature-feedback");
                 parature_fed = $("#parature-fed");
                
                // test
                
                function scrollParatureIcon(scrollOut) {
                    
                    jQuery("#parature_ticket_min").attr('src', '/sites/all/themes/bususa/images/parature_ticket_busa_blue.png');            
                    jQuery("#parature_ss_min").attr('src', '/sites/all/themes/bususa/images/parature_ss_busa_blue.png');
                    jQuery("#parature_feedback_min").attr('src', '/sites/all/themes/bususa/images/parature_feedback_busa_blue.png');
                    jQuery("#parature_fed_min").attr('src', '/sites/all/themes/bususa/images/parature_fedinfo_busa_blue.png');
                    
                    scroll_chat = 50;
                    var scroll_ticket = 50;
                    var scroll_ss = 50;
                    var scroll_feedback = 50;
                    var scroll_fed = 50;
                    
                    switch (scrollOut) {
                        case 'parature_chat':
                                  scroll_chat = 215;						                            
                                  jQuery("#parature_chat_min").addClass('parature-img-style');			              
                                  jQuery("#parature-chat .icon-text").show();
                                  loadScript(document.location.protocol + '//help.business.usa.gov/ics/csrchat/ChatButtonHttpModule.aspx?buttonId=e0221f2a-9807-4b4b-9fac-9937209ee8b7&clientId=30027&deptId=30030&time=' + time);			
                                  break;
                        case 'parature_ticket':
                                  scroll_ticket = 215;
                                  jQuery("#parature_ticket_min").attr('src', '/sites/all/themes/bususa/images/parature_ticket_busa_blue_hover.png');
                                  jQuery("#parature_ticket_min").addClass('parature-img-style');
                                  jQuery("#parature-ticket").css('background', 'rgb(21, 86, 123)');
                                  jQuery("#parature-ticket .icon-text").show();						
                                  break;
                        case 'parature_ss':
                                  scroll_ss = 215;
                                  jQuery("#parature_ss_min").attr('src', '/sites/all/themes/bususa/images/parature_ss_busa_blue_hover.png');
                                  jQuery("#parature_ss_min").addClass('parature-img-style');
                                  jQuery("#parature-ss").css('background', 'rgb(21, 86, 123)');
                                  jQuery("#parature-ss .icon-text").show();						  
                                  break;
                        case 'parature_feedback':
                                  scroll_feedback = 215;
                                  jQuery("#parature_feedback_min").attr('src', '/sites/all/themes/bususa/images/parature_feedback_busa_blue_hover.png');
                                  jQuery("#parature_feedback_min").addClass('parature-img-style');
                                  jQuery("#parature-feedback").css('background', 'rgb(21, 86, 123)');
                                  jQuery("#parature-feedback .icon-text").show();						  
                                  break;
                        case 'parature_fed':
                                  scroll_fed = 215;
                                  jQuery("#parature_fed_min").attr('src', '/sites/all/themes/bususa/images/parature_fedinfo_busa_blue_hover.png');
                                  jQuery("#parature_fed_min").addClass('parature-img-style');
                                  jQuery("#parature-fed").css('background', 'rgb(21, 86, 123)');
                                  jQuery("#parature-fed .icon-text").show();						  
                                  break;
                            default:
                                  jQuery(".parature-div").css('background', '#f3f3f3');			
                                  jQuery("#icon-text").hide();	
                                  loadScript(document.location.protocol + '//help.business.usa.gov/ics/csrchat/ChatButtonHttpModule.aspx?buttonId=e0221f2a-9807-4b4b-9fac-9937209ee8b7&clientId=30027&deptId=30030&time=' + time);
                                  break;
                    }			

                    parature_chat.stop();
                    parature_ticket.stop();
                    parature_ss.stop();
                    parature_feedback.stop();
                    parature_fed.stop();
                    
                    parature_chat.animate({
                            width: scroll_chat
                    },400,function(){ });                
                    
                   parature_ticket.animate({
                             width: scroll_ticket
                    },400,function(){ });

                     parature_ss.animate({
                          width: scroll_ss
                     },400,function(){ });

                     parature_feedback.animate({
                          width: scroll_feedback
                     },400,function(){ });
                     
                     parature_fed.animate({
                          width: scroll_fed
                     },400,function(){ });
                }
                
                parature_ticket.mouseenter(function () {
                    scrollParatureIcon('parature_ticket');
                });

                parature_ticket.mouseleave(function () {
                    scrollParatureIcon('');
                });

                parature_chat.mouseenter(function () {
                    scrollParatureIcon('parature_chat');			
                });

                parature_chat.mouseleave(function () {
                    scrollParatureIcon('');
                });
                
                parature_ss.mouseenter(function () {
                    scrollParatureIcon('parature_ss');
                });

                parature_ss.mouseleave(function () {
                    scrollParatureIcon('');
                });
                
                parature_feedback.mouseenter(function () {
                    scrollParatureIcon('parature_feedback');
                });

                parature_feedback.mouseleave(function () {
                    scrollParatureIcon('');
                });
                
                parature_fed.mouseenter(function () {
                    scrollParatureIcon('parature_fed');
                });

                parature_fed.mouseleave(function () {
                    scrollParatureIcon('');
                });
                
                $('a').click(function () {      
                    window.location = $(this).attr('href');
                });
                
                var badBox = jQuery('#block-boxes-parature-ticket--2');
                badBox.hide();
                badBox.attr('note', 'Hidden by jQuery from parature-ticket-box');
            });
        </script>
        
<!-- END: parature_ticket.php  --></div></div>    </div>
  </div>
</div>  </div>
</aside>  </div>
</div></section>    
  
