<script type="text/javascript">
<!--//--><![CDATA[//><!--

            /* The following JavaScript is stored in sites/all/modules/custom/hooks_reaction/reactions/EventsView-RemoveCommas.php */
            
            /* 
                I am having a difficult time understand where the commas are comming from for this View 
                We need to strip away double commas, and commas after new lines
                This jQuery will apply this action
                */
            // for each div in the view that has the Events address
            jQuery(document).ready( function() {
                jQuery('div.view-id-event_calendar.view-display-id-events_listing .event-location').each( function () {
                    var jqThis = jQuery(this);
                    jqThis.attr('note', 'contents altered by jQuery, see EventsView-RemoveCommas.php for the JQ-code');
                    var html = jqThis.html();
                    html = String(html);
                    html = jQuery.trim(html);
                    
                    var trimThings = true;
                    while ( trimThings == true ) {
                    
                        trimThings = false;
                        
                        if ( html.indexOf(', ,') != -1 ) {
                            html = html.replace(', ,', ', ');
                            html = jQuery.trim(html);
                            trimThings = true;
                        }
                        
                        if ( html.indexOf('<br>,') != -1 || html.indexOf('<BR>,') != -1 ) {
                            html = html.replace('<br>,', ',<br>');
                            html = html.replace('<BR>,', ',<BR>');
                            html = jQuery.trim(html);
                            trimThings = true;
                        }
                        
                        if ( html.substring( html.length - 1 ) == ',' ) {
                            html = html.substring(0, html.length - 1 );
                            html = jQuery.trim(html);
                            trimThings = true;
                        }
                        
                        if ( html.substring( html.length - 4 ) == '<br>' || html.substring( html.length - 4 ) == '<BR>' ) {
                            html = html.substring(0, html.length - 4 );
                            html = jQuery.trim(html);
                            trimThings = true;
                        }
                    }
                    //alert(html);
                    jqThis.html(html);
                });
            });
        
//--><!]]>
</script>
