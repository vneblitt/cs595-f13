<script type="text/javascript">
<!--//--><![CDATA[//><!--

            /* The following code is injected from SelectBoxItSupport.php */
            
            // The following function is used to apply jQuery-SelectBoxIt styling into html-select boxes, and trigger any other events that needs to be triggered after this is done.
            function triggerSelectBoxItLib() {
                // Trigger SelectBoxIt
                jQuery( function() {
                    $("select").selectBoxIt({
                      nostyle: true
                    });
                });
                
                // Run post-execution-scripts (if there are any)
                if ( typeof selectBoxItPostRun != "undefined" ) {
                    for ( var x = 0 ; x < selectBoxItPostRun.length ; x++ ) {
                        selectBoxItPostRun[x].call();
                    }
                }
            }
            
            // Load jQuery 1.8.2 (requiered by SelectBoxIt lib), and then the library - we need to do this after the body loads because our current Drupal application uses and needs a lower jQuery version
            jQuery(document).ready( function () {
                setTimeout( function () {
                    
                    // Dont bypass Akamai cache for the following $.getScript() commands
                    jQuery.ajaxSetup({ cache: true });
                    
                    jQuery.getScript("/sites/all/libraries/jquery/jquery-1.8.2.js", function(data, textStatus, jqxhr) {
                    
                        // Dont bypass Akamai cache for the following $.getScript() commands
                        jQuery.ajaxSetup({ cache: true });
                    
                        jQuery.getScript("/sites/all/libraries/jquery/jquery-ui-1-9-0.min.js", function(data, textStatus, jqxhr) {
                                                        
                            // Also load the some other jQuery plugins we want to retain
                            jQuery.getScript("/sites/all/libraries/colorbox/colorbox/jquery.colorbox-min.js?mtxtc1");
                        
                            // Load SelectBoxIt library
                            jQuery.getScript("/sites/all/themes/bususa/js/jquery.selectBoxIt.js", function(data, textStatus, jqxhr) {
                                triggerSelectBoxItLib();
                            });
                            
                            // Restore default operation for $.getScript() commands
                            jQuery.ajaxSetup({ cache: false });
                        });
                    });
                }, 250);
            });
            
            /* The previous code is injected from SelectBoxItSupport.php */
        
//--><!]]>
</script>
