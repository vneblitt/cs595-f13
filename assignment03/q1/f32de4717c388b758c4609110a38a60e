    <!-- JavaScripts -->
    <script>

        consoleLog('Wizard results-area JavaScript now initializing. /* Coder Bookmark: CB-GTHCXIW-BC */ ');
        wizard_current_step = 1;
        wizard_step_count = 4;
        wizard_result_template = 'divide-by-ctype';
        wizard_selected_tags = {};
        wizard_uses_side_bar = "healthcare_sidebar";
        wizard_donotusecache = false;
        wizard_resetFutureElementsOnAnwser = true;
        wizard_all_wizard_tags = ['National', 'Alabama', 'Alaska', 'American_Samoa', 'Arizona', 'Arkansas', 'California', 'Colorado', 'Connecticut', 'Delaware', 'District_of_Columbia', 'Florida', 'Georgia', 'Guam', 'Hawaii', 'Idaho', 'Illinois', 'Indiana', 'Iowa', 'Kansas', 'Kentucky', 'Louisiana', 'Maine', 'Maryland', 'Massachusetts', 'Michigan', 'Minnesota', 'Mississippi', 'Missouri', 'Montana', 'Nebraska', 'Nevada', 'New_Hampshire', 'New_Jersey', 'New_Mexico', 'New_York', 'North_Carolina', 'North_Dakota', 'Northern_Marianas_Islands', 'Ohio', 'Oklahoma', 'Oregon', 'Pennsylvania', 'Puerto_Rico', 'Rhode_Island', 'South_Carolina', 'South_Dakota', 'Tennessee', 'Texas', 'Utah', 'Vermont', 'Virginia', 'Virgin_Islands', 'Washington', 'West_Virginia', 'Wisconsin', 'Wyoming', 'Self_Employed', 'Employee_less_25', 'Employee_less_50', 'Employee_more_50', 'Currently_Yes', 'Current_Planning', 'NotCurrent_NotPlanned', 'TwoCurrently_Yes', 'TwoNotCurrent_Planning', 'TwoNotCurrent_NotPlanned'];
        disableNextButtonSelectors = [''];
        
        /* Event handelers */
        jQuery('.wizard-welcomemessage-startbtn-inner').bind('click', function () {
            enforceVisibilityBasedOnDependencies();
            jQuery('.wizard-welcomemessage').fadeOut( 500, function () { 
                jQuery('.wizard-contents').fadeIn(500, function () {
                    checkboxClickEventHandelerForTagging(true);
                    enforceVisibilityBasedOnDependencies();
                }); 
            });
        });
        
        jQuery('.wizard-next-link').bind('click', function () { 
            // Set the step target to the next step
            if ( futureSlideExists() == false ) {
                alert('There is no next slide in this wizard, please select an option, or a different option.');
            } else {
                wizard_current_step++;
                // Check if this step is hidden, if so, move to the next
                while ( jQuery('.sections-container .section.section-number-' + wizard_current_step).eq(0).hasClass('hidden-due-to-dependency') ) {
                    wizard_current_step++;
                }
                enforceWizardClasses(false);
                document.location.href = '#wizard-step-id-' + wizard_current_step;
            }
        });
        
        jQuery('.wizard-back-link').bind('click', function () {
            // Set the step target to the previous step
            wizard_current_step--;
            // Check if this step is hidden, if so, move to the previous
            while ( jQuery('.sections-container .section.section-number-' + wizard_current_step).eq(0).hasClass('hidden-due-to-dependency') ) {
                wizard_current_step--;
            }
            enforceWizardClasses(false);
            document.location.href = '#wizard-step-id-' + wizard_current_step;
        });
        
        jQuery(document).ready( function () {
            enforceWizardClasses(false);
        });
        
        /* Event Handeler + Bug Killer; In Chrome it seems that when users tab to the selectbox that foward-tabbing gets stuck there */
        jQuery(document).ready( function () {
            jQuery('body.chrome .wizard-container select').bind('keydown', function (e) {
                if ( e.keyCode == 9 && e.shiftKey == false ) { // If the TAB key was pressed and SHIFT was not...
                    var jqThis = jQuery(this);
                    var allSelectables = jQuery('input:visible, select:visible');
                    // Find where jqThis is in allSelectables
                    for ( var x = 0 ; x < allSelectables.length ; x++ ) {
                        if ( jQuery(allSelectables[x]).attr('id') == jqThis.attr('id') ) {
                            // Ok, so allSelectables[x] is where this/jqThis is in the allSelectables array
                            // Set the focus on the next element
                            setFocusTo = allSelectables[x+1];
                            setTimeout( function () {
                                setFocusTo.focus();
                            }, 150);
                        }
                    }
                }
            });
        });
        
        /* Enfore resetFutureElementsOnAnwser */
        jQuery('.wizard-container .wizard-question input').bind('change', function () {
            var jqThis = jQuery(this);
            lastWizardQuestionAnwsered = jqThis;
            setTimeout( function () {
                doResetFutureElementsOnAnwser();
            }, 50);
        });
        function doResetFutureElementsOnAnwser() {
            
            if ( wizard_resetFutureElementsOnAnwser == false ) { return; }
            
            var lastWizardAnwseredSlide = lastWizardQuestionAnwsered.parents('.questions-for-step-container').attr('questionsforstep');
            for ( var x = parseInt(lastWizardAnwseredSlide) + 1 ; x < wizard_step_count ; x++ ) {
                jQuery('.questions-for-step-' + x + '-container input').each( function () {
                    var jqThis = jQuery(this);
                    jqThis.get(0).checked = false;
                    jqThis.parents('label').removeClass('r_on');
                });
            }
        }
        
        /* Detect when the hash in the address bar changes, and update the wizard as needed */
        setInterval( function () {
            if ( typeof wizardLastHash != 'undefined' && typeof document.location.hash != 'undefined' && String(document.location.hash).indexOf('wizard-step-id') != -1 && jQuery.trim(document.location.hash) != '' ) {
                if ( wizardLastHash != document.location.hash || String(document.location.hash).indexOf('force-') != -1 ) {
                    // At this point we know the hash in the address bar has changed since the last time we checked
                    // We will wait 100 miliseconds to ensure any other javascript thread running has done its business in terms of changing the wizard sections
                    setTimeout( function () {
                        // Check if the new hash number in the address bar does, or does not represent the current active slide in the wizard
                        var slideIdInAddressBar = document.location.hash.split('-').pop();
                        if ( parseInt(slideIdInAddressBar) != parseInt(wizard_current_step) ) {
                            // There is a difference between the slide-id in the address bar and the current slide being show, so we know it is time to force-seek the wizard
                            consoleLog('force-seeking the wizard to slide ' + slideIdInAddressBar + ' based on hash in address bar. The hash seen is ' + document.location.hash + ' and the slideIdInAddressBar var is ' + slideIdInAddressBar + ' /* Coder Bookmark: CB-PD6X4PY-BC */ ');
                            if ( jQuery('.wizard-welcomemessage:visible').length > 0 ) {
                                consoleLog('It appears the wizard-splash-screen is visible, force-hiding .wizard-welcomemessage and showing .wizard-contents /* Coder Bookmark: CB-R1GAIMR-BC */ ');
                                jQuery('.wizard-welcomemessage').hide();
                                jQuery('.wizard-contents').show();
                            }
                            wizard_current_step = slideIdInAddressBar;
                            enforceWizardClasses(false);
                            document.location.hash = String(document.location.hash).replace('force-', ''); // If this was a one-time force directive, remove the force-tag.
                        }
                    }, 100);
                }
            }
            wizardLastHash = document.location.hash;
        }, 300);
        
        function postWizardTagChangeEvent() {
            updateWizardContainerClassesForTags();
            showHideNextButtonPerWizadRules();
        }
        
        function updateWizardContainerClassesForTags() {
            
            var wizardContainer = jQuery('.wizard-container');
            
            // Remove all tags that this wizard uses from the WizardContainer (we will add them back later)
            for (var thisTag in wizard_all_wizard_tags) {
                wizardContainer.removeClass(wizard_all_wizard_tags[thisTag]);
            }
            
            // Add each selected tag onto the WizardContainer
            for (var thisTag in wizard_selected_tags) {
                wizardContainer.addClass(wizard_selected_tags[thisTag]);
            }
        }
        
        /** string/true showHideNextButtonPerWizadRules()
          * Shows/Hides the Next button on the wizard based on the javascript-global disableNextButtonSelectors variable, which 
          * is built based on information in the Wizard Excel spreadsheet (disable-next-button property)
          * Returns the defined disable-next-button rule (as a string) which states the Next button should be hidden, or TRUE when 
          * the Next button is allowed to be visible
          */
        function showHideNextButtonPerWizadRules() {
        
            // Assume this should be shown, hide later if a rule is found
            jQuery('.next-container').show();
            
            for ( var x = 0 ; x < disableNextButtonSelectors.length ; x++ ) {
                if ( jQuery(disableNextButtonSelectors[x]).length > 0 ) {
                    jQuery('.next-container').hide();
                    return disableNextButtonSelectors[x];
                }
            }
            
            return true;
        }
        
        /** boolean futureSlideExists()
          * Returns true/false based on if there is a future step/slide currently visible on the wizard
          */
        function futureSlideExists() {
            var w = wizard_current_step;
            w++;
            while ( jQuery('.sections-container .section.section-number-' + w).eq(0).hasClass('hidden-due-to-dependency') ) {
                w++;
            }
            
            if ( jQuery('.sections-container .section.section-number-' + w).length > 0 ) {
                return true;
            } else {
                return false;
            }
        }
        
        // Show/Hide questions which depend on other questions to be selected before becomming visible
        function checkboxClickEventHandelerForTagging(triggerForAllCheckboxes) {
            
            if ( typeof triggerForAllCheckboxes == 'undefined' ) {
                myThis = jQuery(this);
            } else {
                myThis = jQuery('.wizard-container .questions-for-step-container input[type=checkbox]');
            }
            
            for ( var x = 0 ; x < myThis.length ; x++ ) {
                var jqThis = jQuery(myThis[x]);
                var thisTag = jqThis.attr('wiztag');
                
                // First ensure this tag does not already exist as selected
                if ( typeof wizard_selected_tags[thisTag] != 'undefined' ) {
                    delete wizard_selected_tags[thisTag];
                    consoleLog('Removing tag ' + thisTag + ' from wizard_selected_tags. /* Coder Bookmark: CB-MMKTTT4-BC */ ');
                }
                
                // Now add this tag to wizard_selected_tags if it should be in there
                if ( jqThis.attr('checked') == 'checked' ) {
                    wizard_selected_tags[thisTag] = thisTag;
                    consoleLog('Adding tag ' + thisTag + ' to wizard_selected_tags. /* Coder Bookmark: CB-EZOQOPG-BC */ ');
                }
            }
            postWizardTagChangeEvent(); // All wizard_selected_tags should be added as a class on jQuery('.wizard-container')
        }
        jQuery('.questions-for-step-container input[type=checkbox]').bind('click', function () { 
            setTimeout( function () {
                consoleLog('Triggering checkboxClickEventHandelerForTagging(true) /* Coder Bookmark: CB-R5XQ7RX-BC */');
                checkboxClickEventHandelerForTagging(true);
            }, 50);
        });
        
        // Add/Remove this tag to wizard_selected_tags (RADIO BUTTONS ONLY)
        jQuery('.questions-for-step-container input[type=radio]').bind('click', function () {
            setTimeout( function () {
                jQuery('.questions-for-step-container input[type=radio]').each( function () {
                    var jqThis = jQuery(this);
                    var thisTag = jqThis.attr('wizTag');
                    delete wizard_selected_tags[thisTag];
                    if ( jqThis.parent().hasClass('r_on') ) {
                        wizard_selected_tags[thisTag] = thisTag;
                        consoleLog('Adding tag ' + thisTag + ' to wizard_selected_tags. /* Coder Bookmark: CB-0P8LMHO-BC */ ');
                    }
                    postWizardTagChangeEvent(); // All wizard_selected_tags should be added as a class on jQuery('.wizard-container')
                });
            }, 100);
        });
        
        // Add/Remove this tag to wizard_selected_tags (SELECT BOXES ONLY)
        jQuery('.wizard-contents .wizard-question-widget-type-drop-down').bind('click', function () {
            setTimeout( function () {
                consoleLog('Triggering wizardSelectBoxTagUpdate() /* Coder Bookmark: CB-7GR1OVY-BC */ ');
                wizardSelectBoxTagUpdate();
            }, 100);
        });
        
            
        function wizardSelectBoxTagUpdate() {
            jQuery('.wizard-contents .wizard-question-widget-type-drop-down').each( function () {
                
                var jqThis = jQuery(this);
                // Untag all vlues for this select-box
                jqThis.find('option').each( function () {
                    thisTag = jQuery(this).attr('wiztag');
                    thisTag = jQuery.trim(thisTag);
                    delete wizard_selected_tags[thisTag];
                });
                
                // Add the current selected value
                var selectText = jqThis.find('select').val();
                thisTag = jqThis.find('select option:contains("' + selectText + '")').attr('wizTag');
                thisTag = jQuery.trim(thisTag);
                wizard_selected_tags[thisTag] = thisTag;
            });
            postWizardTagChangeEvent(); // All wizard_selected_tags should be added as a class on jQuery('.wizard-container')
        }
        
        if ( typeof selectBoxItPostRun == "undefined" ) { selectBoxItPostRun = []; }
        selectBoxItPostRun.push( function () {
            consoleLog('Triggering wizardSelectBoxTagUpdate() /* Coder Bookmark: CB-J73Z0SZ-BC */ ');
            wizardSelectBoxTagUpdate();
        });
    
        
        jQuery(document).ready( function () {
            jQuery('.wizard-contents').bind('click', function () {
                setTimeout( function () {
                    enforceVisibilityBasedOnDependencies();
                }, 25);
            });
        });
        
        /** void enforceVisibilityBasedOnDependencies()
          * Hides and show questions, and slides/steps based on wizard2-tag-dependancies
          */
        function enforceVisibilityBasedOnDependencies() {
            
            // Show or hide questions based on dependencies
            enforceVisibilityBasedOnDependencies_questions();
            
            // Show or hide steps/slides based on dependencies
            if ( jQuery('.in-fading-process').length == 0 ) {
                enforceVisibilityBasedOnDependencies_slides_fadeout( function () {
                    enforceVisibilityBasedOnDependencies_slides_fadein();
                });
            }
        }

        function enforceVisibilityBasedOnDependencies_questions() {
            // Show or hide questions based on dependencies
            jQuery('.wizard-question-has-dependency-true').each( function () {
            
                var jqThis = jQuery(this);
                var dependencyids = jqThis.attr('dependencyid');
                
                if ( tagDependenciesMet(dependencyids) ) {
                    jqThis.show();
                    jqThis.removeClass('hidden-due-to-dependency');
                } else {
                    jqThis.hide();
                    jqThis.addClass('hidden-due-to-dependency');
                }
            });
        }

        /** void enforceVisibilityBasedOnDependencies_slides_fadeout(callback)
          * Hides slides/steps that do not meet wizard2-tag-dependancies
          * @param callback - A call back function to trigger when the fadeOut of the elements on page page has completed
          */
        function enforceVisibilityBasedOnDependencies_slides_fadeout(callbackFunct) {
            
            isFadingOut = false;
            if ( typeof wizardFadeDelay == 'undefined' ) { wizardFadeDelay = 400; }
            
            jQuery('.sections-container .section.section-hasdependency-true').each( function () {
                var jqThisSection = jQuery(this);
                
                // Determin if this step should be visible or not
                shouldBeVisible = tagDependenciesMet(jqThisSection.attr('dependencyid'));
                
                // Enfore the fade-in or fade-out
                if ( shouldBeVisible == false && jqThisSection.hasClass('hidden-due-to-dependency') == false ) {
                    isFadingOut = true;
                    jqThisSection.addClass('hidden-due-to-dependency');
                    jqThisSection.addClass('in-fading-process');
                    jqThisSection.fadeOut(wizardFadeDelay, function () {
                        jQuery(this).removeClass('in-fading-process');
                        updateSlideCountClass();
                    });
                }
            });
            
            // Trigger callback if/when nessesary
            if ( typeof callbackFunct == 'function' ) {
                if ( isFadingOut == true ) {
                    // We are in the process of fading which should complete in wizardFadeDelay milliseconds
                    setTimeout(function () {
                        //alert('call');
                        callbackFunct.call();
                    }, wizardFadeDelay);
                } else {
                    // We are not fading, so trigger the callback immediately
                    callbackFunct.call();
                }
            }
        }

        /** void enforceVisibilityBasedOnDependencies_slides_fadein(callback)
          * Shows slides/steps that have meet wizard2-tag-dependancies
          * @param callback - A call back function to trigger when the fadeOut of the elements on page page has completed
          */
        function enforceVisibilityBasedOnDependencies_slides_fadein(callbackFunct) {
            //alert('enforceVisibilityBasedOnDependencies_slides_fadein');
            isFadingIn = false;
            if ( typeof wizardFadeDelay == 'undefined' ) { wizardFadeDelay = 400; }
            if ( typeof wizardHilightDelay == 'undefined' ) { wizardHilightDelay = 300; }
            
            jQuery('.sections-container .section.section-hasdependency-true').each( function () {
                var jqThisSection = jQuery(this);
                
                // Determin if this step should be visible or not
                shouldBeVisible = tagDependenciesMet(jqThisSection.attr('dependencyid'));
                
                // Enforce the fade-in or fade-out
                if ( shouldBeVisible == true && jqThisSection.hasClass('hidden-due-to-dependency') == true ) {
                    isFadingIn = true;
                    jqThisSection.removeClass('hidden-due-to-dependency');
                    updateSlideCountClass();
                    jqThisSection.addClass('in-fading-process');
                    jqThisSection.fadeIn(wizardFadeDelay, function () {
                        jQuery(this).animate({ backgroundColor:'#9A9A9A'}, wizardHilightDelay, function () {
                            jQuery(this).animate({ backgroundColor:'transparent'}, wizardHilightDelay, function () {
                                jQuery(this).removeClass('in-fading-process');
                            });
                        });
                        updateSlideCountClass();
                    });
                }
            });
            
            // Trigger callback if/when nessesary
            if ( typeof callbackFunct == 'function' ) {
                if ( isFadingIn == true ) {
                    // We are in the process of fading which should complete in wizardFadeDelay milliseconds
                    setTimeout(function () {
                        callbackFunct.call();
                    }, wizardFadeDelay + wizardHilightDelay + wizardHilightDelay);
                } else {
                    // We are not fading, so trigger the callback immediately
                    callbackFunct.call();
                }
            }
        }

        /** bool tagDependenciesMet(dependencyTags)
          * Determins if all of the given tags have been selected in the BUSA-Wizard2
          * @param dependencyTags - A string of a wizard2 tag, a string of multiple wizard2-tags, or an array of wizard2 tags
          * @param operation - A string of either "and" or "or". If left undefined, then will be autodetected (which can only be done when dependencyTags is a string)
          */
        function tagDependenciesMet(dependencyids, operation) {
            
            // If we are given a string, and it contains a double-pipe (||) then assume this is a delimiter for calling this function multiple times
            if ( typeof dependencyids == 'string' ) {
                if ( String(dependencyids).indexOf('||') > 0 ) {
                    var multiDependencyids = String(dependencyids).split('||');
                    for ( var x = 0 ; x < multiDependencyids.length ; x++ ) {
                        if ( tagDependenciesMet(multiDependencyids[x]) == true ) {
                            return true;
                        }
                    }
                    return false;
                }
            }
            
            // Determin operator
            if ( typeof operation == 'undefined' ) {
                operation = 'and'; // Default operator
                if ( typeof dependencyids == 'string' ) {
                    if ( dependencyids.indexOf('+') > 0 ) {
                        operation = 'and';
                    } else { 
                        operation = 'or';
                    }
                }
            }
        
            var givenType = String(typeof dependencyids).toLowerCase();
            switch (givenType) {
                case 'string':
                    dependencyids = String(dependencyids).split('|').join('+');
                    dependencyids = String(dependencyids).split('+');
                    break;
                case 'array':
                case 'object':
                    // Do nothing - we assume the variable is in an acceptable state
                    break;
                default:
                    consoleLog('Criticall Error in tagDependenciesMet() - Given dependencyids variable is of an unaccaptable type: ' + givenType);
                    return false;
            }
            
            if ( operation == 'and' ) {
                // Determin if all dependancies have been met (AND logic)
                var toReturn = true;
                for ( var x = 0 ; x < dependencyids.length ; x++ ) {
                
                    var thisDepId = String(dependencyids[x]);
                    if ( thisDepId.substring(0, 1) == '!' ) {
                        thisDepId = thisDepId.replace('!', '');
                        var notOperatorPresent = true;
                    } else {
                        var notOperatorPresent = false;
                    }
                    
                    dependencyAnwserObjAnwser = tagDependencyMet(thisDepId);
                    // Invert this value if the not (!) operator was present with this wizard-tag reference
                    if ( notOperatorPresent == true ) {
                        dependencyAnwserObjAnwser = ( dependencyAnwserObjAnwser == false );
                    }
                    
                    if ( dependencyAnwserObjAnwser == false ) {
                        toReturn = false;
                    }
                }
            } else {
                // Determin if any of the dependancies have been met (OR logic)
                var toReturn = false;
                for ( var x = 0 ; x < dependencyids.length ; x++ ) {
                
                    var thisDepId = String(dependencyids[x]);
                    if ( thisDepId.substring(0, 1) == '!' ) {
                        thisDepId = thisDepId.replace('!', '');
                        var notOperatorPresent = true;
                    } else {
                        var notOperatorPresent = false;
                    }
                    
                    dependencyAnwserObjAnwser = tagDependencyMet(thisDepId);
                    // Invert this value if the not (!) operator was present with this wizard-tag reference
                    if ( notOperatorPresent == true ) {
                        dependencyAnwserObjAnwser = ( dependencyAnwserObjAnwser == false );
                    }
                    
                    if ( dependencyAnwserObjAnwser == true ) {
                        toReturn = true;
                    }
                }
            }
            
            return toReturn;
        }
        
        /** bool tagDependencyMet(targetTag)
          * Determins the given tag have been selected in the BUSA-Wizard2
          * This function checks the imput directly, it does not depend on the wizard_selected_tags variable
          * @param targetTag - A wizard tag, defined in the first spreadsheet in the Wizard-Excel-Workbook
          */
        function tagDependencyMet(targetTag) {

            var dependencyAnwserObj = jQuery('option.wizard-tag-' + targetTag + ',input.wizard-tag-' + targetTag);
            if ( dependencyAnwserObj.length > 0 ) {
                dependencyAnwserObj = dependencyAnwserObj.eq(0);
            } else {
                return false;
            }
            
            var thisTagName = dependencyAnwserObj.get(0).tagName;
            
            // Use altername detection if this is a selectbox option
            if ( String(thisTagName).toLowerCase() == 'option' ) {
                dependencyAnwserObj = dependencyAnwserObj.parent();
                if ( dependencyAnwserObj.find('option:checked').attr('wiztag') == targetTag ) {
                    return true;
                } else {
                    return false;
                }
            }
            
            // If the above code-block did not return, then this dependencyAnwserObj is not a list-item
            if ( typeof dependencyAnwserObj.attr('checked') == 'undefined' || dependencyAnwserObj.attr('checked') != 'checked' ) {
                return false;
            } else {
                return true;
            }
            
        }
        
        /**
         * int updateSlideCountClass(optional AssumeAdditional)
         * @param AssumeAdditional - Given an integer, add +AssumeAdditional to the assumed total in the end.
         *
         * Updates the class associated with jQuery('.sections-container') div(s) that states how many slides this wizard uses.
         * The usage of this function is nessesary when hiding/showing/adding slides to the wizard.
         *
         * Return the number of slides that was set to be associated as visible with the wizards, and -1 when in mobile view
         */
        function updateSlideCountClass(assumeAdditional) {
            // assumeAdditional = 0 on default/missing
            if ( typeof assumeAdditional == 'undefined' ) {
                assumeAdditional = 0;
            }
            // Update the class that states the step/slide count on .sections-container
            if ( jQuery('body.responsive-layout-mobile').length == 0 ) { // Only nessesary if we are not in mobile view
                var sectionsContainer = jQuery('.sections-container');
                var visibleSectionCount = jQuery('.sections-container-top .section').not('.hidden-due-to-dependency').length + assumeAdditional;
                // Remove the current sections-count-# class associated with the sectionsContainer
                for ( var x = 0 ; x < wizard_step_count ; x++ ) {
                    sectionsContainer.removeClass('sections-count-' + x);
                }
                sectionsContainer.addClass('sections-count-' + visibleSectionCount);
                return visibleSectionCount;
            } else {
                return -1;
            }
        }
        
        /* Ensure parent-question have the wizard-question-parent class */
        // For each quest that is dependant on another question...
        jQuery('.wizard-question-has-dependency-true').each( function () {
            var jqThis = jQuery(this);
            var depId = jqThis.attr('dependencyid');
            
            // Ensure the question in which this question is depending on has the wizard-question-parent class
            jQuery('.wizard-question.wizard-tag-' + depId).addClass('wizard-question-parent');
        });
        
        function enforceWizardClasses(exclusiveSeek) {
            
            /* if we have not been set to the last step, then do the normal slider-seek */
            
            jQuery('.sections-container .section').removeClass('section-past');
            jQuery('.sections-container .section').removeClass('section-current');
            jQuery('.sections-container .section').removeClass('section-future');

            jQuery('.masterstep-title').removeClass('section-past');
            jQuery('.masterstep-title').removeClass('section-current');
            jQuery('.masterstep-title').removeClass('section-future');
            
            jQuery('.sections-container .section').removeClass('section-not-active');
            jQuery('.sections-container .section').removeClass('section-active');
            jQuery('.sections-container .section').addClass('section-not-active');
            
            jQuery('.section-number-' + wizard_current_step).removeClass('section-not-active');
            jQuery('.section-number-' + wizard_current_step).addClass('section-active');
            
            jQuery('.questions-for-step-container').removeClass('active-step');
            jQuery('.questions-for-step-container').removeClass('inactive-step');
            jQuery('.questions-for-step-container').addClass('inactive-step');
            jQuery('.questions-for-step-' + wizard_current_step + '-container').removeClass('inactive-step');
            jQuery('.questions-for-step-' + wizard_current_step + '-container').addClass('active-step');
            
            jQuery('.wizard-container').removeClass('wizard-state-slide-first');
            if ( wizard_step_count == 1 ) {
                jQuery('.wizard-container').addClass('wizard-state-slide-first');
            }
            
            jQuery('.wizard-container').removeClass('wizard-state-slide-results');
            if ( wizard_step_count == wizard_current_step ) {
                jQuery('.wizard-container').addClass('wizard-state-slide-results');
            }
            
            for ( var s = 1 ; s < wizard_step_count + 1 ; s++ ) {
                if ( s < wizard_current_step ) {
                    jQuery('.section-number-' + s).addClass('section-past');
                    jQuery('.masterbeforestep-' + s).addClass('section-past');
                }
                if ( s == wizard_current_step ) {
                    jQuery('.section-number-' + s).addClass('section-current');
                    jQuery('.masterbeforestep-' + s).addClass('section-current');
                }
                if ( s > wizard_current_step ) {
                    jQuery('.section-number-' + s).addClass('section-future');
                    jQuery('.masterbeforestep-' + s).addClass('section-future');
                }
                jQuery('.wizard-container').removeClass('wizard-state-slide-' + s);
            }
            
            jQuery('.wizard-container').addClass('wizard-state-slide-' + wizard_current_step);
            
            /* Set the current slids title as a class as a class-name/status on the wizard */
            // Remove all slide titles from wizard class statuses
            jQuery('.wizard-container .section .section-title').each( function () {
                var jqThis = jQuery(this);
                var thisTitle = String(jQuery.trim(jqThis.text())).toLowerCase();
                jQuery('.wizard-container').removeClass('wizard-state-slide-title-' + thisTitle);
            });
            // Set current slide
            var currentSlideTitle = jQuery('.section.section-active .section-title').eq(0).text();
            currentSlideTitle = jQuery.trim(currentSlideTitle);
            jQuery('.wizard-container').addClass('wizard-state-slide-title-' + currentSlideTitle.toLowerCase());
            
            /* 
                if we are in mobile view, toggle visibility to the currently selected step, and hide all others 
                Assume all slides will be hidden except for the slide with the mobile-selected class
            */
            if ( exclusiveSeek == true ) {
                var currentSlide = jQuery('.questions-for-step-' + wizard_current_step + '-container');
                if ( currentSlide.hasClass('mobile-selected') ) {
                    jQuery('.questions-for-step-container').removeClass('mobile-selected');
                } else {
                    jQuery('.questions-for-step-container').removeClass('mobile-selected');
                    currentSlide.addClass('mobile-selected');
                }
            } else {
                jQuery('.questions-for-step-container').removeClass('mobile-selected');
            }

            /* if we have been set to the last step, then do an ajax call to get the results html */
            if ( wizard_step_count == wizard_current_step ) {
                wizardGetResults();
            }
            
            /* Show welcome message if we clicked back off from the normal wizard steps */
            if ( wizard_current_step < 1 ) {
                // Run code from a JavaScript hook, if no hook exists, run the default caode below
                if ( typeof wizard_showwizardwelcome_hook != 'undefined' ) {
                    wizard_showwizardwelcome_hook.call();
                } else {
                    wizard_current_step = 1;
                    enforceWizardClasses(false);
                    jQuery('.wizard-contents').fadeOut( 500, function () { 
                        jQuery('.wizard-welcomemessage').fadeIn(500); 
                    });
                }
            }
            
            /* Set focus to the top of the currently visible slide */
            jQuery('.section-start-skipback-button:visible').focus();
            setTimeout( function () {
                // And do this again in 250 milliseconds - Bug killer since NVDA somehow gets in the way of this firing the first time
                jQuery('.section-start-skipback-button:visible').focus();
            }, 250);
            
            showHideNextButtonPerWizadRules();
        }
        
        function wizardGetResults() {
            
            var targetContainerSlide = jQuery('.questions-for-step-' + wizard_step_count + '-container, .results-slide-container');
            targetContainerSlide.html('<div style="color: black; padding-top: 50px; text-align: center; padding-bottom: 50px;"><img src="/sites/all/themes/bususa/images/ajax-loader.gif" style="margin-right: 10px;" />Please wait as we search our resources...</div>');
            targetContainerSlide.removeClass('inactive-step');
            
            if ( typeof lock_wizardGetResults != 'undefined' && lock_wizardGetResults == true ) {
                consoleLog('Holding off on executing wizardGetResults() due to lock.');
                setTimeout( function () {
                    consoleLog('Triggering wizardGetResults() now.');
                    wizardGetResults();
                }, 500);
                return;
            }
            
            /* Get the (results) html to inject into the dom */
            var postData = buildWizardPostData();
            lastPostData = postData;
            
            var wizardCachedResultURL = getWizardResultCacheLocation(postData);
            wizardCachedResultURL = wizardCachedResultURL['path']
            
            if ( typeof wizard_donotusecache != 'undefined' && wizard_donotusecache == true ) {
                wizardCachedResultURL = '/foobar/url/always/return/404';
            }
            
            jQuery.get(wizardCachedResultURL, wizardGotResults)
            .fail( function() {
                consoleLog('Could not obtain cached version of results ( ' + wizardCachedResultURL + ' ). Hitting origin for results...');
                jQuery.post('/sites/all/themes/bususa/templates/wizard/wizard-results.ajax.php' + document.location.search, postData, wizardGotResults).fail( function() {
                    consoleLog('Wizard origin-results has returned with failure!');
                    alert('An error has occurred while processing your request. Please refresh/reload your page and try again.');
                });
            });
            
        }
        
        function wizardGotResults(data) {
        
            // We expect a JSON object, parse it if it is not already done
            if ( typeof data != 'object' ) {
                data = JSON.parse(data);
            }
            
            // Inject html
            var targetContainerSlide = jQuery('.questions-for-step-' + wizard_step_count + '-container, .results-slide-container');
                targetContainerSlide.fadeOut( function () {
                targetContainerSlide.html(data.html);
                targetContainerSlide.addClass('results-slide-container');
                targetContainerSlide.fadeIn( function () {
                
                    targetContainerSlide.css('display', '');
                    
                    // Tigger triggerSelectBoxItLib(), which converts/themes html-select boxes by jQuery-SelectBoxIt library
                    triggerSelectBoxItLib();
                    
                    // Apply event handeler onto the select box that decides which template we are going to use
                    jQuery('.wizard-results-formatselector-originalselectbox').bind('change', function () {
                        wizard_result_template = jQuery(this).val();
                        wizardGetResults();
                    });

                });
            });
            
            debug_last_wizardGetResults_data = data;
        }
        
        function wizardGetExcelResults(eventHandeler) {
            
            /* Get the (results) html to inject into the dom */
            var postData = buildWizardPostData();
            postData['template'] = 'excel';
            lastPostData = postData;
            
            jQuery.post('/sites/all/themes/bususa/templates/wizard/wizard-results.ajax.php', postData, function(data) {
            
                // We expect a JSON object, parse it if it is not already done
                if ( typeof data != 'object' ) {
                    data = JSON.parse(data);
                }
                
                // Trigger event haneler
                eventHandeler.apply(this, [data]);
                
            });
            
        }
        
        function buildWizardPostData() {
            
            wizardSelectBoxTagUpdate();
            checkboxClickEventHandelerForTagging(true);
            var postData = {};
            
            /* START: By-wizard hacks go here */
            
                // Healthcare wizard to always search for the National tag
                if ( jQuery('.wizard-container-healthcarechanges').length > 0 ) {
                    consoleLog('Note: The Nation tag is getting applied to wizard_selected_tags. This is a special override for the HealthCare wizard. /* Coder Bookmark: CB-F4F7LT6-BC */ ');
                    wizard_selected_tags['National'] = 'National';
                }
                
            /* END: By-wizard hacks go here */
            
            // Bug killer - dont search for blank tags
            if ( typeof wizard_selected_tags[''] != 'undefined' ) {
                consoleLog('Removing blank-tag from wizard_selected_tags. /* Coder Bookmark: CB-4DZMECH-BC */ ');
                delete wizard_selected_tags[''];
            }
            
            postData['template'] = wizard_result_template;
            postData['wizard_selected_tags'] = JSON.stringify(wizard_selected_tags);
            postData['wizard_uses_side_bar'] = wizard_uses_side_bar;
            postData['inputs'] = {};
            postData['wizardName'] = 'healthcarechanges';
            postData['wizard_file_src'] = '/var/www/business/files-directory/healthcare_3.xls';
            postData['wizard_unique_id'] = 'healthcare';
            postData['wizard_url'] = 'http://business.usa.gov/healthcare'; /* Coder Bookmark: CB-DHHOAKG-BC */
            postData['wizard_current_step'] = wizard_current_step; /* Coder Bookmark: CB-KTTT4A5-BC */
            postData['wizard_result_cache_location'] = getWizardResultCacheLocation(postData);
            
            jQuery('.wizard-container input').each( function () {
                var jqThis = jQuery(this);
                if ( typeof jqThis.attr('wiztag') != 'undefined' && jQuery.trim(jqThis.attr('wiztag')) != 'wizard-tag-' && jQuery.trim(jqThis.attr('wiztag')) != '' ) {
                    if ( jqThis.attr('type') == 'checkbox' ) {
                        if ( typeof jqThis.attr('checked') != 'undefined' && jqThis.attr('checked') == 'checked' ) {
                            postData.inputs[jqThis.attr('wiztag')] = true;
                        } else {
                            postData.inputs[jqThis.attr('wiztag')] = false;
                        }
                    }
                    if ( jqThis.attr('type') == 'textbox' ) {
                        postData.inputs[jqThis.attr('wiztag')] = jqThis.val();
                    }
                }
            });
            jQuery('.wizard-container textarea').each( function () {
                var jqThis = jQuery(this);
                if ( typeof jqThis.attr('wiztag') != 'undefined' && jQuery.trim(jqThis.attr('wiztag')) != 'wizard-tag-' && jQuery.trim(jqThis.attr('wiztag')) != '' ) {
                    postData.inputs[jqThis.attr('wiztag')] = jqThis.val();
                }
            });
            
            last_buildWizardPostData = postData;
            return postData;
        }
        
        /**
         * string getWizardResultCacheLocation(string)
         * Based on the MD5 of the postData argument - determin where on the server the resultset may be cached
         */
        function getWizardResultCacheLocation(postData) {
            
            var partialPostData = {
                wizard_unique_id: postData['wizard_unique_id'],
                wizard_file_src: postData['wizard_file_src'],
                wizard_selected_tags: postData['wizard_selected_tags']
            };
            
            // If there is a zip-code input, add this to the ppData
            var zips = jQuery('.wizard-uniqueid-' + postData['wizard_unique_id'] + ' .auto-fill-zip-code');
            if ( zips.length > 0 ) {
                partialPostData['auto-fill-zip-code'] = zips.val();
            }
            
            partialPostData = JSON.stringify(partialPostData);
            var partialPostDataMd5 = md5( partialPostData );
            return {
                partialPostData: partialPostData,
                partialPostDataMd5: partialPostDataMd5,
                path: '/sites/default/files/wizard-result-cache-' + partialPostDataMd5 + '.txt'
            };
        }
        
        /**
         * void getWizardResultCacheLocation(string)
         * Based on the wizard_prepop_tags parameter in the URL-query (if there is one), select certain tags on the wizard 
         */
        function applyWizardPrePopTags () {
            wizard_prepop_tags = unescape(objUrlQueries['wizard_prepop_tags']); // Later: To do in PHP
            wizard_prepop_tags = JSON.parse(wizard_prepop_tags); // Later: To do in PHP

            // foreach wazard tag that needs to be set
            for ( var thisTag in wizard_prepop_tags ) {
                var targetTagElement = jQuery('[wiztag=' + thisTag + ']').not('.prepop-applied');
                if ( targetTagElement.length > 0 ) {
                    var elementTagName = String(targetTagElement.get(0).tagName).toLowerCase();
                    
                    if ( elementTagName == 'option' ) {
                        // This wizard-tag refers to an option in a selectbox
                        consoleLog('As part of the wizard_prepop_tags directive, setting a selectbox to the value of ' + thisTag + ' /* Coder Bookmark: CB-DSQAMSU-BC */ ');
                        var selectElement = targetTagElement.parent().not('.prepop-applied');
                        selectElement.val(thisTag); // Set the value for the <select> element
                        selectElement.change(); // Trigger any change() events bound to this element
                        selectElement.addClass('prepop-applied'); // We will use this class to note that pre-population action has been taken on this element, and should not be taken again
                    } else {
                        // This wizard-tag refers to something else (probably a checkbox)
                        consoleLog('Simulating click onto [wiztag=' + thisTag + '] as part of the wizard_prepop_tags directive. /* Coder Bookmark: CB-DSQAMSU-BC */ ');
                        targetTagElement.click();
                    }
                    
                    targetTagElement.addClass('prepop-applied'); // We will use this class to note that pre-population action has been taken on this element, and should not be taken again
                }
            }

            // Bug killer, override the wizard_selected_tags variable with wizard_prepop_tags
            setTimeout( function () {
                consoleLog('overriding the wizard_selected_tags variable with wizard_prepop_tags. wizard_prepop_tags is currently the following:');
                consoleLog(wizard_prepop_tags);
                wizard_selected_tags = wizard_prepop_tags;
                consoleLog('wizard_selected_tags set, its value is now the following:');
                consoleLog(wizard_selected_tags);
                consoleLog('/* Coder Bookmark: CB-A76T2UU-BC */');
            }, 100 );
        }
        applyWizardPrePopTags(); 
        
        
        
        function wizardLoadMoreFBOResults(eventHaneler) {
            lastPostData.skipCount = fbo_getTotalDownloadedResultCount();
            jQuery.post('/sites/all/themes/bususa/templates/wizard/wizard-results.ajax.php' + document.location.search, lastPostData, function (data) {
                lastMoreRslts = data;
                jQuery(data.html).find('#wizard-results-type-fbo .result-type-fbo').each( function () {
                    var jqThis = jQuery(this);
                    thisClassRsltId = jqThis.attr('class').split(' ')[2];
                    if ( jQuery('#wizard-results-type-fbo .' + thisClassRsltId).length == 0 ) {
                        var thisClasses = jqThis.attr('class').split(' ');
                        thisClasses[1] = '';
                        thisClasses[1] = 'result-id-' + (fbo_getTotalDownloadedResultCount() + 1);
                        jqThis.attr('class', thisClasses.join(' '));
                        jqThis.insertBefore('#wizard-results-type-fbo .section-no-results-area');
                    }
                });
            }).always( function () {
                if ( typeof eventHaneler != 'undefined' ) {
                    eventHaneler.call();
                }
            });
        }

        function fbo_getTotalDownloadedResultCount() {
            var fboLastResult = jQuery('#wizard-results-type-fbo .result-type-fbo').last();
            var fboLastResultClasses = fboLastResult.attr('class').split(' ');
            var fboLastResultCountId = fboLastResultClasses[1].split('-')[2];
            return parseInt(fboLastResultCountId);
        }
        

        
