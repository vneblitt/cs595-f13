    <script>
    
        /* Event handelers for the Share Content links */
        jQuery(document).ready( function () {
        
            ignoreBodyClickEventForHidingShareContentLinks = false;
            
            // When the user clicks the "Share" button, toggle the GMail/FaceBook/Twitter/LinkedIn options UI
            jQuery('.sharecontent-mastercontainer-uniqid-52496c63ad7e6 .sharecontent-sharebtn-share').bind('click', function () { /* Coder Bookmark: CB-ZMT29RZ-BC */
                consoleLog('.sharecontent-sharebtn-share click event. Coder Bookmark: CB-ZMT29RZ-BC');
                var jqContainer = jQuery(this).parents('.sharecontent-mastercontainer');
                if ( jqContainer.find('.sharecontent-dropdown-container-outter').css('display') == 'none' ) {
                    jqContainer.find('.sharecontent-dropdown-container-outter').fadeIn();
                } else {
                    jqContainer.find('.sharecontent-dropdown-container-outter').hide();
                }
            });
            
            // We will want to hide the Share Content links (drop down) when the user clicks on the document's body
            jQuery('body').bind('click', function () { /* Coder Bookmark: CB-WCJEMIE-BC */
                if ( ignoreBodyClickEventForHidingShareContentLinks == false ) {
                    jQuery('.sharecontent-dropdown-container-outter').hide();
                }
            });
            
            // But we will not want to hide the Share Content links (drop down) when the user clicks within the drop-down (if a users clicks on a Share-To-# button, the event of THAT button will hide the drop-down)
            jQuery('.sharecontent-mastercontainer-uniqid-52496c63ad7e6, .sharecontent-mastercontainer-uniqid-52496c63ad7e6 .sharecontent-sharebtn sharecontent-sharebtn-share, .sharecontent-mastercontainer-uniqid-52496c63ad7e6 .sharecontent-dropdown-container-outter').bind('click', function () {
                /* Coder Bookmark: CB-8EPVTD0-BC */
                ignoreBodyClickEventForHidingShareContentLinks = true;
                setTimeout( function () {
                    ignoreBodyClickEventForHidingShareContentLinks = false;
                }, 150);
            });
            
                        
                /* When ShareContentLinks.php::generateShareContentMarkup() was called to generate this markup, the $visibilityDependancyJQuery was NOT set to FALSE
                This mean this "Share +" widget's visibility willb e dependant on jQuery("<visibilityDependancyJQuery>").length
                Refer to the ShareContentLinks.php::generateShareContentMarkup() function-documentation for more information on this implementation */
                if ( jQuery('#page-title:visible').length == 0 ) {
                    consoleLog('Hiding .sharecontent-mastercontainer-uniqid-52496c63ad7e6 since its visibility is dependant on the jQuery #page-title:visible /* Coder Bookmark: CB-PO1TOG1-BC */ ');
                    jQuery('.sharecontent-mastercontainer-uniqid-52496c63ad7e6').hide();
                }
                setTimeout( function () {
                    if ( jQuery('#page-title:visible').length == 0 ) {
                        consoleLog('Hiding .sharecontent-mastercontainer-uniqid-52496c63ad7e6 since its visibility is dependant on the jQuery #page-title:visible /* Coder Bookmark: CB-H7QIBV7-BC */ ');
                        jQuery('.sharecontent-mastercontainer-uniqid-52496c63ad7e6').hide();
                    }
                }, 500);
                setTimeout( function () {
                    if ( jQuery('#page-title:visible').length == 0 ) {
                        consoleLog('Hiding .sharecontent-mastercontainer-uniqid-52496c63ad7e6 since its visibility is dependant on the jQuery #page-title:visible /* Coder Bookmark: CB-Q5K1XE5-BC */ ');
                        jQuery('.sharecontent-mastercontainer-uniqid-52496c63ad7e6').hide();
                    }
                }, 1000);
                
            
                        
        });
        function close_email_dialogue(){
            jQuery('#colorbox #cboxClose').click();
        }
    </script>
    
