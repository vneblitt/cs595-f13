<script type="text/javascript">
<!--//--><![CDATA[//><!--

			/* JavaScript from the Version Awareness module */
			codebaseStatus = {"Path":"core","URL":"https:\/\/omb-dashboardplatform.reisys.com:10058\/Business-USA\/trunk\/webapp\/core","Relative URL":"^\/trunk\/webapp\/core","Repository Root":"https:\/\/omb-dashboardplatform.reisys.com:10058\/Business-USA","Repository UUID":"948744eb-f611-4f28-8820-250fb6d8f47d","Revision":7320,"Node Kind":"directory","Last Changed Author":"dfrey","Last Changed Rev":7320,"Last Changed Date":"2013-09-24 12:44:04 -0400 (Tue, 24 Sep 2013)","commit-unix-time":1380043196}; 
			codebaseStatus.hotfixes = [];
			
			/** ApplyHotfix
			  * Will execute parameter 4 as a function is parameter 1 is an integer 
			  * greater then the current codebaseStatus.Revision
			  */
			function ApplyHotfix(onlyWhenRevisionIsLowerThan, onDocReady, functionArguments, functionToExecute) {
				
				// Note this hotfix for debug purposes
				var report = {
					'hotfixUntilRevision': onlyWhenRevisionIsLowerThan,
					'toApplyOnDocReady': onDocReady,
					'functionToExecute': functionToExecute
				};
				
				if ( onlyWhenRevisionIsLowerThan != 'always' && codebaseStatus.Revision >= onlyWhenRevisionIsLowerThan ) {
					report.wasApplied = false;
					codebaseStatus.hotfixes.push(report);
					return false;
				} else {
					if ( typeof onDocReady != 'undefined' && onDocReady == true ) {
						jQuery(document).ready( function () { 
							ApplyHotfix(onlyWhenRevisionIsLowerThan, 'applied on doc-ready', functionArguments, functionToExecute)
						});
					} else {
						if (typeof functionArguments != 'undefined' && typeof functionArguments.length != 'undefined' && functionArguments.length > 0 ) {
							report.calledWithArguments = true;
							report.functionArguments = functionArguments;
							report.functionReturned = functionToExecute.apply(this, functionArguments);
						} else {
							report.calledWithArguments = false;
							report.functionReturned = functionToExecute.call();
						}
						report.wasApplied = true;
						codebaseStatus.hotfixes.push(report);
					}
					return true;
				}
			}
		
//--><!]]>
</script>
