<!-- #main-content landing anchor -->
<script>
    jQuery(document).ready( function () {
        jQuery('#main-content').attr('href', 'javascript: // Main content starts here');
        jQuery('#main-content').attr('alt', '');
        jQuery('#main-content').text('Main content starts here');
        jQuery('#main-content').addClass('offscreen-508');
        
        jQuery('#skip-link a').attr('href', 'javascript: // Skip to main content');
        jQuery('#skip-link a').bind('click', function () {
            jQuery('#main-content').focus();
        });

    });
</script>

            <!-- The following script (tag) is stored in sites/all/themes/bususa/templates/wizard/wizard-abstract.tpl.php -->
            <script>
                /* This script (tag) is stored in sites/all/themes/bususa/templates/wizard/wizard-abstract.tpl.php */
                // Run this function every 250ms
                setInterval( function () {
                    // Add/Remove the vertical-layout-mode-[true/false] class to the div.wizard-container as nessesary
                    var wizardContainer = jQuery('div.wizard-container');
                    
                    // If we are in the tablet-narrow layout mode AND the wizard is in widget mode...
                    if ( jQuery('body.responsive-layout-narrow').length > 0 && wizardContainer.hasClass('wizard-in-widget-mode-true') ) {
                        // Then we SHOULD have a vertical-layout-mode-true class on the div.wizard-container element 
                        if ( wizardContainer.hasClass('vertical-layout-mode-true') == false ) {
                            wizardContainer.removeClass('vertical-layout-mode-true');
                            wizardContainer.removeClass('vertical-layout-mode-false');
                            wizardContainer.addClass('vertical-layout-mode-true');
                            consoleLog('Switched wizard into vertical-layout-mode');
                        }
                    } else {
                        // Then we should NOT have a vertical-layout-mode-true class on the div.wizard-container element
                        if ( wizardContainer.hasClass('vertical-layout-mode-true') == true ) {
                            wizardContainer.removeClass('vertical-layout-mode-true');
                            wizardContainer.removeClass('vertical-layout-mode-false');
                            wizardContainer.addClass('vertical-layout-mode-false');
                            consoleLog('Killed the wizard vertical-layout-mode');
                        }
                    }
                }, 250);
            </script>
        
