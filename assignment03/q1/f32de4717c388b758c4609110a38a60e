<script>

    // Hotfix to stop next-slide checking in IE - destory this hotfix ASAP
    jQuery(document).ready( function () {
        setTimeout( function () {
            function futureSlideExists() {
                
                // Temporary bug-killer
                if ( jQuery('body.ie').length > 0 ) {
                    return true;
                }
                
                var w = wizard_current_step;
                w++;
                while ( jQuery('.sections-container .section.section-number-' + w).eq(0).hasClass('hidden-due-to-dependency') ) {
                    w++;
                }
                
                if ( jQuery('.sections-container .section.section-number-' + w).length > 0 ) {
                    return true;
                } else {
                    return false;
                }
            }
        }, 1000);
    });

</script>



