	if ( typeof dfmNav.pData.loc != 'undefined' ){
		switch(dfmNav.pData.loc.city){
			case "San Rafael":
				dfmNav.pData.loc.city = "Marin";
				break;
				
			case "Scotts Valley":	
				dfmNav.pData.loc.city = "Santa Cruz";
				break;
				
			default:
				dfmNav.pData.loc.city = dfmNav.pData.loc.city;
				break;
		}
	}
	  var scriptOL = document.createElement( 'script' );
	  scriptOL.type = 'text/javascript';
	  scriptOL.id = 'ownLocalJS';
	  scriptOL.src = 'http://extras.mnginteractive.com/live/js/ownLocal/ownLocalRDS.js?domain=mylocal.' + dfm.env.domain + '.com&city=' + dfmNav.pData.loc.city + '&state=' + dfmNav.pData.loc.state;
	  dfm.$('head')[0].appendChild(scriptOL);
  }
	// Address the weather icon. In FF targeting with CSS just.doesn't.work, so, this:
	// CSS that doesn't work in FF: 
	// .weatherIcon, .weatherBox img, #weatherBoxReal img { width:10px!important; height:10px!important; max-width:10px!important; }
	dfm.$('.weatherIcon').css('height', '10px');
	dfm.$('.weatherIcon').show();
