    var section = document.location.pathname.split('/');
    var article_id;
    var sectiontmp = section[section.length-1]
    if ( sectiontmp.indexOf('ci_') == -1 )
    {
        sectiontmp = section[section.length-2]
    }
    var tmp = sectiontmp.split('ci_');
    
    //article_id = tmp[1].substr(0,8);
    article_id = articleId;
    var article_id_tmp = redirect_lookup(article_id);
    if ( article_id_tmp != article_id && article_id_tmp != '' ) article_id = article_id_tmp;

    //The section comes from the URL -- we split it at the slashes to get each part.
    //DEFINED ABOVE: var section = document.location.pathname.split('/');
    //If there is more than one part then we know we've got a section value
    var section_str = '';
    var section_name = '';
    var section_path = '';
    if ( section.length >  2 ) 
    {
        section_name = section[1];
        section_str = '&section=' + section_name;
        section_path = section_name + '/';
    }

    if ( document.location.hash == '#t' ) document.write('<script type="text\/javascript" src="http:\/\/neighbors.denverpost.com\/cache\/js\/feed_comments_new_' + article_id + '.js"><\/script>');
    else document.write('<script type="text\/javascript" src="http:\/\/extras.denverpost.com\/cache\/neighbors\/comments\/feed_comments_' + article_id + '.js"><\/script>');
    
