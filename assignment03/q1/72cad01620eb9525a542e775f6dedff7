// Co-branding, part one
// Do we have a hash with a value of a whitelisted domain?
// If so, the branding decision has been made.
var dfm_cobrand = false;
if ( document.location.hash != '' )
{
	var the_old_domain = dfm.env.domain;
	var dfm_hash = document.location.hash.substring(1,document.location.hash.length);
	switch ( dfm_hash )
	{
	case 'contracostatimes':
	case 'insidebayarea':
		dfm.env.domain = dfm_hash; // This won't work for all later references until the nav stops loading the dfm-core a second time and overwriting what's being assigned here.  
		dfm_cobrand = true;
		//console.log("COBRANDING WITH ", dfm_hash);
		break;
	}
	if ( dfm_cobrand == true )
	{
		// Rewrite the links on the page, adding the hash to article urls.
		dfm.$('a').attr('href', function() 
		{
			if ( is_it_article(this) == true ) return this + '#' + dfm.env.domain;
		});
		if ( dfm.$('#hot-topics').length ) dfm.$('#hot-topics').remove();
	}
}

