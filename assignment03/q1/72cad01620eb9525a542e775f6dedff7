<script type="text/javascript">
var write_pop_emailed = write_denverpost_widget_photos_hour8 = write_pop_entertainment = write_pop_business = write_denverpost_widget_hour8 = write_denverpost_widget_hourly8 = write_pop_news = write_pop_opinion = write_pop_random = write_pop_sports = write_pop_style = false;

// What widgets do we want? What order should they be in? What should they be named?
// The order here is the order the tabs will appear.
// The first of the pair of values is what the tab will be named,
// and the second of the pair is what list will be published.
// These are the available lists: day, hour, news, sports, business, entertainment, style, opinion
var pop_tabs = {
	'Day': 'day',
	'Hour': 'hour',
	'News': 'news',
	'Sports': 'sports',
        'Biz': 'business',	
        'A&amp;E': 'entertainment',
	//'Photos': 'photo'
	'Email': 'emailed',
	'Life': 'style'
}

// Don't mess with this, here is where we pick which tab to start on.
//if ( typeof(console) != "undefined" ) console.log(pop_tabs);
var pop_tabs_list = [];
for ( var key in pop_tabs ) pop_tabs_list.push(pop_tabs[key]);
var pop_tabs_rand = pop_tabs_list[Math.floor(Math.random() * pop_tabs_list.length)];
</script>

