function olBandAid(){    
 // if ( typeof dfmNav != 'undefined' && 'pData' in dfmNav ) {
// if ( typeof dfmNav == 'object' && 'pData' in dfmNav && typeof dfmNav.pData == 'object' && 'loc' in dfmNav['pData'] && 'city' in dfmNav['pData']['loc'] ) {
