function comment_order(action, comments, comments_reverse)
{
    var linktext = document.getElementById('commentreverse');
    var commentwrapper = document.getElementById('commentwrapper');
    var comments_list = comments;
    
    if ( action == 'reverse' )
    {
        linktext.innerHTML = '<a href="#" onClick="comment_order(\'forward\', forumtext, forumtext_reverse); return false;">List the oldest comments at the top</a>';
        commentwrapper.innerHTML = comments_reverse;
        var comment_ul = document.getElementById('forumtext_reverse');
        comment_ul.style.display = 'block';
    }
    else //if ( action == 'forward' )
    {
        // This "typeof(commenteight)" logic is used to differentiate
        // between the original comments and commenting functionality
        // and the revised.
        if ( typeof(commenteight) != 'undefined' ) 
        {
            comments_list.reverse();
            if ( typeof(SectionName) != "undefined" )
            {
                if ( section_name != "cartooncontest" ) comments_list = comments_list.slice(0,7);
            }
            
            
            if ( jQuery('.commentreverse').hasClass('hilit') == false )
            {
                jQuery('.commentforward').removeClass('hilit');
                jQuery('.commentreverse').addClass('hilit');
            }
            else
            {
                jQuery('.commentforward').addClass('hilit');
                jQuery('.commentreverse').removeClass('hilit');
            }
            
            jQuery('#comments').remove();
            jQuery('#commentwrapper').append('<ul id="comments" />');
            jQuery.each(comments_list, function(index, value) {
                jQuery('#comments').append(value);
            });
            if ( jQuery('abbr.timestamp').length > 0 && jQuery().timeago ) jQuery('abbr.timestamp').timeago();
        }
        else 
        {
            linktext.innerHTML = '<a href="#" onClick="comment_order(\'reverse\', forumtext, forumtext_reverse); return false;">List the newest comments at the top</a>';
            commentwrapper.innerHTML = comments;
        }


        // Make the thumbs-up / down open in a new page
        jQuery(".vote").attr('target', 'vote');
    }
}

if ( typeof(SectionName) != "undefined" )
{
    if ( SectionName != "Mark Kiszla" && SectionName != "Woody Paige" )
    {
        if ( commentnumber > 0 )
        {
            if ( typeof(commenteight) != 'undefined' ) 
            {
                document.write('<p style="clear:both; margin-right:5px;"><a href="#" onClick="comment_order(\'\', comment_items, \'\'); return false;"><b id="commentreverse" class="commentreverse hilit">Newest First</b> <b id="commentforward" class="commentforward">Oldest First</b> <b class="commentlabel">Toggle:</b></a></p>');
            }
            else document.write('<p style="clear:both;">&raquo; <strong id="commentreverse"><a href="#" onClick="comment_order(\'forward\', forumtext, forumtext_reverse); return false;">Show oldest comments first</a> (currently newest comments are first)</strong></p>');
        }
    }
}
</script>
<div id="commentwrapper">
<script type="text/javascript">
if ( commentnumber > 0 ) 
{
    // We don't display comments in some sections
    if ( typeof(SectionName) != "undefined" )
    {
        if ( SectionName == "Mark Kiszla" || SectionName == "Woody Paige" )
        {
            document.write("<h3>Click the link below to view the comments.</h3>");
        }
        else
        {
            if ( typeof(commenteight) != 'undefined' ) 
            {
                jQuery('.commentforward').addClass('hilit');
                jQuery('.commentreverse').removeClass('hilit');
                comment_order('', comment_items, '');
            }
            else document.write(forumtext_reverse);
        }
    }
    else
    {
        if ( typeof(commenteight) != 'undefined' ) 
        {
            jQuery('.commentforward').addClass('hilit');
            jQuery('.commentreverse').removeClass('hilit');
            comment_order('', comment_items, '');
        }
        else document.write(forumtext_reverse);
    }

}
