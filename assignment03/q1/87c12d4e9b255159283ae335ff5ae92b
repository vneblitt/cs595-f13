<script type="text/javascript">


// will show / hide the reply to comment fieldset
function ControlDisplay(fieldsetId,allowreply,value)
{
		if ( allowreply ==0)
		{
				alert("Log In to Post a Comment")
		}
		else
		{
		document.getElementById('replytext_'+fieldsetId).value = ""
			if(value ==1)
			{
				document.getElementById('fieldsetreplytoCommentId_' + fieldsetId).style.display = "";
			}
			else
			{
				document.getElementById('fieldsetreplytoCommentId_' + fieldsetId).style.display = "none";	
				
			}
		}
	}


// Reply to comment
function ReplyToComment(commentId,parentClassFormName,classFormId,autoApproval,name,email,webuserId)
{
	
	var replyText=document.getElementById('replytext_'+commentId).value

	cspHttpServerMethod('35AEQ5Q8BfZ14YmeVNO2$8FwdsUzIslesmdy2Alx0kbZxvYXfxKFbhWrf50Zzxe6kyw2bka_Qqr2bP4kIAOJoNGWrbPFbng_XycvEbeu45pmi35w4txiBAezZb8Vo2Wk',commentId,parentClassFormName,classFormId,replyText,name,email,autoApproval,webuserId)
	
	window.location.reload()
}

// will redirect to report comment page 
function ReportComment(commentId,childClassSqlFormName,classFormId)
{	
	window.location = "/csp/mediapool/sites/Recorder/templates/reportComment.csp?commentId="+commentId+"&childClassSqlFormName="+childClassSqlFormName+"&classFormId="+classFormId
}

function RateComment(commentId,classFormId,childClassFormName,allowRate,webuserId)
{

	if(allowRate==0)
	{
		alert("Log In to Rate a Comment")
	}
	else
	{
		var ratevalue=document.getElementById('rate_'+commentId).value
		if (ratevalue>0)
		{
			var value = cspHttpServerMethod('Z8zzWUIVBBQvvvBRww3nzwRQp7Y7bx4OY_kmQv43lIhOxuvpsTV_kuXAR4OqyG_2WRxPNfXz9yE4PVLDC5aUVQj8ATGIOPJjIx$cyg4FJ_iJIUOQo2QSg8E4Vm8YTlSw',commentId,classFormId,childClassFormName,ratevalue,webuserId)
			window.location.reload()
		}
	}

}


var nbPages = 0;
var currentPage = 1;



function previousPage()
{
	if (currentPage==1)
		return;
	
	//shiftOpacity('page' + currentPage, 1000);
	document.getElementById('page' + currentPage).style.display = "none";
	currentPage = currentPage - 1;
	document.getElementById('page' + currentPage).style.display = "";
	//shiftOpacity('page' + currentPage, 1000);
}

function nextPage()
{
	if (currentPage==nbPages)
		return;
	
	//shiftOpacity('page' + currentPage, 1000);
	document.getElementById('page' + currentPage).style.display = "none";
	currentPage = currentPage + 1;
	document.getElementById('page' + currentPage).style.display = "";
	//shiftOpacity('page' + currentPage, 1000);
}

function shiftOpacity(id, millisec) {
    //if an element is invisible, make it visible, else make it ivisible
    if(document.getElementById(id).style.opacity == 0) {
        opacity(id, 0, 100, millisec);
    } else {
        opacity(id, 100, 0, millisec);
    }
}

function opacity(id, opacStart, opacEnd, millisec) {
    //speed for each frame
    var speed = Math.round(millisec / 100);
    var timer = 0;

    //determine the direction for the blending, if start and end are the same nothing happens
    if(opacStart > opacEnd) {
        for(i = opacStart; i >= opacEnd; i--) {
            setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
            timer++;
        }
        document.getElementById(id).style.display = "none";
    } else if(opacStart < opacEnd) {
	    document.getElementById(id).style.display = "";
        for(i = opacStart; i <= opacEnd; i++)
            {
            setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
            timer++;
        }
    }
}

//change the opacity for different browsers
function changeOpac(opacity, id) {
    var object = document.getElementById(id).style;
    object.opacity = (opacity / 100);
    object.MozOpacity = (opacity / 100);
    object.KhtmlOpacity = (opacity / 100);
    object.filter = "alpha(opacity=" + opacity + ")";
} 
</script>






