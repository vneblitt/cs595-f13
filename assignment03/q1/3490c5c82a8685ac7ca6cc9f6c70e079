<script type="text/javascript">
		
	function CallGoogleSearch(modeAvance) {
		
			var searchQuery = encodeURIComponent(document.getElementById('idSearchString').value);
		var sitesearch = "";
				
		/***********************************************		
		/ URL Need to be updated. 
		/ http://10.18.65.15/search is url for Dev
		/**********************************************/
		var GoogleUrl='http://search.ohchr.org/search'
		var GoogleEntqr='0'
		var GoogleUD='1'
		var GoogleSort='date%3AD%3AL%3Ad1'
		var GoogleOutput='xml_no_dtd&ie=UTF-8&oe=UTF-8'
		var GoogleSite='default_collection'
		var GoogleClient ='default_frontend'
		var GoogleProxyStyleSheetEN='en_frontend'
			
		
		var urlSearch = GoogleUrl 
		urlSearch += "?site=" + GoogleSite
		urlSearch += "&client=" + GoogleClient  
		urlSearch += "&output=" + GoogleOutput
		urlSearch += "&Entqr=" + GoogleEntqr
		urlSearch += "&ud=" + GoogleUD 
		urlSearch += "&sort=" + GoogleSort 
		urlSearch += "&proxystylesheet=" + GoogleProxyStyleSheetEN + "&q=";
		urlSearch += searchQuery;
		

	
		if (sitesearch.length > 0) {
			urlSearch += '&sitesearch=' + encodeURIComponent(sitesearch);
		}	
		
		document.location = urlSearch;
	}
    </script>
	
		

