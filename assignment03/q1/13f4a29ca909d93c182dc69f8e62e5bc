		jQuery(document).ready(function(){
			
		var currentVideo=428866;
	
		var nowPlaying=jQuery('div.videoTabsPromo #frag_'+currentVideo);
		
		jQuery(nowPlaying)
			.find('a.play_arrow').addClass('now_playing').removeClass('play_arrow')
			.parent().parent('.imageBorder').addClass('blueBorder');
		
		var wrapperDistance = jQuery(nowPlaying).position();

		jQuery(nowPlaying).parent('.middle').scrollTop(wrapperDistance.top);
				
		});
