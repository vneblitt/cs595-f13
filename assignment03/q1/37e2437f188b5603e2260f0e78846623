	<!-- Google Map API Key -->
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
      
	

		  
	<script type="text/javascript">
	function splitMap(maplocList)
		{
			if (maplocList.indexOf("|")== -1)
				{
				var address = $.trim(maplocList);
				var zoomLevel = 15;
				}
				else
				{
				var mapArray=maplocList.split("|");
				var address = $.trim(mapArray[0]);
				var zoomCase = mapArray[1];
				var zoomCaseInt = parseInt(zoomCase);
				var zoomLevel = zoomCaseInt;
				}
			if (zoomLevel > 21)
				{
				var zoomLevel = 15;
				}
			if (zoomLevel < 0)
				{
				var zoomLevel = 15;
				}
		
			return {addressSplit:address,zoomSplit:zoomLevel};
		}
	</script>
		  

 <script type="text/javascript">
 	maplocList = "St. Clair County, IL";
	mapLocField = splitMap($.trim(maplocList));
    var geocoder;
    var map;
      function initialize() {
        geocoder = new google.maps.Geocoder();
        var latlng = new google.maps.LatLng(-34.397, 150.644);
		var zoomOUT = mapLocField.zoomSplit;
        var myOptions = {
          zoom: zoomOUT,
          center: latlng,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        }
        map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
      }  
        
        function codeAddress() {
            var addressOUT = ($.trim(mapLocField.addressSplit));
            geocoder.geocode( { 'address': addressOUT}, function(results, status) {
              if (status == google.maps.GeocoderStatus.OK) {
                  map.setCenter(results[0].geometry.location);
                  var marker = new google.maps.Marker({
                        map: map, 
                        position: results[0].geometry.location
                  });
              } else {
                  alert("Geocode was not successful for the following reason: " + status);
              }
            });
        }     
    </script>
	

  
