   <script lang="javascript">
function setRecurAmounts() {
		document.getElementById('field-amount_total_1').value = '5';
		document.getElementById('amount_label_1').innerHTML  = '$5';
		document.getElementById('field-amount_total_2').value = '25';
		document.getElementById('amount_label_2').innerHTML  = '$25';
		document.getElementById('field-amount_total_3').value = '75';
		document.getElementById('amount_label_3').innerHTML  = '$75';
		document.getElementById('field-amount_total_4').value = '150';
		document.getElementById('amount_label_4').innerHTML  = '$150';
		document.getElementById('field-amount_total_5').value = '250';
		document.getElementById('amount_label_5').innerHTML  = '$250';
		document.getElementById('field-amount_total_6').value = '10';
		document.getElementById('amount_label_6').innerHTML  = '$10';
		document.getElementById('field-amount_total_7').value = '50';
		document.getElementById('amount_label_7').innerHTML  = '$50';
		document.getElementById('field-amount_total_8').value = '100';
		document.getElementById('amount_label_8').innerHTML  = '$100';
		document.getElementById('field-amount_total_9').value = '200';
		document.getElementById('amount_label_9').innerHTML  = '$200';
}

function setAmounts() {
		document.getElementById('field-amount_total_1').value = '10';
		document.getElementById('amount_label_1').innerHTML  = '$10';
		document.getElementById('field-amount_total_2').value = '50';
		document.getElementById('amount_label_2').innerHTML  = '$50';
		document.getElementById('field-amount_total_3').value = '250';
		document.getElementById('amount_label_3').innerHTML  = '$250';
		document.getElementById('field-amount_total_4').value = '1000';
		document.getElementById('amount_label_4').innerHTML  = '$1000';
		document.getElementById('field-amount_total_5').value = '5200';
		document.getElementById('amount_label_5').innerHTML  = '$5200';
		document.getElementById('field-amount_total_6').value = '25';
		document.getElementById('amount_label_6').innerHTML  = '$25';
		document.getElementById('field-amount_total_7').value = '100';
		document.getElementById('amount_label_7').innerHTML  = '$100';
		document.getElementById('field-amount_total_8').value = '500';
		document.getElementById('amount_label_8').innerHTML  = '$500';
		document.getElementById('field-amount_total_9').value = '2600';
		document.getElementById('amount_label_9').innerHTML  = '$2600';
}
   </script>


