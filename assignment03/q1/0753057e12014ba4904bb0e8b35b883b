<script type="text/javascript">

var previous = 0;

function setBoldAmount(n) {

    var nstr = "amount_label_" + n;

    var label = document.getElementById(nstr);
    label.style.fontWeight = "bold";

    if(previous != 0) {
	var label = document.getElementById(previous);
	label.style.fontWeight = "normal";
    }

    previous = nstr;

}

</script>

