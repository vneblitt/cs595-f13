				<script>
					var countA=1;
					if (countA==1) {
						$('#qleft').hide();
						$('#qright').show();
					}
					
					
					var moveRight = function() {
						if (countA==1) {
							$('#box1').animate({ marginLeft: "-877px"}, 300);countA=2;$('#qleft').show();
						}
						else if (countA==2) {
							$('#box1').animate({ marginLeft: "-1754px"}, 300);countA=3;
						}
						else if (countA==3) {
							$('#box1').animate({ marginLeft: "-2631px"}, 300);countA=4;
						}
						else if (countA==4) {
							$('#box1').animate({ marginLeft: "-3508px"}, 300);countA=5;;
						}
						else if (countA==5) {
							$('#box1').animate({ marginLeft: "-4380px"}, 300);countA=6;;
						}
						else if (countA==6) {
							$('#box1').animate({ marginLeft: "-5252px"}, 300);countA=7;;
						}
						else if (countA==7) {
							$('#box1').animate({ marginLeft: "0px"}, 300);countA=1;$('#qleft').hide();
						}
						else {return false;}
					};
					
					var moveLeft = function() {
						if (countA==2) {
							$('#box1').animate({ marginLeft: "0px"}, 300);countA=1;$('#qleft').hide();
						}
						else if (countA==3) {
							$('#box1').animate({ marginLeft: "-877px"}, 300);countA=2;
						}
						else if (countA==4) {
							$('#box1').animate({ marginLeft: "-1754px"}, 300);countA=3;
						}
						else if (countA==5) {
							$('#box1').animate({ marginLeft: "-2631px"}, 300);countA=4;
						}
						else if (countA==6) {
							$('#box1').animate({ marginLeft: "-3508px"}, 300);countA=5;
						}
						else if (countA==7) {
							$('#box1').animate({ marginLeft: "-4380px"}, 300);countA=6;$('#qright').show();
						}
						else {return false;}
					};
					
					var clickRight = function() {
						clearInterval(int);
						moveRight();
						int = setInterval(moveRight, 7500);
					}
					
					var clickLeft = function() {
						clearInterval(int);
						moveLeft();
						int = setInterval(moveRight, 7500);
					}
					
					$('#qright').click(clickRight);
					$('#qleft').click(clickLeft);
					
					var int = setInterval(moveRight, 7500);
					
					
				</script>
