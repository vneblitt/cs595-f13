					<!-- Entry -->
                    <!-- google_ad_section_start -->
					<div class="entry video clearfix" id="entry_12345">
						<div class="col entry_right full">
																<h1 class="title-news">
																				Head Start Cuts Services For More Than 57,000 Children Due To Sequestration
									</h1>
							
							<div class="comments_datetime_new border_none relative v05" data-beacon='{"p":{"mnid":"entryByline"}}'>
								<a class="print-link absolute" rel="nofollow" href="?view=print" data-beacon='{"p":{"lnid":"print"}}'></a>
																										<p>
										

