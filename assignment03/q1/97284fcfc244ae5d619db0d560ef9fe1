<span class="dateTime"><script type="text/javascript">
		
		

			function zeroPad(num, places) {
  				var zero = places - num.toString().length + 1;
  				return Array(+(zero > 0 && zero)).join("0") + num;
			}
			
            var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
			var daysOfWeek = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"];var todayDate=new Date();
			var day=todayDate.getDay();
			var date=todayDate.getDate();
			var month=todayDate.getMonth();
			var year=todayDate.getFullYear();
			var hours=todayDate.getHours();
			var minutes=todayDate.getMinutes();
			var seconds=todayDate.getSeconds();
			var meridian = (hours < 12) ? " AM, "  : " PM, ";
			if (hours == 0 || hours == 12) {
                                hours = 12;
                                document.writeln(hours + ":" + zeroPad(minutes,2) + meridian ); 
                        } else {
				document.writeln(zeroPad(hours%12, 2) + ":" + zeroPad(minutes,2) + meridian ); // ":" + zeroPad(seconds,2) +
			}
			document.writeln(daysOfWeek[day] + ", " );
			document.writeln(months[month] + " " + date + ", " + year);
			
