        function p_ShowPrintinggTips() {

            var language = getLanguage();
            if (language == 'EN') {
                document.getElementById("englishDialog").setAttribute("style", "display:block");
                $("#englishDialog").dialog({ width: 509, height: 270, resizable: false });
                console.log("dialog was just displayed...");
                      
            }
            else {
                document.getElementById("frenchDialog").setAttribute("style", "display:block");
                $("#frenchDialog").dialog({ width: 509, height: 310, resizable: false });                        
            }

        }

