<script src="/WebResource.axd?d=q3So8sUh9lchAC34_NL-6Ev6GH7j-2LFVsWAg8CcemVyn9ysAC4v7FT6Umu2S6gWdehSWKqNFNOGgtSejUXEu6VlRRxMw75mRKvtXljmWCv_j128Dn_Nen0an6ASKWdgL2BAuv_XcQB3Vl1Ft0qnncbtD5kLplv3DQYDm37az0ODxDhB4vgeBrqG_fIk5DSQuxvHyO4I-A739y5KzW4PoH5-NEw1&amp;t=634901258003798828" type="text/javascript"></script>
<script>
var detect = navigator.userAgent.toLowerCase();
var browser,version,thestring;

function checkBrowser()
{
    if (checkIt('safari'))
    {
        
        var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
        if(is_chrome == false)
        {
	        browser = 'Safari';
        }
        else
        {
            browser = null;
        }
    }
    if (!version) version = detect.charAt(place + thestring.length);
    return browser;
}

function checkIt(string)
{
	place = detect.indexOf(string) + 1;
	thestring = string;
	return place;
}

function toggleLayer(nID) {

	var pLayer;
	var pToggle;
    var currentBrowser;

    currentBrowser = checkBrowser();
    if(currentBrowser == 'Safari')
    {      
        var ctlTreeviewType =new RegExp('ctl00_MENU_TreeView1','gi');
		//Si l'element fait partie du menu gauche des categories
        if(nID.match(ctlTreeviewType))
        {
            var ctSource= window.event.srcElement; // Recupere l'object source
			if(ctSource != null)
			{
				//On cherche le noeud contenant l'indication de la categorie
				var nodeCategorie= ctSource.parentNode.parentNode.previousElementSibling;
				//Si null, alors on est sur un noeud errone
				if(nodeCategorie == null)
				{
					var tempID = parseInt(nID.substring(nID.length-1,nID.length));
					tempID--; 
					var newID = nID.substring(0,nID.length-1) + tempID;
					nID= newID;
				}
			}	
		}
    }               

	if (g__bIE4) {
		pLayer = document.all.item(nID);
		pToggle = document.all.item(nID + g__strToggleIDSuffix);
	} else {
		pLayer = document.getElementById(nID);
		pToggle = document.getElementById(nID + g__strToggleIDSuffix);
	}
	
	if (pLayer.style.display != 'none') {
		pLayer.style.display = 'none';

        var key = 'g__' + nID.substring(0,nID.length-2) + 'strCloseImgPath';
        pToggle.innerHTML = items.get(key);

	} else {
		pLayer.style.display = 'block';
        
        var key = 'g__' + nID.substring(0,nID.length-2) + 'strOpenImgPath';
        pToggle.innerHTML = items.get(key);

	}
}

function actionLayer(bExpand) {

	var pLayers;
	var pToggle;
	var strStyleDisplay;
	var strInnerHTML;
	var i;
	
	if (g__bIE4) {
		pLayers = document.all.tags('ul');
	} else {
		pLayers = document.getElementsByTagName('ul');
	}
	
	if (bExpand) {
		strStyleDisplay = 'block';
		/* strInnerHTML = g__strOpenImgPath; */
        /*strInnerHTML = items.get(nID);*/
	} else {
		strStyleDisplay = 'none';
		strInnerHTML = g__strCloseImgPath;
	}
	
	if (g__bIE4) {
		for (i = 0; i < pLayers.length; i++) {
			pToggle = document.all.item(pLayers[i].id + g__strToggleIDSuffix);
			if (pToggle) {
				pLayers[i].style.display = strStyleDisplay;
				pToggle.innerHTML = strInnerHTML;
			}
		}
	} else {
		for (i = 0; i < pLayers.length; i++) {
			pToggle = document.getElementById(pLayers[i].id + g__strToggleIDSuffix);
			if (pToggle) {
				pLayers[i].style.display = strStyleDisplay;
                pToggle.innerHTML = strInnerHTML;
			}
		}
	}

}
</script>

