				tags.push("poetry");
				
				tags.push("typography");
				
				tags.push("poetrymonth");
				
				tags.push("nytimes");
				
				tags.push("nythid:then-wine-took-over-leaving-little-shelf-space-fo");
				
				tags.push("hk0:Then wine took over__C__");
				
				tags.push("hk1:leaving little shelf space for");
				
				tags.push("hk2:the vodka and Scotch.");
				
				tags.push("hf:Laotian Banh Mi__C__ Surprising Cupcakes and More");
				
				tags.push("tid:0");
				
				tags.push("mobiledetailv:http://int-shared-data.nytimes.com/tumblr/haikus/timeshaiku/then-wine-took-over-leaving-little-shelf-space-fo/mobile_detail_tall_.gif");
				
				tags.push("detail2x:http://int-shared-data.nytimes.com/tumblr/haikus/timeshaiku/then-wine-took-over-leaving-little-shelf-space-fo/detail.2x.gif");
				
				tags.push("detail3x:http://int-shared-data.nytimes.com/tumblr/haikus/timeshaiku/then-wine-took-over-leaving-little-shelf-space-fo/detail.3x.png");
				
				tags.push("detailZ:http://int-shared-data.nytimes.com/tumblr/haikus/timeshaiku/then-wine-took-over-leaving-little-shelf-space-fo/detail4637.png");
				
				allTags["61038876011"] = tags;
			</script>
			
			
		</div><!--nytint-haiku-posts-->
	</div><!--nytint-haiku-content-->
	<div id="loading">
    <img src="http://graphics8.nytimes.com/projects/2013/haikus/images/ajax-loader.gif"/>
	</div>
	

</div><!--nytint-haiku-wrapper-->
<div id="nytint-haiku-responsiveness">
	<span class="hires"></span>
	<span class="phone"></span>
	<span class="mobile"></span>
</div>




<div id="fb-root"></div>
<script type="text/javascript">
	(function() {

		var postsSelector = $("div#nytint-haiku-posts"),
		    windowSelector = $(window),
		    hideListSelector = $('#hidelist'),
		    postListItemSelector = $("div.nytint-haiku-post"),
		    initialCount = postListItemSelector.length,
		    postListSelector = postsSelector,
		    hasMore = true,
		    limit = 5,
		    printCount = [],
		    triggerPoint = 300,
		    lastScrollTop = 0,
		    scrollTimer,
		    checkInterval,
				scrolling,
				$hiRes,
				$phone,
				$mobile;

		var isHiRes = function() {
					$hiRes = $hiRes || $('#nytint-haiku-responsiveness .hires');
					return $hiRes.css('display') != 'none';
				},
				isPhone = function() {
					$phone = $phone || $('#nytint-haiku-responsiveness .phone');
					return $phone.css('display') != 'none';
				},
				isMobile = function() {
					$mobile = $mobile || $('#nytint-haiku-responsiveness .mobile');
					return $mobile.css('display') != 'none';
				},
				loading = function(state) {
					var $loading = $("#loading");
					if (state === false) {
						$loading.hide();
					} else {
						$loading.show();
					}
				},
				revealPage = function() {
					loading(false);
					$('#nytint-haiku-content').css('visibility', 'visible');
				},
				loadMore = function(callback) {
			    loading();
			    var offset = $("div.nytint-haiku-post").length,
					    currPage = ((offset - initialCount) / limit) + 2; // first "page" of loaded in results is designated page 2
			    if ($('a[name="postpage' + currPage + '"]').length < 1) {
			        hideListSelector.append("<a class=\"postpage\" name=\"postpage" + currPage + "\"></a>");
			    }
			    $.ajax({
			        url: location.href + "page/" + currPage, /* location.href adds trailing / */
			        type: "GET",
			        dataType: 'html',
			        success: function(response) {
			            var d = $(response).find("div#nytint-haiku-posts"),
					            postsAdded = false,
					            anchorSelector;
			            if (!(currPage in printCount)) {
			                printCount[currPage] = true;
			                anchorSelector = $('a[name="postpage' + currPage + '"]');
			                anchorSelector.after($(d).html());
			                anchorSelector.nextUntil("a.postpage",".nytint-haiku-post").each(function() {
			                    var targetSelector = postListSelector;
			                    if (targetSelector.find("div[data-post-id='" + $(this).attr('data-post-id') + "']").length === 0) {
			                        targetSelector.append($(this));
			                        if (!postsAdded) {       
			                            postsAdded = true;
			                        }
			                    }
			                    else {
			                        $(this).remove();
			                    }
			                }); //end each
			            } // end if (!(currPage in printCount))
			            if (postsAdded) {
			                parsePostTags();
			                renderIndexPosts();
			            }
			            if ($("div.nytint-haiku-post").length < offset + limit) {
			                hasMore = false;
			            }
			            if ($.isFunction(callback)) {
			            	callback();
			            }
			        }, // end success
			        error: function(jqxhr, txt, errr) {
			            console.log(errr);
			        }, // end error
			        complete: function(jqxhr, txt) {
			            loading(false);
			        } // end complete
			    }); // end ajax
			},
			doLoad = function() {
				var newScrollTop = windowSelector.scrollTop(),
						a,
						b,
						doIt = false;
				if (newScrollTop > lastScrollTop) { // only check when scrolling down
					a = postsSelector.offset().top + postsSelector.outerHeight();
		      b = newScrollTop + windowSelector.height() + triggerPoint;
		      doIt = (hasMore && newScrollTop > lastScrollTop && (a <= b));
		    }
	      lastScrollTop = newScrollTop;
	      return doIt;
			},
			checkAutoLoadMore = function() {
				if (doLoad() || (hasMore && $(document).height() <= windowSelector.height())) {
					loadMore(checkAutoLoadMore);
				}
			},
			parsePostTags = function() {
		    $('.nytint-haiku-post:not([data-headline-full])').each(function (i,postelement) {
		        pid = $(postelement).attr('data-post-id');
		        if (pid in allTags) {
		            postTags = allTags[pid];
		            delete allTags[pid];
		            var postSelector = $("#" + pid);
		            headlinepartscount = 0;
		            $.each(postTags, function(i,thistag) {
		                var tagparts = [];
		                tagparts[0] = thistag.substr(0,thistag.indexOf(":"));
		                tagparts[1] = thistag.substr(thistag.indexOf(":") + 1);
		                
		                tagvalue = tagparts[1].replace(/__C__/g, ',');
		                tagvalue = tagvalue.replace(/__Q__/g, '"');
		                
		                tagvalueencoded = $('<div />').html(tagvalue).text();
		                // console.log(tagparts[0] + " is " + tagvalueencoded);
		                
		                switch (tagparts[0]) {
		                	case 'hf':
			                	postSelector.attr("data-headline-full", tagvalueencoded);
		                		break;
		                	case 'detail2x':
			                	/* portrait detail image retina */
		                		postSelector.attr("data-permalink-image-retina", tagvalueencoded);
		                		break;
		                	case "mobiledetailv":
		                        /* portrait mobile detail image */
		                        postSelector.attr("data-permalink-mobile-image", tagvalueencoded);
		                    break;
			                default:
				                /* parts of headline or haiku */
			                    tagtype = tagparts[0].substr(0,2);
			                    tagnum = tagparts[0].substr(2,1);
			                    if (tagtype === 'hk') {
			                        postSelector.attr("data-haikuline-" + tagnum, tagvalueencoded);
			                    }
			                    else if (tagtype === 'hd') {
			                        postSelector.attr("data-headlinepart-" + tagnum, tagvalueencoded);
			                        headlinepartscount++;
			                    }
		                    break;
		                }
		            }); // end thistag
		            postSelector.attr("data-headlineparts-count", headlinepartscount);
		        }
		    }); // end postTags
			},
			renderIndexPosts = function() {
					$('.nytint-haiku-post').each(function (i, elem) {
						var $elem = $(this);
						if (!$elem.data('rendered-post')) {
							// text version
							$('p.nytint-haiku-text a', $elem).each(function(i) {
								$(this).text($elem.data('haikuline-' + i));
							});
							$('.nytint-article-headline', $elem).text($elem.data('headline-full'));
							$elem.data('rendered-post', 1);
						}
					});
			},
			renderPermalinkPosts =function() {
				$('.nytint-haiku-post').each(function (i, elem) {
					var $elem = $(this),
							imgType = (isPhone()) ?
								((isHiRes()) ? 'permalink-mobile-image' : 'permalink-mobile-image') :
								((isHiRes()) ? 'permalink-image-retina' : 'permalink-image');
					if (!$elem.data('rendered-post')) {
						$('img.nytint-haiku-image', $elem).attr('src', $elem.data(imgType));
						$elem.data('rendered-post', 1);
					}
				});
			},
			updatePageTitle = function(pid) {
			    if (pid && pid != '') {
			        var postSelector = $("#" + pid);
			        if (postSelector.attr("data-haikuline-0") && postSelector.attr("data-haikuline-1") && postSelector.attr("data-haikuline-2")) {
			            /* update page title */
			            document.title += ":  " + $('<div />').html([postSelector.attr("data-haikuline-0"),postSelector.attr("data-haikuline-1"),postSelector.attr("data-haikuline-2")].join(" / ")).text();
			        }
			    }
			};

		

		$(document).ready(function() {
		    parsePostTags();
			
			
		    renderPermalinkPosts();
				
		    updatePageTitle(61038876011);
		    setTimeout(revealPage, 200);
				
			
		    FB.init({appId: "9869919170", status: true, cookie: true}); //nytappid
		});

		// hackrageous
		$('#nytint-haiku-posts').on('click', '.nytint-haiku-text', function() {
			window.location.href = $('a', this)[0].href;
		});

    $('#nytint-haiku-posts').on("click","a.sharing", function (event){
        var url = $(this).attr("href");
        var windowName = $(this).attr("target");
        var windowWidth, windowHeight;
        pSelector = $(this).closest(".nytint-haiku-post");
        if ($(this).hasClass("twitter")) {
            windowWidth = 550;
            windowHeight = 340;
            url = url + "&text=" + encodeURIComponent([pSelector.attr("data-haikuline-0"),pSelector.attr("data-haikuline-1"),pSelector.attr("data-haikuline-2")].join("\n")) + encodeURIComponent("\nvia @nytimes");
        } else if ($(this).hasClass("facebook")) {
            var fbUrl = $(this).attr("data-fb-link");
            var fbImg = 'http://graphics8.nytimes.com/projects/2013/haikus/images/timeshaiku_FB_200.png';
            var obj = {
                method: 'feed',
                display: 'popup',
                link: fbUrl,
                picture: fbImg,
                caption: 'Times Haiku',
                description:  'Serendipitous Poetry from The New York Times',
                name: [pSelector.attr("data-haikuline-0"),pSelector.attr("data-haikuline-1"),pSelector.attr("data-haikuline-2")].join(" / \n")
            };
            FB.ui(obj);            
        } else {
            windowWidth = 650;
            windowHeight = 520;
        }
        if (!$(this).hasClass("facebook")) {
            var windowLeft = parseInt((screen.availWidth/2) - (windowWidth/2));
            var windowTop = parseInt((screen.availHeight/2) - (windowHeight/2));
            var windowSize = "width=" + windowWidth + ",height=" + windowHeight + ",left=" + windowLeft + ",top=" + windowTop + ",screenX=" + windowLeft + ",screenY=" + windowTop + ",scrollbars=no";
            window.open(url, windowName, windowSize);
        }
        event.preventDefault();
    });

		// WEB TRENDS:
		// (let's just keep it separate from other sharing code)
		$('.share-tools-list').on('mousedown', 'a', function(e) {
			var $elem = $(this),
					path,
					title,
					location;
			if (dcsMultiTrack !== undefined && $.isFunction(dcsMultiTrack)) {
				if ($elem.hasClass('twitter')) {
					path = '/Article-Tool-Share-Twitter.html';
					title = 'Article-Tool-Share-Twitter';
				} else if ($elem.hasClass('tumblr')) {
					path = '/Article-Tool-Share-Tumblr.html';
					title = 'Article-Tool-Share-Tumblr';
				} else if ($elem.hasClass('facebook')) {
					path = '/Article-Tool-Share-facebook.html';
					title = 'Article-Tool-Share-facebook';
				}
				if (path && title) {
					dcsMultiTrack('DCS.dcssip', 'www.nytimes.com', 'DCS.dcsuri', path, 'WT.ti', title, 'WT.z_dcsm ','1');
				}
			}
		});

	})();
</script>
