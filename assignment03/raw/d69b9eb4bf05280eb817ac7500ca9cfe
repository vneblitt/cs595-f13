<script type='text/javascript'>
var gigya_catID = 'News';
var gigya_streamID = 'ece-http://www.dallasnews.com/-1167417';
var gigya_streamTitle = 'Perot Foundation announces $1 million gift to Planned Parenthood of Greater Texas';
var gigya_linkback = 'http://www.dallasnews.com/news/metro/20130829-perot-foundation-announces-1-million-gift-to-planned-parenthood-of-greater-texas.ece';
var gigya_deviceType = 'desktop';
var gigya_width = '620';
var gigya_providers = 'facebook, twitter, google, linkedin, messenger, yahoo';
var gigya_media = 'http://www.dallasnews.com/incoming/20130829-0203plannedparenthood.jpg.ece/ALTERNATES/w100/0203plannedparenthood.jpg';
gigya_media = (gigya_media == '') ? 'http://www.dallasnews.com/skins/dmn/gfx/fblogo.jpg' : '';
var gigya_leadtext = 'The foundation is continuing its support fort the nonprofit at a time when many lawmakers and charities are maintaining distance.';
</script>
<script type='text/javascript'>
var act = new gigya.socialize.UserAction();
act.setTitle(gigya_streamTitle);
act.setDescription(gigya_leadtext);
act.setLinkBack(gigya_linkback);
act.setUserMessage('');
act.addActionLink("Follow Us", 'http://www.facebook.com/dallasmorningnews');
act.addMediaItem({ type: 'image', src: gigya_media, href: gigya_linkback });
var templates = {
loginCanvas_loggedIn: '<div class="gig-comments-photo-container gig-comments-commentBox-photo-container">'+
'$photoDiv' +
'</div>' +
'<span class="gig-comments-username gig-comments-commentBox-username">Hello! $username </span> ' +
'<span style="padding-left: 5px; display: inline-block">$logoutLink</span>' +
'<div class="gig-comments-headerText gig-comments-commentBox-headerText" style="">Post your comments in the box below</div>',
loginCanvas_loggedOut: '<div class="gig-comments-photo-container gig-comments-commentBox-photo-container">'+
'$photoDiv' +
'</div>' +
' <div class="gig-comments-headerText gig-comments-commentBox-headerText">Login to post a comment</div>' +
'$loginDropdown '
}
var params ={
categoryID: gigya_catID, // category created in the gigya admin
streamID: gigya_streamID, // id of the article. ece or blog prefix
containerID: 'commentsDiv', //div id to load the content into
// Optional
streamTitle: gigya_streamTitle,
deviceType: gigya_deviceType, // desktop (default), mobile, or auto
width: gigya_width,
minShareOptions: 5, //The number of share providers to display as check-boxes in the "Share to" section of the comments UI. The default value is 2.
templates: templates,
userAction: act,
replyUserAction: act,
enabledProviders: gigya_providers,
onCommentSubmitted:function() {
FBtrackEvent(23,'StreamID:'+this.streamID,32,"Social:Comment:Add")
},
//Activity feed Parameters
scope:'internal',
privacy: 'public'
}
gigya.socialize.showCommentsUI(params);
</script>
