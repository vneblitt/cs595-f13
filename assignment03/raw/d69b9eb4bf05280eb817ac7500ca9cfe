<script type="text/javascript">
if (belo.cgm.util.isLoggedInForBelo() && !belo.cgm.util.isSubscribedToBelo())
{
document.getElementById("regSubscribeAction").style.display = "block";
}
else
{
document.getElementById("regLoginAction").style.display = "block";
}
function goTo(url,param)
{
var href = url + "?fw=http://" + document.location.hostname + document.location.pathname;
if (typeof(param) != 'undefined')
{
href += "&" + param;
}
document.location.href = href;
}
function setFW(myForm)
{
myForm.fw.value = "http://" + document.location.hostname + document.location.pathname;
return true;
}
function setFWAction(myForm)
{
myForm.fw.value = "http://" + document.location.hostname + document.location.pathname;
myForm.action = window.regServer + '/registration/postsignin.do';
return true;
}
APTsetCookie('AHCNonSub','1',365);
$(document).ready(function(){bindSubscribe();});
function bindSubscribe()
{
s.eVar18=s.prop24 = 'not logged in';
if (belo.cgm.util.isLoggedInForBelo()) s.eVar18=s.prop24 = 'logged in';
s.eVar12=s.prop29 = omSiteSection;
s.eVar30=s.prop30 = omSubSection1;
s.eVar31=s.prop31 = omSubSection2;
s.eVar32=s.prop32 = omSubSection3;
s.eVar34=s.prop34 = omSectionContentType + ':paywall';
s.eVar35=s.prop35 = omArticleTitle;
s.eVar36=s.prop36 = omArticleId;
s.eVar37=s.prop37 = 'registration';
s.eVar5=s.prop39 = omPageName;
s.eVar6=s.prop38 = 'not subscribed';
$("#regNonPremiumRegister").bind("click", function(e){
s.linkTrackVars='prop24,prop29,prop30,prop31,prop32,prop34,prop35,prop36,prop37,prop38,prop39,eVar5,eVar6,eVar18,eVar37';
s.linkTrackEvents='event12,event36';
s.events='event12,event36';
s.tl(this,'o','non subscriber sign-up');
});
$("#regPremiumRegister").bind("click", function(e){
s.linkTrackVars='prop24,prop29,prop30,prop31,prop32,prop34,prop35,prop36,prop37,prop38,prop39,eVar5,eVar6,eVar18,eVar37';
s.linkTrackEvents='event12,event36';
s.events='event12,event36';
s.tl(this,'o','subscriber sign-up');
});
$("#regPremiumSignIn").bind("click", function(e){
s.linkTrackVars='prop24,prop29,prop30,prop31,prop32,prop34,prop35,prop36,prop37,prop38,prop39,eVar5,eVar6,eVar18,eVar30,eVar31,eVar32,eVar34,eVar35,eVar36,eVar37';
