<script type="text/javascript">
function changeCol(obj) {
var objname=obj.name;
obj.value='';
obj.style.color = '#000000';
}
function doSubmit(frm) {
if (frm.when.value == 'null') {
if (frm.pricerange.value != 'null') {
window.open('http://listings.dallasnews.com/search?swhat='+frm.what.value+'&swhere='+frm.where.value+'&srad='+frm.rad.value+'&srss=10&st='+frm.name+'&search=true&svt=text&price='+frm.pricerange.value);
} else {
window.open('http://listings.dallasnews.com/search?swhat='+frm.what.value+'&swhere='+frm.where.value+'&srad='+frm.rad.value+'&srss=10&st='+frm.name+'&search=true&svt=text');
}
} else {
window.open('http://listings.dallasnews.com/search?swhat='+frm.what.value+'&swhen='+frm.when.value+'&swhere='+frm.where.value+'&srad='+frm.rad.value+'&srss=10&st='+frm.name+'&search=true&svt=text');
}
}
function hiLiteLiveBtn(btnid) {
document.getElementById(btnid).className="searchnavhover";
document.getElementById(btnid+'form').style.display="block";
if (btnid != 'events') {document.getElementById('events').className="searchnav"; document.getElementById('eventsform').style.display="none";}
if (btnid != 'restaurants') {document.getElementById('restaurants').className="searchnav"; document.getElementById('restaurantsform').style.display="none";}
if (btnid != 'movies') {document.getElementById('movies').className="searchnav"; document.getElementById('moviesform').style.display="none";}
if (btnid != 'venues') {document.getElementById('venues').className="searchnav"; document.getElementById('venuesform').style.display="none";}
}
</script>
