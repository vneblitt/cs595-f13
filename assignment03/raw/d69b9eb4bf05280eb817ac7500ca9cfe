<script type="text/javascript">
var thisLocation = ('' != '') ? encodeURIComponent('') : 'dfw';
$.ajax({
url: 'http://res.dallasnews.com/weather/api/conditions/forecast/astronomy/q/' + thisLocation + '.json',
cache: true,
dataType: "jsonp",
jsonpCallback: "jsonCallBackDallasWeatherConditionsMasthead",
success: function(weatherData) {
// Current conditions.
var sun = [parseInt(weatherData.moon_phase.sunrise.hour+weatherData.moon_phase.sunrise.minute),
parseInt(weatherData.moon_phase.sunset.hour + weatherData.moon_phase.sunset.minute)],
thisHour = parseInt(weatherData.moon_phase.current_time.hour+weatherData.moon_phase.current_time.minute);
$('.weather-masthead .DMNConditions .wu-temp-wrapper .wu-temp .wu-temp-link').html((Math.round(weatherData.current_observation.temp_f) > 100) ? Math.round(weatherData.current_observation.temp_f) : Math.round(weatherData.current_observation.temp_f) + '&deg;');
$('.weather-masthead .DMNConditions .wu-icon-wrapper .wu-icon .wu-icon-link').html('<img height="50" src="/skins/dmn/gfx/weather/' + (thisHour >= sun[0] && thisHour < sun[1] ? '' : 'nt_') + weatherData.current_observation.icon + '_big.png"/>');
},
error: function() { $('.weather-masthead').html('Sorry, currently weather forecasting for the city/location {0} is not available.'.replace('xxx',decodeURIComponent(thisLocation))); }
});
</script>
