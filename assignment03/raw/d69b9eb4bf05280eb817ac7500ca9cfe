<script type="text/javascript">
$(window).load(function() {
var texSizeHTML = texSizeHTML = '<span class="textToolsWrapper" style="position:absolute; top: 0px;">';
texSizeHTML += '<span class="textToolsLargeText" style="font-size:22px;">A</span>';
texSizeHTML += '<span class="textToolsText" style="font-size: 15px; padding-right: 5px; font-weight: normal;">A</span>';
texSizeHTML += '<span class="textToolsIcons" style="padding-right: 3px;"><img src="/skins/dmn/gfx/size-up.png" id="large" class="changeTextSize" /></span>';
texSizeHTML += '<span class="textToolsIcons"><img src="/skins/dmn/gfx/size-down.png" id="small" class="changeTextSize" /></span>';
texSizeHTML += '</span>';;
$('#shareBarDiv .gig-share-bar-container > table > tbody > tr').append(
$("<td></td>", {'style':'vertical-align:bottom; position:relative; white-space:nowrap;zoom:1; padding-right: 5px;'}).html(texSizeHTML)
);
$('.textToolsIcons .changeTextSize').click(function(){
var ourText = $('p');
var currFontSize = ourText.css('fontSize');
var finalNum = parseFloat(currFontSize, 10);
var stringEnding = currFontSize.slice(-2);
if(this.id == 'large') {
finalNum *= 1.2;
}
else if (this.id == 'small'){
finalNum /=1.2;
}
ourText.css('fontSize', finalNum + stringEnding);
});
});
</script>
