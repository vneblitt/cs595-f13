<script type="text/javascript">
    Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler1);

    function UpdateCuratorialInfo() {
        var hiddenCuratorialInfo = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_CuratorialInfo').value;
        var curatorialInfo = $get('curatorialInfo');
        if(curatorialInfo)
            $get('curatorialInfo').innerHTML = hiddenCuratorialInfo;
    }

    function UpdateDescriptionInfo() {
        var hiddenDescriptionInfo = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_DescriptionInfo').value;
        var descriptionInfo = $get('descriptionInfo');

        if (descriptionInfo)
            descriptionInfo.innerHTML = hiddenDescriptionInfo;
    }

    function UpdateDisplayStatus() {

        var descriptionInfo = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_DisplayStatusInfo').value;
        var elExhibitStatus = $get('ExhibitObject_DisplayStatus');
        var elAssetStatus = $get('Asset_DisplayStatus');
        var override = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_OverrideDisplayStatusInfo').value.toLowerCase();
        
        if(!elExhibitStatus)
        {
            return;
        }
        
        if(override == 'false')
        {
            elExhibitStatus.style.display = 'block';
            elAssetStatus.style.display = 'none';
            return;
        }
        
        //if (descriptionInfo != '')
        {
            elAssetStatus.innerHTML = descriptionInfo;
            elExhibitStatus.style.display = 'none';
            if(descriptionInfo == '')
                elAssetStatus.style.display = 'none';
            else
                elAssetStatus.style.display = 'block';
        }
    }

    function UpdateHighResLink() {
        var hiddenHighResImageUrl = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_HighResImageUrl').value;
        var HighResUrl = $get('HighResUrl');

        if (HighResUrl)
            HighResUrl.href = hiddenHighResImageUrl;
    }

    function EndRequestHandler1(sender, args) {
        if (args.get_error() != null) {
            var errorName = args.get_error().name;
            if (errorName.length > 0) {
                args.set_errorHandled(true);
            }
        }
        else {
            UpdateCuratorialInfo();
            UpdateDescriptionInfo();
            UpdateDisplayStatus();
            UpdateHighResLink();
            UpdateTranscriptWrapperFun();
        }
    }

    function UpdateTranscriptWrapperFun() {
        var currentMediaAssetHidden = $get('M_M_PlaceHolderPageContentBase_PlaceHolderPageContentIntermediate_PlaceHolderPageContent_g_FA4EB508_4176_46a8_BDCC_8C1A8309A42H_ctl00_CurrentMediaID').value;
        Sys.WebForms.PageRequestManager.getInstance().remove_endRequest(EndRequestHandler1);
        UpdateTranscript(currentMediaAssetHidden);
    }
    
</script>

<script type="text/javascript">
    var initialValue;

    function HandleClientLoaded(sender, eventArgs) {
        initialValue = sender.get_minimumValue();
        HandleClientValueChange(sender, null);
    }

    function HandleClientValueChange(sender, eventArgs) {
        //alert(sender.get_slideStep());
        var wrapperDiv = document.getElementById('boxes_wrapper');
        var contentDiv = document.getElementById('boxes_content');

        var oldValue = (eventArgs) ? eventArgs.get_oldValue() : sender.get_minimumValue();
        var change = sender.get_value() - oldValue;

        var contentDivWidth = contentDiv.scrollWidth - wrapperDiv.offsetWidth;
        var calculatedChangeStep = contentDivWidth / ((sender.get_maximumValue() - sender.get_minimumValue()) / sender.get_slideStep());

        initialValue = initialValue - change * calculatedChangeStep;

        if (sender.get_value() == sender.get_minimumValue()) {
            contentDiv.style.left = 0 + 'px';
            initialValue = sender.get_minimumValue();
        }
        else {
            contentDiv.style.left = initialValue + 'px';
        }
    }

    function AddOnResponseEndEvent() {
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler1);
    }
</script>


          
        
        
          

