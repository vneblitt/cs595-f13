<script type="text/javascript">
jQuery('#poll-1566-b27ab73abb6683d24af2319fed2555cb > .poll-results .note-toggle').click(function(evt) {
    evt.preventDefault();
	var container = $('#poll-1566-b27ab73abb6683d24af2319fed2555cb');
