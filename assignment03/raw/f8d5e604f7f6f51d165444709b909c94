<script>
	// check the comment form for errors before submitting
	function submitComment() {
		// assume no errors
		errorList = "";
		jQuery('span.error,div.box-error').remove();		
		// function to trim spaces at the beginning and ending
		function trim(s){return s.replace(/^\s\s*/, '').replace(/\s\s*$/, '');};
		// function to check format of email address
		function validateEmailAddressFormat(email) { 
			var regExpForEmail1 = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
			return regExpForEmail1.test(jQuery.trim(email));
		}
		
		// input: author - do field validation only if field is defined
		if(jQuery('#author').length) { 
			if ( trim(document.forms["commentform"].elements[ 'author' ].value) == "" ) {
				errorList += "<li>Please enter your name.</li>";
				jQuery('#author').before('<span class="error">Please enter your name.<br /></span>');
			}
		} 
		// input: email - do field validation only if field is defined		
		if(jQuery('#email').length) {
			f_email = trim(document.forms["commentform"].elements[ 'email' ].value);
			if (  f_email == "" ) {
				errorList += "<li>Please enter your email address.</li>";
				jQuery('#email').before('<span class="error">Please enter your email address.<br /></span>')
			} else if ( !validateEmailAddressFormat(f_email) ) {
				errorList += "<li>Please enter a valid email address.</li>";
				jQuery('#email').before('<span class="error">Please enter a valid email address.<br /></span>');	
			}	
		} 
		// input: comment - do field validation only if field is defined		
		if(jQuery('#comment').length) {
			if ( trim(document.forms["commentform"].elements[ 'comment' ].value) == "" ) {
				errorList += "<li>Please enter your comment.</li>";
				jQuery('#comment').before('<span class="error">Please enter your comment.<br /></span>');
			}	
		} 
		
		// render the error box or submit the form 
		if ( errorList != "" ) {
			jQuery('#form_field_header').before('<div class="box-error"><h3>Error</h3><ul>' + errorList + '</ul></span></div>');				
		} else {
			document.forms["commentform"].submit();
		}
		
		return true;
	}

	function clearComment() {
		jQuery('span.error,div.box-error').remove();	
		return true;
	}

/*  jQuery(document).ready(function() {  alert ('we are ready');  });   */
</script>


