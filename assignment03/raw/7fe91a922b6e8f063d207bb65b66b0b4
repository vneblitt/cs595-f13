<script type="text/javascript">
$(document).ready(function(){
	$('#clickHereMonthly').show();
	$('#clickHereGross').show();

	$('#clickHereMonthly').click(function() {
	  $('#monthlyPaymentWorksheet').toggle('fast', function() {
			if ($(this).is(':visible')) {
				$('.clsCalcPrimary').focus();
			} else {
				$('.clsPayment').focus();
			}
		});
	  return false;
	});

	$('#clickHereGross').click(function() {
	  $('#grossIncomeWorksheet').toggle('fast', function() {
			if ($(this).is(':visible')) {
				$('.clsNet').focus();
			} else {
				$('.clsIncome').focus();
			}
		});
	  return false;
	});
	
	

	

});
</script>

