<script> 


function my_btnGoSearch_Click() {
	var strQuery = $("#ctl00_PlaceHolderSearchArea_ctl00_txtSearch").val();
	var searchHost = window.location;
	if (/makinghomeaffordable/ig.test(searchHost)){
		window.location = "http://search.treasury.gov/search?affiliate=mha&commit=Search&query=" + strQuery;
	} else {
		window.location = "http://search.treasury.gov/search?affiliate=treasury&commit=Search&query=" + strQuery; 
	}
} 
</script> 
										
