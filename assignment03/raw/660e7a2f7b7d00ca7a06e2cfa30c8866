							if ((skipMobileNote == null)
								&&(mobileNoteDisplayed == null) 
								&& (sUrlParams.indexOf("tnnomobile") == -1)){
								// create session cookie to prevent mobile notice
								$.cookie("diplayedMobileNote", 1);
								// write a message to recommend mobile        
								$(document).ready(function(){
									$("#blox-html-container").prepend('<div id="show-mobile-notice" class="ui-state-highlight" title="Switch to the mobile view"><p>This site is optimized for mobile devices with no download required.</p></div>');
									$("#show-mobile-notice").dialog({
										autoOpen: true,
										modal: true,
										buttons: {
											"Go to mobile view": function() {
												$.cookie("NoAdvMobile", null);
												window.location = 'true';
												$(this).dialog("close");
											},
											"Don't show this message": function() {
												// create long term cookie to disable mobile notice
												$.cookie("noMobileNote", 1, {expires: 30});
												$(this).dialog("close");
											}
										}
									});
