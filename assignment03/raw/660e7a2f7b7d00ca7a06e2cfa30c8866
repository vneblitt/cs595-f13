<script type="text/javascript">
	$( document ).ready(function() {
		$('.rtd_breaking_news .blox-title h3 a').each(function(){
			var boldMe = jQuery(this);
			boldMe.html( boldMe.text().replace(/(^\w+|\s+\w+)/,'<strong>$1</strong>') );
		});
		
		
		$('.blox-title h3 a').each(function(){
			var shrinkMe = jQuery(this);
			shrinkMe.html( shrinkMe.text().replace(">",'<span>></span>') );
		});
	});
</script>
    			
