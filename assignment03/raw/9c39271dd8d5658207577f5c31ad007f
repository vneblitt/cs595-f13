	<script type="text/cjs">
		//start: gog-left-rail-main.js
$(".hover-sensitive").hover(
	function() {$(this).addClass("hover");},
	function(){$(this).removeClass("hover");}
);	

$(".profile.v2 .height-constrain").each(function(){
	$(this).attr("max-height",$(this).css("max-height"));
	if ($(this)[0].scrollHeight<=$(this).height()){
		$(this).find(".gradient-hide-bar").hide();
	} else {
		$($(this).attr("expander")).css("display","block");
	}
});

$(".profile.v2 #seeMore-hours.see-more:visible").bind("click",function(event){
	var hours_div=$(this).attr("associate-dom");
	var status=$(this).attr("status");
	if ($(this).attr("status")=="collapse"){
		$(this).find(".show-hide-button").removeClass("down");
		$(this).find(".show-hide-button").addClass("up");
		$(this).attr("status","expand");
		$(this).find(".label:visible").text($(this).find(".label:visible").attr("collapse-label"));
		$(hours_div).css("max-height","none");
		$(hours_div).find(".gradient-hide-bar").hide();
	} else {
		$(this).find(".show-hide-button").removeClass("up");
		$(this).find(".show-hide-button").addClass("down");
		$(this).attr("status","collapse");
		$(this).find(".label:visible").text($(this).find(".label:visible").attr("expand-label"));
		$(hours_div).find(".gradient-hide-bar").show();
		$(hours_div).css("max-height",$(hours_div).attr("max-height"));
	}
	return false;
});	

/*omniture tracking implementation*/

//for all anchor links
$(".gog.left-rail a[omniture-tracking*='left rail']").on("click",function(){
	logEvent("gog",$(this).attr("omniture-tracking"));
});


function logEvent(project, name) {
	if (name != undefined){
		var prop = project + " - " + name;
		if (sendEventToOmniture) {
			try {
				sendEventToOmniture(prop, null, {prop19: prop});
//				if (console!=undefined &&console!=null)
//					console.log("omniture tracking value: prop="+prop);
			} catch (e) {
				if (console!=undefined &&console!=null)
					console.error("omniture tracking error: prop="+prop+", error msg:"+e);
			}
		}
	}
}


//end: gog-left-rail-main.js
	</script>




                                                                                                                                  </div>
	









