<script src="/static-dev/benefits/common/js/jquery.flexslider-min.js"></script>

<script>
$(document).ready(function() {

	/* get slider html */
	var fullSlider = $(".slider").html(); 
	
	/* initial load */
	if((window.innerWidth || document.documentElement.clientWidth)>767){
		createFlexslider();
	} else {
		destroyFlexslider();
	}
	
	/* on resize */
	$(window).resize(function(){
		if((window.innerWidth || document.documentElement.clientWidth)>767) {
			createFlexslider();
		} else {
			destroyFlexslider();
		}
	});
	
	function createFlexslider() {
		$('.flexslider').flexslider({
		    animation: "slide",
		    useCSS: true
		  });
	}

	function destroyFlexslider() {
		$(".flexslider").remove(); 
		$(".slider").html(fullSlider);
	}
});
 </script></div> </div> <!-- /container -->
