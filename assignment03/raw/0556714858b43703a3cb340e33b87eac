		  //open close
		  function wds_open_close_embed(level,action){
		  	if(action=="open"){
				document.getElementById('wds_open_close_window_'+level).style.display='';
				document.getElementById('wds_open_close_'+level).innerHTML='<a href="javascript:wds_open_close_embed(\''+level+'\',\'close\');">Embed Code<img src="/wp-content/themes/badgestack/images/hide-details.png" class="show-hide"></a>';
			}else{
				document.getElementById('wds_open_close_window_'+level).style.display='none';
				document.getElementById('wds_open_close_'+level).innerHTML='<a href="javascript:wds_open_close_embed(\''+level+'\',\'open\');">Embed Code<img src="/wp-content/themes/badgestack/images/show-details.png" class="show-hide"></a>';
			}
