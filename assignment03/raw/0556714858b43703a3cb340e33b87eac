	  	  <script type="text/javascript">
	  //<![CDATA[
		  function wds_validate_journey(){
			if(document.journey_submission_form.journey_title.value == "") {
				alert("You must include a title in order to submit your post");
				document.journey_submission_form.journey_title.focus();
				return false;
			}
			if(document.journey_submission_form.game_submission_text.value == "") {
				alert("You must include a description in order to submit your post");
				document.journey_submission_form.game_submission_text.focus();
				return false;
			}
			return true;

		  }
		  
		  //]]>
	  </script>
