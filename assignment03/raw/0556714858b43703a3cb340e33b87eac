	  	<script type="text/javascript">
	//<![CDATA[
		function wds_bs_capabilities_select_change(current_group,post_id,cap_key,selected_value){
			var isack = new sack("http://smithsonianeducationconferences.org/wp-admin/admin-ajax.php");			isack.execute = 1;
			isack.method = 'POST';
			isack.setVar( "action", "wds_bs_capabilities_select_change" );
			isack.setVar( "current_group", current_group );
			isack.setVar( "post_id", post_id );
			isack.setVar( "cap_key", cap_key );
			isack.setVar( "selected_value", selected_value );
			isack.runAJAX();
			return true;
		}
		
		function wds_bs_caps_posts(post_id,current_group,cap_key,selected_value,cap_post_id){
			var isack = new sack("http://smithsonianeducationconferences.org/wp-admin/admin-ajax.php");			on_off=document.getElementById('cap_selected_'+cap_key+'_'+selected_value+'_'+cap_post_id).value;
			if(on_off=='on'){
				document.getElementById('cap_'+cap_key+'_'+selected_value+'_'+cap_post_id).className='each_student selected';
				document.getElementById('cap_selected_'+cap_key+'_'+selected_value+'_'+cap_post_id).value='off';
			}else{
				document.getElementById('cap_selected_'+cap_key+'_'+selected_value+'_'+cap_post_id).value='on';
				document.getElementById('cap_'+cap_key+'_'+selected_value+'_'+cap_post_id).className='each_student';
			}
			isack.execute = 1;
			isack.method = 'POST';
			isack.setVar( "action", "wds_bs_caps_posts" );
			isack.setVar( "post_id", post_id );
			isack.setVar( "current_group", current_group );
			isack.setVar( "cap_key", cap_key );
			isack.setVar( "selected_value", selected_value );
			isack.setVar( "cap_post_id", cap_post_id );
			isack.setVar( "on", on_off );
			//alert(on_off);
			isack.runAJAX();
			return true;
		}
		
		//]]>
	</script>
