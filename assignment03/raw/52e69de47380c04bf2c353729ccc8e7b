	<script type="text/javascript">  
	
	$(document).ready(function()
	{ 
			
	var regWallLimit = 999;
	var thisStoryCID = 22583309;
	var cidListFound = 0;
	
	console.log('reg limit: ' + regWallLimit); 
	
	//alert(thisStoryCID);
	if( $.cookie('gigyaLoggedIn') == null ) 		
		{
		if( $.cookie('gigyaCookieCntr') == null ) 
			{
			var now=new Date();
			var expiresDate=new Date();
			expiresDate.setDate(now.getDate() + 14);
			$.cookie('gigyaCookieCntr', 1,  { expires: expiresDate, path: '/', domain: '.suntimes.com' });
			$.cookie('gigyaCookieCID', 22583309,  { expires: expiresDate, path: '/', domain: '.suntimes.com' });
			//alert('You have set the cookie: '+ $.cookie('gigyaCookieCntr'));
			//alert('You have set the cookie: '+ $.cookie('gigyaCookieCID'));
			}	
		else
			{	
			var now=new Date();
			var expiresDate=new Date();
			expiresDate.setDate(now.getDate() + 14);
	
			gigyaCookieCIDValue = $.cookie('gigyaCookieCID');
			if (cidListFound == 0) 
			{
			gigyaCookieCIDValue = gigyaCookieCIDValue + "," + thisStoryCID;
			$.cookie('gigyaCookieCID', gigyaCookieCIDValue,  { expires: expiresDate, path: '/', domain: '.suntimes.com' });
			gigyaCookieCntrValue = $.cookie('gigyaCookieCntr');
			gigyaCookieCntrValue = parseInt(gigyaCookieCntrValue) +1;
			$.cookie('gigyaCookieCntr', gigyaCookieCntrValue,  { expires: expiresDate, path: '/', domain: '.suntimes.com' });
			}
			
			//alert('You have added to the cookie: '+ $.cookie('gigyaCookieCntr'));
			//alert('You have added to the cookie: '+ $.cookie('gigyaCookieCID'));
			  
			}	   
			
			
			
		}	
	});
	
	</script>
	
		

	




 
 


		
