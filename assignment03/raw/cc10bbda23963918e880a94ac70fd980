                // use the smaller size for phone, the larger size for tablet
                var sizes = '640x100';
                if (Modernizr.mq('only screen and (min-width: 768px)')) {
                    sizes = '2048x180';
                }
                
                var targets = ';';
                targets += 'agg=750003;';
                targets += 'theme=750003;';
                targets += 'storyId=222101281;';
                
                document.write('<scr' + 'ipt type="text/javascript" src="http://ad.doubleclick.net/N6735/adj/n6735.nprmobile/no_topic' + targets + 'sz=' + sizes + ';ord=' + ord + '?"><\/script>');
              }
            </script>
    </div>
</div> <!-- END CommonAdhesionCall.mtpl -->
</div>

<!-- END ID="ADHESION" -->
