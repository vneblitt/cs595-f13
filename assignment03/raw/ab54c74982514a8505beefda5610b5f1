<script>


function submitform()
{
		document.actionform.action="http://www.education-transforms.org/en/";
 
 
 document.forms["actionform"].submit();
}




function validateForm()
{
	
	
	if (document.actionform.FirstName.value == "" || document.actionform.LastName.value == "" || document.actionform.Country.value == "" || document.actionform.Email.value == "" ) {
		
		alert("Please fill in all the required fields");
		
	} else {
		
		submitform();
		
	}



}

		
function enableButton()
{
	
	
	if (document.actionform.FirstName.value == "" || document.actionform.LastName.value == "" || document.actionform.Country.value == "" || document.actionform.Email.value == "" ) {
		document.getElementById("submit").style.display = 'none';
		document.getElementById("form-submit-disabled").style.display = 'block';
	} else {
		document.getElementById("form-submit-disabled").style.display = 'none';
		document.getElementById("submit").style.display = 'block';
	}



}



</script>


                
                
                
			</div>	
	</section>
	
                  
                    
