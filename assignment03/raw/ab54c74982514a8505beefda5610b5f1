			var $j = jQuery.noConflict();

			
			$j(function(){
				$j(".thumbs-animate").hover(function(){
					$j("img", this).fadeOut("fast");
				}, function() {
					$j("img", this).fadeIn("fast");
				});







				$j(".thumbs-animate").hover(function(){
					$j("iframe", this).stop().fadeOut({queue:false,duration:400});
				}, function() {
					$j("iframe", this).stop().fadeIn({queue:false,duration:400});
				});

				$j(".thumbs-team-animate").hover(function(){
					$j("img.wp-post-image", this).stop().fadeOut({queue:false,duration:400});
				}, function() {
					$j("img.wp-post-image", this).stop().fadeIn({queue:false,duration:400});
				});

			});
