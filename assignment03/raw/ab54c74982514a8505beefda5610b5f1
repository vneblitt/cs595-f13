	var $j = jQuery.noConflict();
	jQuery(document).ready(function() {

		if ( jQuery().scrollTop ) {
			
			var $DropDownWrapper = $j( '#petition_wrapper' );
			
			if ( $DropDownWrapper.length ) { 
					
				$j('#petition_tab').click(function() {
					
					var petitionHeight = $j('.petition_page').height() + 172; 
					var wrapperHeight = $DropDownWrapper.height();	
						
					if( wrapperHeight == 0 ) {
						$j('.drop-down-arrows').css('backgroundPosition', 'left top');
						
						$DropDownWrapper.animate( { height: petitionHeight }, 500, 'easeOutCubic', function() {
						
							$j(this).css('height', 'auto');	
							
						});
					}
					else {
						$j('.drop-down-arrows').css('backgroundPosition', '0px -11px');
						
						$DropDownWrapper.animate( { height: 0 }, 400, 'easeOutCubic' );
					}
					
					$j( 'body, html' ).animate({ scrollTop: 0 }, 200, 'easeOutCubic' );
					
				});
				
				
			}
		
		}
					
				
				
			
					
				
