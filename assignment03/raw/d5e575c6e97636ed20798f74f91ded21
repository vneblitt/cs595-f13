define('tune-in', ['bridge/util/responsive/client','bridge/util/data/storage/storage'], function( client, storage ) {
var upsellClicked = storage.get('upsell-clicked');
function loadInterstitial(){
require(['/sections/rhome/packages/mobile-interstitial/mobile-interstitial.js'], function(postSeasonInterstitial) {
postSeasonInterstitial.mobileInterstitial({ name:'postseason-tune-in', src: '/mlb/images/ps/y2013/ps_tunein@2x.jpg' });
});
}
if (client.isIPhone && upsellClicked.already) {
loadInterstitial();
} else {
$('#upsell a').on('touchstart click', function() {
loadInterstitial();
});
}
});
require(['tune-in']);
});
});
});
