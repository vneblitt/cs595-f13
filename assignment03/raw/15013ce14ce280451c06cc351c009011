<script>

function increaseVideoSize() {
    var columnWidth = 940; // width of your content column - any
    var defaultVideoWidth = 700; // theme tag width - 400,500
    var increaseRatio = columnWidth/defaultVideoWidth;
    $(".video-post").each(function() {
        var iframe = $("iframe", this);
        if(iframe.length>0){
            if(iframe.attr('src').indexOf("instagram.com") == -1){ 
                var currHeight = iframe.height();
                var newHeight = currHeight * increaseRatio;
                iframe.height(newHeight).width(columnWidth);
            }else{
                iframe.css("marginLeft","160px")
                iframe.height("730")
            }
        } else {
            var object = $("object", this);
            var embed = $("embed", object);
            var currHeight = object.attr('height');
            var newHeight = currHeight * increaseRatio;
            object.width(columnWidth).attr('height', newHeight);
            embed.width(columnWidth).attr('height', newHeight);
        };
    });
};

function addImageClass(){
    $(".post-body p:has(img)").addClass("image-p");
}

function moveStuff(){
    // move products
    if ($(".product").is('*') && $('#posts ul').length > 5) {
        $('#posts li.post:last').after('<li class="post post_products"><div class="post-body"><h3>Support an awesome artist. Buy a Threadless tee.</h3></div><ul class="clearfix"></ul><div class="post-separator"></div></li>');
        $('.product').each(function(){
           $(this).remove();
           $(".post_products ul").append('<li class="post_product"><a href="'+$(this).find(".photo-data li:nth-child(1)").html()+'"><img src="'+$(this).find(".photo-data li:nth-child(3)").html()+'"/></a><div class="note_count"><a href="'+$(this).find(".photo-data li:nth-child(1)").html()+'">'+$(this).find(".photo-data li:nth-child(2)").html()+'</a></div></li>');
        });
        
    }
    
    // move submissions
    if ($(".submission").is('*') && $('#posts ul').length > 5) {
        $('#posts li.post:last').after('<li class="post post_submissions"><div class="post-body"><h3>Which designs do you want to see printed?</h3></div><ul class="clearfix"></ul><div class="post-separator"></div></li>');
        $('.post_submissions ul').append($('.submission'));
        $('.post_submissions ul li .date-permalink li i').removeClass('icon-li')
        $('#posts li.post:nth-child(3)').after($('.post_submissions'));
    }
}

increaseVideoSize();
addImageClass();
moveStuff();
</script>

