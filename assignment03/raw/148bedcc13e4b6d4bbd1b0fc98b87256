<script type="text/javascript">
	var GTVrotatorInterval;
	var GTVTopNewsCurrentIndex = 0;
	var GTVTopNewsItemTimeout;
	var GTVTopNewsItems = new Array();
	var GTVTopNewsCurrentID;
	var GTVVideoPlaying = false;
	jQuery.noConflict(); 

	function AddTopNews(id) {
		GTVTopNewsItems.push(id);
	}

	function GTVTopNewsInitialize() {
		document.getElementById("GTV-TopNewsHeadline-" + GTVTopNewsItems[0]).className = "highlight";
		GTVTopNewsCurrentID = GTVTopNewsItems[0];
		GTVrotatorInterval =  setInterval("GTVTopNewsRotation()", 7000);
	}

	function GTVTopNewsRotation() {
	  if (GTVTopNewsCurrentIndex < GTVTopNewsItems.length - 1) {
		GTVTopNewsRotateItem(GTVTopNewsItems[GTVTopNewsCurrentIndex + 1]);
		GTVTopNewsCurrentIndex = GTVTopNewsCurrentIndex + 1;
	  } else {
		GTVTopNewsRotateItem(GTVTopNewsItems[0]);
		GTVTopNewsCurrentIndex = 0;
	  }
	}

	function GTVTopNewsStopRotation() {
		if (GTVrotatorInterval != null) {
			clearInterval(GTVrotatorInterval);
		}
	}

	Array.prototype.findIndex = function (value) {
		var ctr = "";
		for (var i = 0; i < this.length; i++) {
			if (this[i] == value) {
				return i;
			}
		}
		return ctr;
	};

	function GTVTopNewsStartRotation() {
		GTVrotatorInterval = setInterval("GTVTopNewsRotation()", 7000);
	}

	function GTVTopNewsLoadNews(id) {
		if (id != GTVTopNewsCurrentID) {
			document.getElementById("GTV-TopNews-" + GTVTopNewsCurrentID).style.display = "none";
			jQuery("#GTV-TopNews-" + id).fadeIn("slow");
			GTVTopNewsSetStyles(id);
			GTVTopNewsCurrentID = id;
			GTVTopNewsCurrentIndex = GTVTopNewsItems.findIndex(id);
		}
	}
	
	function GTVTopNewsHeadlineHighlight(id) {
		GTVTopNewsStopRotation();
		GTVTopNewsItemTimeout = setTimeout("GTVTopNewsLoadNews(" + id + ")", 500);
	}

	function GTVTopNewsItemOut() {
		if (GTVTopNewsItemTimeout != null) {
			window.clearTimeout(GTVTopNewsItemTimeout);
			if (GTVVideoPlaying != true) {
			   GTVTopNewsStartRotation();
			}
		}
	}

	function GTVTopNewsRotateItem(id) {
	  if (id != GTVTopNewsCurrentID) {
		document.getElementById("GTV-TopNews-" + GTVTopNewsCurrentID).style.display = "none";
		jQuery("#GTV-TopNews-" + id).fadeIn("slow");
		GTVTopNewsSetStyles(id);
		GTVTopNewsCurrentID = id;
	  }
	}

	function GTVTopNewsSetStyles(id) {
		document.getElementById("GTV-TopNewsHeadline-" + id).className = "highlight";
		document.getElementById("GTV-TopNewsHeadline-" + GTVTopNewsCurrentID).className = "";
	}   
	
	var BCSetting = {};
	BCSetting.overlayson = true;		
	BCSetting.publisherID = "34300055001";
	BCSetting.omnitureAccountID = "gntbcstwgrz,gntbcstglobal";
	BCSetting.marketName = "Buffalo, NY:wgrz";
	BCSetting.SSTS = "";
	BCSetting.immersiveBaseURL = "http://www.wgrz.com/video/default.aspx?bctid=";
	
		
	(function (){                
	  var pageInfo= GEL.namespace("thepage.pageinfo"),
	  size=[640,480]; 
	
	  GEL.thepage.initializer.addInitRoutine({
	   name:"loadVideo",
	   namespace:["widget.AdBanner"],
	   callback:loadVastUrl,
	   priority:205
	});

	function loadVastUrl(){
	   dfpBanner= new (GEL.thepage.bannerLocator.getProvider())({
	   position: 'Video_prestream', 
	   size: size.join("x")  
	}); 
															
	dfpBanner.setKeyvalues({companion: "no"}).getVideoUrl(function(u){ 
	BCSetting.adServerURL = u; 
	GEL.log("bc="+BCSetting.adServerURL);
	});
	}
	})();
	             
</script>
