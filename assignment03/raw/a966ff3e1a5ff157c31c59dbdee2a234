$(document).ready(function() {
$('.mainmenu').find('h2 a').removeAttr('tabIndex');

	// Slideshow
	$("#rslider").responsiveSlides({
		auto: false,
		pager: true,
        nav: true,
        speed: 500,
		manualControls: '.sliderPagination',
		speed: 300,
		after: function () {
			$('#rslider li').each(function(){
				if($(this).hasClass('rslides1_on')){
					$(this).attr('aria-live','polite')
				}else{
					$(this).attr('aria-live','off')
				}
			})
		}
	});

});
