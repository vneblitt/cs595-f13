        var body = Y.one(Y.config.win.document.body);

        if (Y.UA.touchEnabled) {
            body.removeClass('zss-no-touch');
            body.addClass('zss-touch');
        }
        var _trackGAEvent = function(e){
            var target = e.currentTarget,
                targetHref = null,
                 LEFT_BUTTON = 1,
                 MIDDLE_BUTTON = 2;

            //the target may not be the element with the tracking data on it (if you put your tacking class on a div which contains the anchor tag)
            target = (target.hasClass('track-ga-event')) ? target : target.ancestor('.track-ga-event');

            // Prevent the page from reloading too soon on standard links if data-ga-standard-href is set to true and
            // no data-ga-href attribute is provided.
            if ((target.getDataAttribute('gaStandardHref') === 'true') && !target.hasAttribute('data-ga-href')) {
                e.preventDefault();
                target.setDataAttribute('gaHref', target.getAttribute('href'));
            }

            // COM-3315: Ensure that lightbox links don't get redirected via href attribute in trackLink().
            // COM-3780: Ensure that login-required links don't get redirected as well
            if (!target.hasClass('show-lightbox') && !target.hasClass('login-required')) {

                // SHO-3273: Win7/IE9 : Clicking 'Save' button on HDP redirects to search and maps page
                // Added href check so we don't navigate on single hash anchor tags
                var tempHref = target.getAttribute('href');
                //if (tempHref && tempHref.length > 0 && tempHref.charAt(0) !== '#' && !target.getDataAttribute('gaHref')) {
                if (tempHref && tempHref.length > 0 && tempHref.charAt(0) !== '#') {
                    targetHref = target.getDataAttribute('gaHref') || target.get('href');
                }
            }
            var value = target.getDataAttribute('gaValue');
            if (value) {
                value = parseInt(value);
            }
            var gaJson = {
                category : target.getDataAttribute('gaCategory'),
                action : target.getDataAttribute('gaAction'),
                label : target.getDataAttribute('gaLabel'),
                noninteraction : target.getDataAttribute('gaNoninteraction'),
                value : value,
                href : targetHref
            };

            if (target.getDataAttribute('gaActionIncludeText')) {
                gaJson.action += ' - ' + Y.Lang.trim(target.get('text'));
            }

            // Handle targets other than _self
            if (e.currentTarget) {
                gaJson.target = e.currentTarget.get('target') || e.currentTarget.getDataAttribute('target');
            }
            // Support middle click and ctrl-click and prevent multiple windows when target is already _blank
            if (e.button === MIDDLE_BUTTON ||
                (e.button === LEFT_BUTTON && e.ctrlKey) ||
                (Y.UA.os === "macintosh" && event.metaKey) ||
                gaJson.target === '_blank') {
                    gaJson.target = '_blank';
                    e.preventDefault();
            }

            Y.Z.track(gaJson);
        };
        Y.delegate("click", _trackGAEvent, body, ".track-ga-event");
    

