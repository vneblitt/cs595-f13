		// Common JS for site:
		// Apparently people care:
		if (window.top != window.self) 
			window.top.location = window.self.document.location;
		
		function showLightbox(elm)
		{
			var parts = elm.alt.split(" || ");
			var imgNum = elm.src.match(/(\d+)\.jpg/i)[1];
			window.myLightbox = new mr.Lightbox();
			//window.myLightbox.Show(document.getElementById("enlargedphoto").innerHTML.replace(/images\/spacer\.gif/i, elm.src.replace(/w_[^,]+,h_[^,]+,c_fill,g_faces/i, "w_640,h_480,b_rgb:222222,c_pad").replace()).replace(/%name%/i, parts[0]).replace(/%caption%/i, parts[1]).replace(/%museum%/i, parts[2]).replace(/%year%/i, parts[3]).replace(/%state%/i, parts[4]).replace(/%fb_like%/i, '<div class="fb-like" data-href="http://www.americasfamilyalbum.org/?p=' + imgNum + '" data-send="false" data-width="250" data-show-faces="false" data-font="arial" data-colorscheme="dark"></div>'), 754, 683, {
			
			if (parts[1] != "")
				parts[1] = "\"" + parts[1] + "\"";
			if (parts[2] == "")
				parts[2] = "No museum";
			if (parts[3] == "")
				parts[3] = "No year";
			if (parts[4] == "")
				parts[4] = "No state";
			
			// NOTE: Twitter and Pinterest share set at end:
			var lbContent = document.getElementById("enlargedphoto").innerHTML.replace(/images\/spacer\.gif/i, elm.src.replace(/w_[^,]+,h_[^,]+,c_fill,g_faces/i, "w_640,h_480,b_rgb:222222,c_pad").replace()).replace(/%name%/i, parts[0]).replace(/%caption%/i, parts[1]).replace(/%museum%/i, parts[2]).replace(/%year%/i, parts[3]).replace(/%state%/i, parts[4]).replace(/%tw%/i, "http://twitter.com/intent/tweet?text=Check%20out%20this%20photo%20of%20me%20at%20the%20Smithsonian!%20Add%20your%20own%20photo%20and%20Ford%20Fund%20will%20donate%20%245%3A&url=" + encodeURIComponent("http://www.americasfamilyalbum.org/?p=" + imgNum + "&s_src=siod_cm063pefsitetw") + "&hashtags=smithsonianfamily").replace(/%pi%/i, "http://pinterest.com/pin/create/button/?url=" + encodeURIComponent("http://www.americasfamilyalbum.org/?p=" + imgNum + "&s_src=siod_cm063pefsitepin") + "&media=" + encodeURIComponent(elm.src) + "&description=" + encodeURIComponent("Have you ever visited the Smithsonian? Upload a photo from your visit--you'll be entered to win a 10-day cruise for two from Smithsonian Journeys and Ford Fund will donate $5 to the Smithsonian! http://www.americasfamilyalbum.org/?p=" + imgNum + "&s_src=siod_cm063pefsitepin"));
			
			// Have to use the iframe on mobile, otherwise Facebook will show a non-mobile-width-friendly comment box:
			if (mr.Browser.isMobile)
			{
				lbContent = lbContent.replace(/%fb_like%/i, '<iframe src="//www.facebook.com/plugins/like.php?href=' + encodeURIComponent('http://www.americasfamilyalbum.org/?p=' + imgNum + "&s_src=siod_cm063pefsitefb") + '&amp;send=false&amp;layout=standard&amp;width=250&amp;show_faces=false&amp;font=arial&amp;colorscheme=dark&amp;action=like&amp;height=35" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:250px; height:35px;" allowTransparency="true"></iframe>');
			}
			else
			{
				lbContent = lbContent.replace(/%fb_like%/i, '<div class="fb-like" data-href="http://www.americasfamilyalbum.org/?p=' + imgNum + '&s_src=siod_cm063pefsitefb" data-send="false" data-width="250" data-show-faces="false" data-font="arial" data-colorscheme="dark"></div>');
			}
			
			window.myLightbox.Show(lbContent, 754, 683, {
				backgroundColor: "#3f3f3f",
				backgroundClickCloses: true,
				mobileWidth: 320,
				mobileHeight: 500,				
				onClose: function(elm)
				{
					if (history.replaceState)
						history.replaceState({ }, document.title, "/");
						
					window.clearInterval(window.fbLikeInterval);
				}
			});
			
			// Change the browser URL to show the URL that would get you directly to this image:
			// https://developer.mozilla.org/en-US/docs/DOM/Manipulating_the_browser_history#Adding_and_modifying_history_entries
			if (history.replaceState)
				history.replaceState({ }, parts[1], "?p=" + imgNum);
			
			// And analytics:
			try
			{
				ga("send", "pageview", "/?p=" + imgNum);
			}
			catch (e) { }
			
			// And hookup the FB like button -- this works for both HTML5 (what we're using) and FBML:
			FB.XFBML.parse();
			
			// Now hookup the Pin, and Tweet tracking:
			window.sharepi = false;
			window.sharetw = false;
			window.sharefb = false;
			
			window.myLightbox.contentDiv.getElementsByClassName("enpi")[0].onclick = function()
			{
				if (window.sharepi == false)
				{
					document.getElementById("hiddenframe").src = "http://www.americasfamilyalbum.org/store-share.php?network=pin&p=" + imgNum;
					window.sharepi = true;
				}
				return true;
			}
			window.myLightbox.contentDiv.getElementsByClassName("entw")[0].onclick = function()
			{
				if (window.sharetw == false)
				{
					document.getElementById("hiddenframe").src = "http://www.americasfamilyalbum.org/store-share.php?network=tweet&p=" + imgNum;
					window.sharetw = true;
				}
				return true;
			}
			
			// And the FB Like tracking -- works on desktop only:
			if (!mr.Browser.isMobile)
			{
				window.fbLikeInterval = window.setInterval(function()
				{
					if (window.sharefb == false && window.myLightbox.contentDiv.getElementsByTagName("iframe")[0].offsetHeight > 50)
					{
						document.getElementById("hiddenframe").src = "http://www.americasfamilyalbum.org/store-share.php?network=fblike&p=" + imgNum;
						window.sharefb = true;
					}
				}, 500);
			}
			
		}

		// Animation image list (also appended to donation form links)
		window.imagesArray = [{"uid":"8486","first_name":"Toni","last_name":"O.","state":"International","museum":"Natural History Museum","year":"2013","caption":"I was trying to take a selfie of the mammoth next door and found these ears so had a photo taken"},{"uid":"8485","first_name":"Magda","last_name":"D.","state":"International","museum":"Smithsonian Gardens","year":"2013","caption":"smithsonian institution"},{"uid":"8484","first_name":"Hannah","last_name":"Z.","state":"Virginia","museum":"Hirshhorn Museum and Sculpture Garden","year":"2013","caption":"Dog by Peter Coffin"},{"uid":"8483","first_name":"Tammy","last_name":"G.","state":"West Virginia","museum":"National Zoo","year":"2013","caption":"Eating her cud"},{"uid":"8482","first_name":"Tammy","last_name":"G.","state":"West Virginia","museum":"National Zoo","year":"2013","caption":"Talking face to face"},{"uid":"8480","first_name":"Carmen","last_name":"G.","state":"Virginia","museum":"Smithsonian Castle","year":"2013","caption":"After earth matters tour"},{"uid":"8479","first_name":"Heidi","last_name":"K.","state":"Virginia","museum":"National Zoo","year":"2013","caption":"Zoo walk Me and Em"},{"uid":"8477","first_name":"Heidi","last_name":"K.","state":"Virginia","museum":"Natural History Museum","year":"2013","caption":"They don't taste like butter..."},{"uid":"8476","first_name":"Roxana","last_name":"A.","state":"Virginia","museum":"Ripley Center","year":"2012","caption":"Office of the Smithsonian Associates"},{"uid":"8475","first_name":"Roxana","last_name":"A.","state":"Virginia","museum":"Freer Gallery of Art","year":"2012","caption":"Grandfather visiting the exhibition for the first time..."},{"uid":"8474","first_name":"Roxana","last_name":"A.","state":"Virginia","museum":"Natural History Museum","year":"2012","caption":"The entrance"},{"uid":"8473","first_name":"Roxana","last_name":"A.","state":"Virginia","museum":"Natural History Museum","year":"2012","caption":"Elephant"},{"uid":"8472","first_name":"Ro