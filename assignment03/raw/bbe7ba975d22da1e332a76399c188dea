		// Animation setup -- only useful on pages with the photoslider, but doesn't affect others
		function setupAnimation()
		{
			window.animationSpeed = 1300;
			window.animationDelay = 2000;
			window.currentImage = 4;  // We start with 4 visible, and a 5th hidden, and count is zero based
			window.cloudinaryBase = "http://res.cloudinary.com/americas-family-album/image/upload/a_exif,w_221,h_221,c_fill,g_faces,q_70/";
			
			window.photoslider = document.getElementById("photoslider");
			
			for (var i = 0; i < 5; i++)
			{
				var initImg = document.createElement("img");
				initImg.src = window.cloudinaryBase + window.imagesArray[i]["uid"] + ".jpg";
				initImg.width = "221";
				initImg.height = "221";
				initImg.alt = window.imagesArray[i]["first_name"].replace(/\"/, "''") + " " + window.imagesArray[i]["last_name"].replace(/\"/, "''") + " || " + window.imagesArray[i]["caption"].replace(/\"/, "''") + " || " + window.imagesArray[i]["museum"] + " || " + window.imagesArray[i]["year"] + " || " + window.imagesArray[i]["state"];
				initImg.onclick = function()
				{
					showLightbox(this);
				}
				window.photoslider.appendChild(initImg);
			}
			
			window.setTimeout(startAnimation, window.animationDelay * 3);
		}

		// Animates the photos on the index page
		function startAnimation()
		{
			// If we've loaded about 40 images, go ahead and reset so we don't have such a large memory footprint:
			if ((mr.Browser.isMobile && window.photoslider.offsetLeft < -3300) || window.photoslider.offsetLeft < -10000)
			{
				// Fade out:
				window.photoslider.mrFxFade = new Fx.Style(window.photoslider, "opacity", {duration: 1300, onComplete: function()
				{
					window.photoslider.style.left = "0px";
					// Remove all but 5 images:
					for (var i = 0; i < (window.photoslider.getElementsByTagName("img").length - 5); i++)
					{
						window.photoslider.removeChild(window.photoslider.firstChild);
					}
					// Fade in:
					window.photoslider.mrFxFade = new Fx.Style(window.photoslider, "opacity", {duration: 1300, onComplete: function()
					{
						window.setTimeout(startAnimation, window.animationDelay);
					}});
					window.photoslider.mrFxFade._start(0.01, 1.0);
				}});
				window.photoslider.mrFxFade._start(1.0, 0.01);
			}
			else
			{
				window.photoslider.mrFx = new Fx.Style(window.photoslider, 'left', {duration: window.animationSpeed, onComplete: function()
				{
					//window.photoslider.firstChild.style.display = "none";
					window.setTimeout(startAnimation, window.animationDelay);
				}});
				//window.photoslider.removeChild();
				var nextImg = document.createElement("img");
				if (window.currentImage > (window.imagesArray.length - 2))
					window.currentImage = 0;
				else
					window.currentImage++;
				nextImg.src = window.cloudinaryBase + window.imagesArray[window.currentImage]["uid"] + ".jpg";
				nextImg.alt = window.imagesArray[window.currentImage]["first_name"].replace(/\"/, "''") + " " + window.imagesArray[window.currentImage]["last_name"].replace(/\"/, "''") + " || " + window.imagesArray[window.currentImage]["caption"].replace(/\"/, "''") + " || " + window.imagesArray[window.currentImage]["museum"] + " || " + window.imagesArray[window.currentImage]["year"] + " || " + window.imagesArray[window.currentImage]["state"];
				nextImg.onclick = function()
				{
					showLightbox(this);
				}
				window.photoslider.appendChild(nextImg);
				if (mr.Browser.isMobile)
					window.photoslider.mrFx._start(window.photoslider.offsetLeft, window.photoslider.offsetLeft - 155);				
				else
					window.photoslider.mrFx._start(window.photoslider.offsetLeft, window.photoslider.offsetLeft - 236);				
			}	
		}

		/*
		 * Creates the XMLHTTP Object
		 */
		function wsCreateXMLHttpRequest()
		{
			if (document.all && !window.opera && navigator.userAgent.match(/MSIE [56]/))
				return new ActiveXObject("Microsoft.XMLHTTP");
			else
				return new XMLHttpRequest();
		}

		/*
		 * Format for POST, takes an array of elements
		 */
		function wsFormatForPost(pElements)
		{
			var formatted = "";

			for (var i = 0; i < pElements.length; i++)
			{
				if (pElements[i].name && (pElements[i].tagName.toLowerCase() == "select" || pElements[i].tagName.toLowerCase() == "textarea"
				   || (pElements[i].tagName.toLowerCase() == "input" && ((pElements[i].type && pElements[i].type.toLowerCase() != "radio" && pElements[i].type.toLowerCase() != "checkbox" && pElements[i].type.toLowerCase() != "file") 
				   || (pElements[i].type && (pElements[i].type.toLowerCase() == "radio" || pElements[i].type.toLowerCase() == "checkbox") && pElements[i].checked)))))
				{
					formatted += pElements[i].name + "=" + encodeURIComponent(pElements[i].value) + "&";
				}
			}

			return formatted;
		}
		
		/*
		 * Things common to setting up all pages:
		 */
		function pageSetup()
		{
						
			// Setup placeholder restoring function:
			window.originalPlaceholder = {
				first_name: "FIRST NAME",
				last_name: "LAST NAME",
				email: "EMAIL ADDRESS",
				caption: "DESCRIPTION / CAPTION",
				search: "SEARCH PHOTOS"
			};
			var inputs = document.getElementsByTagName("input");
			for (var i = 0; i < inputs.length; i++)
			{
				// Don't want to change the buttons!
				if (inputs[i].type.toLowerCase() == "submit" || inputs[i].type.toLowerCase() == "button")
					continue;

				// Only change value if originalPlaceholder is set!
				inputs[i].onfocus = function()
				{
					if (originalPlaceholder[this.name] && originalPlaceholder[this.name] == this.value)
						this.value = "";
				}
				inputs[i].onblur = function()
				{
					if (originalPlaceholder[this.name] && this.value == "")
						this.value = originalPlaceholder[this.name];
				}
			}
			
		}
		
		// This needs to be onload instead of on DOMContentReady since we need to make sure FB's code has initialized.
		window.onload = pageSetup;
		
	</script>
</head>
<body>
