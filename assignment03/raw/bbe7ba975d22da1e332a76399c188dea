	/*
	 * Gets and processes page contents
	 */
	function wsProcessPageContents()
	{
		if (window.xmlHttpReq.readyState == 4)
		{
			var cloudinaryContents = window.xmlHttpReq.responseText;
			
			// Now that these have been stored in our server, we don't want to submit them to Cloudinary:
			formElements = document.getElementById("cloudinaryform").getElementsByTagName("*");
			for (var i = 0; i < formElements.length; i++)
			{
				if (formElements[i].name && formElements[i].name != "file" && (formElements[i].tagName.toLowerCase() == "select" || formElements[i].tagName.toLowerCase() == "input"))
				{
					formElements[i].disabled = true;
				}
			}
			
			// Set the submission URL:
			document.getElementById("cloudinaryform").action = window.location.protocol + "//api.cloudinary.com/v1_1/americas-family-album/image/upload";
			// Add in the Cloudinary data fields:
			document.getElementById("cloudinaryadditions").innerHTML = cloudinaryContents.replace(/<form[^>]*>/gim, "").replace(/<\/form>/gim, "");
			// Submit the form and photo to Cloudinary:
			document.getElementById("cloudinaryform").submit();
		}
	}
		
	function errorBuilder(makeIt, errText)
	{
		if (makeIt)
			return ("<br> - " + errText);
		else
			return "";
	}
	
	// Setup specific to the home page:
	function indexSetup()
	{
		document.getElementById("submitter").onclick = function()
		{
			// Form validation code here:
			var errorText = "";

			errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("first_name").value.replace(originalPlaceholder.first_name, "")), "Your first name");
			errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("last_name").value.replace(originalPlaceholder.last_name, "")), "Your last name");
			errorText += errorBuilder(!mr.Test.ValidEmail(document.getElementById("email").value), "Your email address");
			errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("photo").value), "Your photo");
			//errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("state").value), "Your home state");
			//errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("museum").value), "Your photo's location");
			//errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("year").value), "Your photo's year");
			//errorText += errorBuilder(mr.Test.IsEmpty(document.getElementById("caption").value.replace(originalPlaceholder.caption, "")), "Your photo's caption");
			errorText += errorBuilder(!document.getElementById("permission").checked, "Your acceptance of the terms and conditions");
			
			if (errorText == "")
			{
				// Remove any placeholder that might be left over in the caption:
				
				document.getElementById("caption").value = document.getElementById("caption").value.replace(originalPlaceholder.caption, "");
				window.xmlHttpReq = wsCreateXMLHttpRequest();
				window.xmlHttpReq.open("get", "store.php?" + wsFormatForPost(document.getElementById("cloudinaryform").getElementsByTagName("*")), true);
				window.xmlHttpReq.onreadystatechange = wsProcessPageContents;
				window.xmlHttpReq.send("");
				
				document.getElementById("submitter").onclick = function() { };
				document.getElementById("submitter").style.cursor = "default";
			}
			else
			{
				(new mr.Dialog()).Show("Error", "Please fill in:" + errorText, "OK");
			}

			return false;
		}
		
		setupAnimation();
		
	}
	
	window.onload = function()
	{
		// Revising this here since we need both the wrapper's "pageSetup" and our specific to the index page "indexSetup"
		// These both need to be onload, not on DOMContentReady.
		pageSetup();
		indexSetup();
		
		// if there was an error uploading:
		var errors = mr.Get.Parameter("error");
		if (errors != "")
		{
			if (/unsupported/i.test(errors))
			{
				(new mr.Dialog()).Show("Sorry", "The file you uploaded was not an image or was not in a supported format.<br>Supported formats are JPG, GIF, PNG and BMP.", "OK");
			}
			else if (/toosmall/i.test(errors))
			{
				(new mr.Dialog()).Show("Sorry", "The image you uploaded was too small.<br>Your image must be 240 x 240 or larger to be a part of America's Family Album.", "OK");
			}
		}
		
		// And if they've come back from the donation page, refill their info:
		var wsUserInfo = window.location.hash.replace(/\+/g, " ").split("***");
		
		if (wsUserInfo.length == 5)
		{
			document.getElementById("email").value = decodeURIComponent(wsUserInfo[0]).replace("#", "");
			document.getElementById("first_name").value = decodeURIComponent(wsUserInfo[1]);
			document.getElementById("last_name").value = decodeURIComponent(wsUserInfo[2]);
			window.location.hash = "";
		}
	}
			
</script>

