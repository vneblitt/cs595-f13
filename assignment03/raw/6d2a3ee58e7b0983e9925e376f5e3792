try {
var pageTracker = _gat._getTracker("UA-8867852-14")
pageTracker._setDomainName(".ppaction.org");
pageTracker._addIgnoredOrganic("ppaction.com");
pageTracker._addIgnoredOrganic("ppaction.org");
pageTracker._setAllowHash(false);
pageTracker._setAllowLinker(true);
pageTracker._trackPageview();
} catch(err) {}</script>

