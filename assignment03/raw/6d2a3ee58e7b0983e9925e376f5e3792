<!--Ecommerce--> 
<script type="text/javascript"> 
/**
 * Function wrapper to implement transactions on ppaction
 *
 * Call this function after the _trackPageview call
 *
 * @author Eduardo Cereto Carvalho <ecereto@cardinalpath.com>
 * @param {String} total Total amount donated.
 * @param {String} [donationType="Unknown Donation"] Either "One Time Gift",
 * "Monthly Giving" or "Honorary Gift".
 * @param {String} [orderId] The orderId if available. If it's not provided
 * we'll generate a random one.
 * @version 2
 */
function cp_gaTrans(total, donationType, orderId) {
    if (typeof total === 'number') {
        total = total.toString();
    }
    if(total.indexOf('$') >= 0) {
        total = total.match(/\$(.*)/)[1]
    }
    if (typeof orderId === 'number') {
        orderId = orderId.toString();
    }
    donationType = donationType || 'Unknown Donation';

    var randSeed = '' + (new Date()).getTime() +
        '-' + Math.round(Math.random() * 10e10);

    // Closure to lock in the variables
    (function(total, donationType, orderId) {
        _gaq.push(function() {
            // Return all trackers defined in the page.
            var trackers = _gat._getTrackers();
            // We'll run the transaction for each tracker.
            for (var i = 0; i < trackers.length; i++) {
                trackers[i]._addTrans(orderId, '', total, '', '', '', '', '');
                trackers[i]._addItem(orderId, donationType, donationType,
                    'Donation', total, '1');
                trackers[i]._trackTrans();
            }
        });
    })(total, donationType, orderId || randSeed);
}
</script>

