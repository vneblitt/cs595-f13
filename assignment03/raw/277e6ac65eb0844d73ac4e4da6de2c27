<script defer charset="utf-8" type="text/javascript" language="javascript1.2">
				function RedirectURI(Params)
				{
				    
				    var Platform = PlatformCheck();
				    var Browser = BrowserCheck();
				    var BrowserCode = Platform + Browser;

				    if (BrowserCode != "xx")
				    {
				        
				        NewURI = "z=" + BrowserCode + "&" + Params;
				        
				        return NewURI;
				    }
				    else
				        return Params;
				}
				
				
				function PlatformCheck()
				{
				    
				    if (navigator.userAgent.indexOf("Windows CE") != -1)
				    {
				        return "winCE_";
				    }
				    
				    else if (navigator.platform.indexOf("Win32") != -1)
				    {
				        return "win_";
				    }
				    
				    else if (navigator.userAgent.indexOf("Mac") != -1)
				    {
				        return "mac_";
				    }
				    
				    else if (navigator.userAgent.indexOf("Linux") != -1)
				    {
				        return "lin_";
				    }
				    
				    else if (navigator.userAgent.indexOf("SunOS") != -1)
				    {
				        return "sun_";
				    }
				    
				    else
				    {
				        return "xx";
				    }
				}
				
				
				function BrowserCheck()
				{
				    
				    if (navigator.appName == "Netscape" && CheckForOpera() == 'false')
				    {
				    if (parseInt(navigator.appVersion) == 4)
				    {
				        return "ns";
				    }
				    
				    else if (navigator.appName == "Netscape" && CheckForOpera() == 'false' && parseInt(navigator.appVersion) >= 5)
				    {
				        return "ns6";
				    }
				    }
				    
				    else if (navigator.appName == "Microsoft Internet Explorer" && CheckForOpera() == 'false')
				    {
				        
				        var VersionNum = parseInt(navigator.appVersion);
				        
				        if (VersionNum >= 4)
				        {
				            return "ie";
				        }
				    }
				    else if (CheckForOpera() == "true" && parseInt(navigator.appVersion) >= 4)
				    {
				        return "opr";
				    }
				    else
				    {
				        return "xx";
				    }
				}
				
				
				function CheckForOpera ()
				{
				    if (navigator.userAgent.indexOf("Opera") > -1)
				    {
				        return "true";
				    }
				    else
				    {
				        return "false";
				    }
				}
			</script><script defer charset="utf-8" type="text/javascript" language="javascript">
		    var version = 1;
		</script><script defer charset="utf-8" type="text/javascript" language="javascript1.1">
		    var version = 1.1;
		</script><script defer charset="utf-8" type="text/javascript" language="javascript1.2">
		    var version = 1.2;
		</script><script defer language="javascript1.1" type="text/javascript">
		    function GoBack()
		    {
		        if (document.referrer != "")
		        {
		            document.location.href = document.referrer;
		        }
		        else
		        {
		            document.location.href = 'browse?props=item(Name,Description),cat(Name,Description)';
		        }
		    }
		</script><script defer charset="utf-8" type="text/javascript" language="javascript">
		    function RedirectURIWrap(Command,Params,name)
		    {
		        //Params = Params + "&style=" + Style;
		        if (version >= 1.2)
		        {
		            Params = RedirectURI(Params);
		            var win = NewWindow(Command,Params,name);
		            win.focus();
		        }
		        else
		            document.location.href = Command + Params;
		    }
		
		    function NewWindow(Command,Params,name)
		    {
			    var width = '500';
			    var height = '520';
			    return window.open(Command + Params,name,',alwaysraised=yes,dependent=no,resizable=yes,status=no,toolbar=no,width=' + width + ',height=' + height);
		    }
		</script>
