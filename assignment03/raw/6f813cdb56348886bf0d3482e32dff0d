<script type="text/javascript">
(function($){
  var url   = $('meta[property="og:url"]').attr("content");
  var title = $('meta[property="og:title"]').attr("content");
  var desc  = $('meta[property="og:description"]').attr("content");
  var img   = $('meta[property="og:image"]').attr("content");
 
  var act = new gigya.socialize.UserAction();
  //  act.setUserMessage("This is the user message");
      act.setTitle(title);
      act.setLinkBack(url);
      act.addActionLink("Read This", url);
      act.setDescription(desc);
      act.addMediaItem({ type: 'image', src:img , href:url });
			
	gigyaUI = new Aggrego.Gigya(jQuery, "suntimes", "Chicago Sun-Times", "suntimes.com", { shareBar: { userAction: act } });
})(jQuery);
</script>
