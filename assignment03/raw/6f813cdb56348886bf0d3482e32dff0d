		<script type="text/javascript">
			// mg2 metering  
			(function($){		
			  var now = new Date();
			  var expiresDate = new Date();
			  expiresDate.setDate(now.getDate() + 7);
			  var storyAlreadyMetered = 0;
			  var postID = "22602222";  
			  
			  if( $.cookie('mg2StoryIDs') != null ){
				if ($.cookie('mg2StoryIDs').indexOf(postID)>-1){
				  storyAlreadyMetered = 1;
				  //console.debug('in: ' + postID);
				}else{
				  var cookieCurrent = $.cookie('mg2StoryIDs');
				  $.cookie('mg2StoryIDs', cookieCurrent + postID + "|", { expires: expiresDate, path: '/', domain: '.suntimes.com' });
				  //console.debug('out: ' + postID);          
				}
			  }else{
				$.cookie('mg2StoryIDs', postID + "|", { expires: expiresDate, path: '/', domain: '.suntimes.com' });
			  }   
		
			  if( storyAlreadyMetered == 0 ){
				if( $.cookie('mg2PayWallCntr') == null ){
				  $.cookie('mg2PayWallCntr', 1, { expires: expiresDate, path: '/', domain: '.suntimes.com' });
				}else{
				  var mg2Count = parseInt($.cookie('mg2PayWallCntr'))+1;
				  $.cookie('mg2PayWallCntr', mg2Count, { expires: expiresDate, path: '/', domain: '.suntimes.com' });      
				}
			  }
			  //console.debug('Pay Wall Count: ' + $.cookie('mg2PayWallCntr')); 		
			})(jQuery);						 	
		</script>
	
	


	
