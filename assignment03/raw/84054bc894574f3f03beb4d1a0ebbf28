    // width: 257
    // height: 90
    // keywords: 
    // bust: 774494156
    // tile: 176
    // refreshable: False
    // regex: ^/[.]*
    // page_taxonomy: news.nationalgeographic.com::news::Animals
    // ad_taxonomy: news.nationalgeographic.com::news::Animals
    // taxonomy_match: True
-->


<script type="text/javascript">

// Falling back because someone put this include on foreign sites that we care about.
try{
    (function(ctx){ 
        var slot = this.WPF_admanager.defineAd({
            'placement':"news/animals",
            'dimensions':"257x90", 
            'refreshable':"False",
            'variable_dimensions': '[{"width": "257", "variability": 100, "height": "90"}]'
        });
        this.WPF_admanager.displayAd(slot.id, ctx)
    })(window);
}catch(e){
    if(console)
        if(console.log){
            console.log("Warning, advertising_init is not included.");
            console.log("Some features like tiling will be ignored.");
            console.log("images.nationalgeographic.com/wpf/sites/common/j/advertising_init.js");
        }
    var inc=new Array('//ad.doubleclick.net/adj/ng.news/animals');
    inc.push('sz=257x90');
    inc.push('ptile=1');
    inc.push('ord='+Math.floor(Math.random() * 1e6) + 1);
    document.write( '<script type="text/javascript" src="${script}"><\/script>'.split('${script}').join(inc.join(';')));
}
</script>
    


    
