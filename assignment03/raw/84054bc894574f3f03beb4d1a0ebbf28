<script type="text/javascript" src="http://images.nationalgeographic.com/wpf/sites/common/j/jquery.detectmobilebrowser-cb1345725133.js"></script>
<script type="text/javascript">
	if (!jQuery.browser.mobile) {
	    (function() {
	        function loadHorizon() {
	            var s = document.createElement('script');
	            s.type = 'text/javascript';
	            s.async = true;
	            s.src = location.protocol + '//ak.sail-horizon.com/horizon/v1.js';
	            var x = document.getElementsByTagName('script')[0];
	            x.parentNode.insertBefore(s, x);
	        }
	        loadHorizon();
	        var oldOnLoad = window.onload;
	        window.onload = function() {
	            if (typeof oldOnLoad === 'function') {
	                oldOnLoad();
	            }
	            Sailthru.setup({
	                domain: 'horizon.nationalgeographic.com',
	                concierge: { from: 'top', threshold: 400 }
	            });
	            // Add tracking parameter to silthru links
	            $(document).bind("SailthruDataLoaded", function(){
	                $('.recommendationWrapper a').each(function() {
	                  this.href += (/\?/.test(this.href) ? '&' : '?') + 'source=sailthru';
	                });
	            });
	        };
	    })();
	}
</script>



