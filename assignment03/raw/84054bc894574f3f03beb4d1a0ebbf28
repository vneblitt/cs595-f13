    $.getScript(window.globalnavURL + 'sites/themes/global/j/global-nav.js');

    // If is PhotoContest load the new nav.
    if (window.location.pathname.match('/photo-contest/').length === 1) {
        var photoContestYear = window.location.pathname.match(/[\d]+/)[0];

        // Load the new nav
        $.getScript(window.globalnavURL + 'sites/all/nirvana/ngpc-' + photoContestYear + '/j/custom-header.js');
    }
