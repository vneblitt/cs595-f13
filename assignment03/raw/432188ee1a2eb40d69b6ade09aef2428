<script type="text/javascript">
    window.fbAsyncInit = function() {
        FB.init({
            appId: '',
            status: true,
            cookie: true,
            xfbml: true
        });

        // listen to the login event
        FB.Event.subscribe('auth.login', function (response) {
            if (response.status === 'connected') {
                $('.popbox').fadeOut();
                var accessToken = response.authResponse.accessToken;
                window.location = '?ref=http://www.threadless.com/info/blog/976345/score_my_sound_world_designs_to_win_npr_swag?utm_medium=social_profile&amp;utm_source=twitter.com&token=' + accessToken;
            }
        });

        // if we're logged in, wire up the logout button
        FB.getLoginStatus(function (response) {
            if (response.status === 'connected') {
                $(document).delegate('#auth_nav .logout a', 'click', function (e) {
                    FB.logout();
                });
            }
        });
    };

        (function(d){
            var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
