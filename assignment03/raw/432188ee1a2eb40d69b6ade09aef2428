                    Grab this new design, <a href="/product/{{this.details.productid}}/{{this.details.product_slug}}" onClick="_gaq.push(['_trackEvent', 'Following', 'Notification', 'Product Launch: Design Title']);">{{this.details.title}}</a> before anyone else does.
                </figcaption>
                <a href="/product/{{this.details.productid}}/{{this.details.product_slug}}" onClick="_gaq.push(['_trackEvent', 'Following', 'Notification', 'Product Launch: Design Image']);">
                    <img class="design-image" src="https://dov5cor25da49.cloudfront.net/products/{{this.details.productid}}/140x100design_01.jpg" alt=""/>
                </a>
            </figure>
    {{/if_eq}}
  </li>
{{/each}}

        </script>

<script type="text/javascript">
Handlebars.registerHelper('if_eq', function(a, b, block) {
  return a == b
     ? block(this)
     : block.inverse(this);
});
Handlebars.registerHelper('others', function(audience) {
    if(audience.length == 2) {
        return "and 1 other friend has scored";
    } else if(audience.length > 2) {
        return "and "+audience.length+" other friends have scored";
    } else {
        return "has scored";
    }
});
</script>

