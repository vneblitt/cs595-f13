<script type="text/javascript">
    $('.dropdown-menu a, .dropdown-menu').click(function(e) {
        e.stopPropagation();
    });
    function last_notified_success() {
      // Quietly swallow this success
  }
  function last_notified_error(jqXHR, textStatus, errorThrown) {
      if (window.console) {
          console.log(textStatus, errorThrown, jqXHR.responseText);
      }
  }
  function notification_success(data) {
      var template = Handlebars.compile($('#notification-entries').html());
      $('<ul/>', {'class': 'notifications', html: template(data)}).appendTo('.activity-stream .notifications-list');
      // I actually have new notifications: show me the count (up to 99)
      if (data.new_count > 0) {$(".activity-stream .hitexpand").append('<span class="msg-count">' + data.new_count + '</span>');}
      // I have more than 99 unread notifications: cap the display number at 99+
      if (data.new_count > 99) {$(".activity-stream .hitexpand").append('<span class="msg-count">99+</span>');}
      // What happens if I'm logged out: Show me a learn more button
      if (threadbare.getAnonymousNotifications) {$('.showmore').html('<a href="/info/blog/976347/keep_tabs_on_your_favorite_artists_with_following"><em>Learn More</em></a>');}
      // If have more than 10 notifications: let's load more
      if (data && data.result && data.result.length >= 9) {$('.showmore').html('<em>Load More</em>');}
      // I don't have any notifications :( 
      if (data && data.result && data.result.length === 0) {
          $('.notifications-list, .showmore').addClass('empty');
          $('.showmore').replaceWith('<span class="nm showmore"><em>You have no new notifications.</em></span>');
      }
    }

  function notification_error(jqXHR, textStatus, errorThrown) {
      if (window.console) {
          console.log(textStatus, errorThrown, jqXHR.responseText);
      }
  }

  $(".showmore").click(function () {
      $('.showmore').html('<img src="https://d2djzakkfkyr8w.cloudfront.net/static/imgs/global/loading-v.gif" alt=""/>');
      threadbare.getNotifications(function (data) {
          if (data && data.result && data.result.length > 0) {
              var template = Handlebars.compile($('#notification-entries').html());
              $('.notifications-list .notifications').append(template(data));
              $('.showmore').html('<span class="lm showmore"><em>Load More</em></span>');
          } else {
              $('.showmore').replaceWith('<span class="nm showmore"><em>No more posts to show</em></span>');
          }
      });
  });                                                                         
</script>


