<script type="text/javascript">
    $(function() {
        // handle quote link
        $('section.comments').delegate('.quote_bug a', 'click', function() {
            var d = $(this).closest('header').children('h5').children('a').html(),
                a = $(this).closest('article').children('div').html();

            $('#id_post').val(
                function(i, val){
                    return '<blockquote><cite>' + $.trim(d) + ' said:</cite>' + $.trim(a) + '</blockquote>\n' + val;
                }
            ).focus();
        });
        // Update Tooltips
        $('div.avatar').each(function() {
            window.update_tooltips();
        });
    });
</script>



