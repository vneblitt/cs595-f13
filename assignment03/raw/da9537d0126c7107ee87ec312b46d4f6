        jQuery("#ad_zone8").append(tp_ad_zone8);
        
    // display the newswhip widget on both desktop and mobile
    var newswhipWidget = "<script src=\"http:\/\/www.newswhip.com\/WidgetJavaScriptAction?style=thinkprogress&widgetRequested=id_4&count=5&removeInteractions=false&removePreview=false&removeImage=false&widgetTitle=Most Shared on Health\" type=\"text\/javascript\">\r\n<\/script>\r\n<div id=\"newswhip-trending\"><\/div>\r\n<div class=\"tp-powered\">\r\n\t<span>Powered by <a href=\"http:\/\/www.newswhip.com\/\" target=\"_blank\">NewsWhip<\/a><span class=\"ico\"><\/span><\/span>\r\n<\/div>";
    jQuery("#newswhip-widget").append(newswhipWidget);

    // display the sidebar donate ad on either desktop or mobile
    var sidebarDonateAd = "<a href=\"\/donate\/\"><img src=\"http:\/\/thinkprogress.org\/wp-content\/themes\/tp4\/images\/donate.jpg\" class=\"side-ad\" alt=\"Donation Ad\"><\/a>";
    jQuery("#sidebar-donate-ad").append(sidebarDonateAd);

});

</script>

