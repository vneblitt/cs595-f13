<script type="text/javascript">
$(function() {
	var links = $('a');
	var linkFragments = $(links).filter('[href^="#"]');
	var links = $(links).not(linkFragments);
	var slideActionLinks = $('#actions').children('a');
	var slideNavLinks = $('.navi').children('a');
	var links = $(links).not(slideActionLinks).not(slideNavLinks); // Subtracting these links fixes IE bug for slideshow.
	var outboundLinks = $(links).filter('[href^="http://"],[href^="https://"]');
	var mailLinks = $(links).filter('[href^="mailto:"]');
	var subsetLinks = $(links).not(outboundLinks).not(mailLinks);
	var pdfLinks = $(subsetLinks).filter('[href*=".pdf"]');
	var zipLinks = $(subsetLinks).not(pdfLinks).filter('[href*=".zip"]');
	var epubLinks = $(subsetLinks).not(pdfLinks).not(zipLinks).filter('[href*=".epub"]');
	var mobiLinks = $(subsetLinks).not(pdfLinks).not(zipLinks).not(epubLinks).filter('[href*=".mobi"]');
	$(outboundLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf('://') + 3,action.length);
		recordLinkDelay(this, 'Outbound Links', action); return false;
	});
	$(pdfLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf('.gov') + 4,action.length);
		recordLinkDelay(this, 'PDF Downloads', action); return false;
	});
	$(zipLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf('.gov') + 4,action.length);
		recordLinkNoDelay(this, 'Zip Downloads', action);
	});
	$(epubLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf('.gov') + 4,action.length);
		recordLinkNoDelay(this, 'Epub Downloads', action);
	});
	$(mobiLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf('.gov') + 4,action.length);
		recordLinkNoDelay(this, 'Mobi Downloads', action);
	});
	$(mailLinks).click(function() {
		var action = this.href;
		action = action.substring(action.indexOf(':') + 1,action.length);
		recordLinkNoDelay(this, 'Mail Links', action);
	});
});
</script>
